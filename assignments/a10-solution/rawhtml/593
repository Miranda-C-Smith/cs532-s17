			var s = document.createElement("script"), el = document.getElementsByTagName("script")[0];
			s.async = true;
			s.src = (document.location.protocol == "https:" ? "https://sb" : "http://b") + ".scorecardresearch.com/beacon.js";
			el.parentNode.insertBefore(s, el);
		})();

		// Google
		var pageTracker = _gat._getTracker("UA-38131116-1");
		pageTracker._trackPageview(url+'?p=ver_mas');

		// Nielsen
		var _rsCI="es-epi";
		var _rsCG="0";
		var _rsDN="//secure-uk.imrworldwide.com/";
		var _rsCC=0;
		var _rsDT=0;
		var _rsDU=0;
		var _rsDO=0;
		var _rsX6=0;
		var _rsSI=escape(base)+escape(url+'/comentarios/'+cont_ver_mas);
		var _rsLP=location.protocol.indexOf('https')>-1?'https:':'http:';
		var _rsRP=escape(document.referrer);
		var _rsND=_rsLP+'//secure-uk.imrworldwide.com/';
		var _rsRD=(new Date()).getTime();
		var _rsSE=0;
		var _rsSV="";
		var _rsSM=0;

		$("#ojd").html("<IMG SRC=\""+_rsND+'cgi-bin/m?ci='+_rsCI+'&cg='+_rsCG+'&si='+_rsSI+'&rp='+_rsRP+"\" WIDTH=\"1\" HEIGHT=\"1\">");
