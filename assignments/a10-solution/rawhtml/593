		<script>
			var logIn = false;
			$('#top-login-form').submit(function(ev){
				if(logIn != undefined && logIn) return false;
				logIn = true; 
				$.ajax({
					type: 'POST',
					url: '/index.php/mod.usuarios/mem.loginAjaxJson',
					cache: false,
					dataType: 'json',
					data: jQuery(this).serialize(),
					success: function(data){
						logIn = false;
						if(data.success){
							location.href = location.href;
						}else{
							$('#top-login-message').html(data.error);
						}
					}
				});
				$('#top-login-message').html('Entrando...');
				return false;
			});
		</script>
		
		
	    <script type="text/javascript">	    	
			$("a#enl-us").toggle(
				function() {					
					$("#block-ajax ul#men-us").removeClass("ocu");
					$("#block-ajax #arrow_nav").removeClass("ocu");
				 },
				function() {					
					$("#block-ajax ul#men-us").addClass("ocu");
					$("#block-ajax #arrow_nav").addClass("ocu");
			 });
			 var date = new Date();
	
			 var pathName=document.location.pathname;
			 cookieValue = $.cookie("lulg");
			 
			 if(cookieValue != null || pathName.indexOf('mod.indice') >= 0 || pathName.indexOf('mod.usuarios') >= 0 || pathName.indexOf('mem.listado') >= 0){
			 	recargaBlockUser();
			 }
	
			 function recargaBlockUser(){
				$.ajax({
					type: 'POST',
					url: '/index.php/mod.usuarios/mem.usuarioAjax',
					cache: false,
					//dataType: 'json',
					success: function(data){
						if($.trim(data) != "") {
							$('.usr-block').html(data);
							$("[title='Hazte socio']").remove();
						}
						$("#block-ajax ul#men-us").addClass("ocu");//Ocultamos el menu de usuario
						$("#block-ajax #arrow_nav").addClass("ocu");//Ocultamos el menu de usuario						
						$("a#enl-us").toggle(
							function() {								
								$("#block-ajax ul#men-us").removeClass("ocu");
								$("#block-ajax #arrow_nav").removeClass("ocu");
							 },
							function() {								
								$("#block-ajax ul#men-us").addClass("ocu");
								$("#block-ajax #arrow_nav").addClass("ocu");
						 });
					}
				});
			}
			 
			 function desplegarLogin(){
				if(!$("#top-login-form").hasClass("ocu")){
					$("#top-login-form").addClass("ocu");
					$("#arrow_nav_login").addClass("ocu");
					$('#top-login-message').html('');
					$("#top-login-usuario").attr("value", "");
			    	$("#top-login-contrasena").attr("value", "");
				} 
				else {
					$("#top-login-form").removeClass("ocu");
					$("#arrow_nav_login").removeClass("ocu");
					if(window.scrollY > $("#top-login-form").offset().top){
						window.scrollTo(0,0);
					}
					$("#top-login-usuario")[0].focus();
					if($('#top-registro-form').length > 0
							&& $('#top-registro-form').css("display") != "none"){
						$('#top-registro-form').slideUp(200);
					};
				}
			 }
			 
			$(document).ready(function(){					
	    		$("#top-login-form").click(function(e){
	    			e.stopPropagation();	
	    		});	    		
	     		$("body").click(function() { 
					$("#top-login-form").addClass("ocu");
					$("#arrow_nav_login").addClass("ocu");
					$('#top-login-message').html('');
					$("#top-login-usuario").attr("value", "");
			    	$("#top-login-contrasena").attr("value", "");
					$("#men-cab").addClass("ocu");			
			    });	
			    $('#iniciar-sesion-link').click(function(){
			    	desplegarLogin();
					return false;
				});	
				$('#iniciar-sesion-link-sticky').click(function(){
			    	desplegarLogin();
			    	var alturaPubli = $("#div-gpt-ad-mega").height();			    	
			    	alturaPubli = -(alturaPubli + 31)
			    	//alert(alturaPubli);
			    	if($("body").width() < 1024){
			    		$("#block-ajax").css("top", alturaPubli);
			    	}			    	
					return false;
				});					
				$('#op-men').click(function(){
					if($("#men-cab").hasClass("ocu")){
						$("#men-cab").removeClass("ocu");						
					} 
					else {
						$("#men-cab").addClass("ocu");												
					}
					return false;			
				});		      			   
	   		});
			
		</script>
	</div>		
