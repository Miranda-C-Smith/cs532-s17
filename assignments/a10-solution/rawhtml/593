<div id="web-dec" class="con">
	<div id="web" class="con">
  		<div id="cab" class="con">
	    			
 
	    			<!-- BEGIN DEP_COP /global/global-interiorcabecera -->
	    	
<script type="text/javascript">
	$(document).ready(function(){			
		$("#toggl_capa_perfil").click(function(event ){
			$("#usr-block-sticky").toggleClass("ocu");
			$("#men-us-sticky").toggleClass("ocu");	
			$("#usr-block-sticky strong").toggleClass("ocu");
			$("#usr-block-sticky #men-us").toggleClass("ocu");
			event.stopPropagation();
		});
		
		$(document).bind("click", function(){
			if ($("#usr-block-sticky").is(":visible")){
				$("#usr-block-sticky").addClass("ocu");
				$("#usr-block-sticky strong").addClass("ocu");
				$("#men-us-sticky").addClass("ocu");	
			}
		});	
			
		
 		/*Menu sticky*/
		var scroll=false;
    	$("#toggle_nav_sticky").bind("click touchstart", function(event){
			event.preventDefault();
			event.stopPropagation();
			if (!$(".nav-sticky .menu-sticky").is(":animated")) {									
				$(".nav-sticky .menu-sticky").slideToggle("slow");				
			}
       	});   	
       	
        $(document).bind("click touchstart", function(){

			setTimeout(function(){ 
				if (scroll==false){
			    	if ($('#toggle_nav_sticky').is(":visible") && $(".nav-sticky .menu-sticky").is(":visible")){
			        	$(".nav-sticky .menu-sticky").hide(); 
			        	$('.nav-sticky .menu-sticky > ul li').each(function(){
                           if ($(this).children("ul").length > 0){
                            	$(this).children("ul").addClass("ocu");               
                        	}
                        }); 				        	
			        }
				}
			},300);
        });
             
		$(window).bind('scroll', function(event){
			scroll=true;
			setTimeout(function(){
				scroll=false;
			},300);
		});
		
		$('.nav-sticky .menu-sticky').bind('click touchstart', function(event){
			event.stopPropagation();
		});		

		var width = $(window).width();
       	
		$(window).resize(function(){
			
			if($(window).width() != width){
				if ($('#toggle_nav_sticky').is(":visible") && $(".nav-sticky .menu-sticky").is(":visible")){
	            	$(".nav-sticky .menu-sticky").hide();	
				}				
				width=$(window).width();
				$(window).scroll();
			}
		});
		
		$(window).scroll(function(){			
			if ($(".nav-sticky").is(":visible")){				
				if ($("#cab").offset().top >= $(this).scrollTop()) {										
					$("#cab ul.men_nav").hide(); 
					$('.nav-sticky').hide();									
					$(".nav-sticky .menu-sticky").hide();						 
				}
			}
			else{				
				if ($(this).scrollTop() > ($("#cab").offset().top)) {
					// Ocultamos el menu secciones en caso de que no quepa por el menu principal
					$('.nav-sticky').show();
					$("#cab ul.men_nav").hide(); 
				}else{
					if(width > 1023) {
						$("#cab ul.men_nav").show(); 	
					}				
				}
			}
		});
		
		$('.nav-sticky .menu-sticky > ul li').each(function(){
	   		if ($(this).children("ul").length > 0){
	   			$(this).children("ul").addClass("ocu");  
            	$(this).addClass("child");
	        }
	   	});
		
		$('.nav-sticky .menu-sticky > ul li').hover(
            function() {
                $(this).children("ul").show();               
            }, 
            function() {
                $(this).children("ul").hide();
            }
        );
   	}); 
   	
</script>

<div class="nav-sticky">
	<a id="toggle_nav_sticky" href="javascript:void(0);"><i class="fa fa-bars"></i><span>Secciones</span></a>
	<div class="menu-sticky">		
		<form  method="post" name="buscador" action="/index.php/mod.global/mem.buscadorHemeroteca">
			<fieldset>
				<legend>Buscador de la Hemeroteca</legend>
				<label for="texto-libre_sticky">Palabra a buscar</label>
				<input type="text" value="" id="texto-libre_sticky" name="texto-libre" />
				<input type="submit" class="btn" value="" title="Buscar" />
			</fieldset>
		</form>
