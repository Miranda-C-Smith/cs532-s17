	function verMasComentarios(id, limit){
		$.ajax({
			type: 'POST',
			url: '/index.php/mod.opiniones/mem.listadoAjax/limit.'+limit+'/vars.'+variables,
			cache: false,
			success: function(data){
				$('#com').html(data);
				cont_ver_mas++;
				trackPage('/noticias/opinion/2017/02/07/lo_que_faltaba_60766_1023.html', 'http://www.infolibre.es', cont_ver_mas);
				$('.responder').css('cursor', 'pointer');
			}
		});
	}
	
	function trackPage(url, base, cont_ver_mas){
		// ComScore
		var _comscore = _comscore || [];
		_comscore.push({ c1: "2", c2: "16790405" });
