<script>

function toggleRegistro(){
	if($('#top-registro-form').css("display") == "none"){
		$('#top-registro-form form')[0].reset();
		$('#top-registro-form').slideDown(200);
	}else{
		$('#top-registro-form').slideUp(200);
	};
}

var honeypot = false;
$('#registrate-link').click(function(ev){
	toggleRegistro();
	
	if(!honeypot){
		honeypot = true;
		$.ajax({
			url: '/ajax.php',
			cache: false,
			data: {
				mod: 'usuarios',
				clase: 'VPusuariosAjaxHoneypot',
				metodo: 'getJSON'
			},
			dataType: "json",
			type: "POST",
			success: function(data){
				$('#top-registro-timestamp_hon').val(data.timestamp);
				$('#top-registro-timestamp_hon_sec').val(data.timestamp_hash);
			}
		});
	}
	
	if(!$("#top-login-form").hasClass("ocu")){
		desplegarLogin();
	}
	return false;
});

$("#top-registro-mail2").attr("onPaste", "return false");
	
var registrando = false;
$('#top-registro-form form').submit(function(ev){
	if(registrando != undefined && registrando) return false;
	if (!$("#top-registro-privacidad_user").is(":checked")){
		alert("No has aceptado la clausula de privacidad");
		return false;
	}
	registrando = true;
	$.ajax({
		type: 'POST',
		url: '/index.php/mod.usuarios/mem.aUsuarioAjaxJson',
		cache: false,
		dataType: 'json',
		data: jQuery(this).serialize(),
		success: function(data){
			registrando = false;
			if(data.success){
				$('#top-registro-form > *').hide();
				$('#top-registro-message').show();
			}else{
				$('#top-registro-error').addClass("err");
				$('#top-registro-error').removeClass("ocu");
				$('#top-registro-error').html(data.error);
			}
		},
		error: function(data){
			registrando = false;
		}
	});
	$('#top-registro-error').removeClass("err");
	$('#top-registro-error').addClass("ocu");
	//$('#top-registro-message').html('Registrando...');
	return false;
});

$(document).ready(function(){			
	$("#top-registro-form").click(function(e){
		e.stopPropagation();	
	});
	$("body").click(function() { 
		$('#top-registro-form').slideUp(200);					
	});
});
</script>

	    			<!-- END DEP_COP /global/global-interiorsubcabecera -->
	    		    			
	    	
  		




