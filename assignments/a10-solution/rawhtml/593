			$("#cont-Top-8760 ul.tabs-top li:first").addClass("tabs-selected-top").show();
			$("#cont-Top-8760 div.tabs-container-top:first").show();			
		}

		$("#cont-Top-8760 ul.tabs-top li").click(function() {
			$("#cont-Top-8760 ul.tabs-top li").removeClass("tabs-selected-top");
			$(this).addClass("tabs-selected-top");
			$("#cont-Top-8760 div.tabs-container-top").hide();
			
