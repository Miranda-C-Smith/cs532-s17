function onYouTubeIframeAPIReady() {
  if (!SrGlobal.isOwner && SrGlobal.isLive !== 1 && !!SrGlobal.youtubeId) {
    SrGlobal.playYoutube(SrGlobal.youtubeId);
  }
}

SrGlobal.playYoutube = function(youtubeId) {
  SrGlobal.stopYoutube();

  SrGlobal.youtubePlayer = new YT.Player('youtubePlayer', {
    height: '360',
    width: '640',
    videoId: youtubeId,
    playerVars: {
      autoplay: 1,
      loop: 1,
      playlist: youtubeId,
      showinfo: 0,
      disablekb: 0,
      controls: 0
    }
  });

  $("#youtubeArea").css("display", "block")
}

SrGlobal.stopYoutube = function () {
  $("#youtubeArea").css("display", "none");

  if (SrGlobal.youtubePlayer) {
    SrGlobal.youtubePlayer.destroy();
    SrGlobal.youtubePlayer = null;
  }
}

</script>







