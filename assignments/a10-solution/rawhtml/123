//var flashvars = {"roomId":85439, "apiUrl":"https://www.showroom-live.com", "commentViewTime":3};
var flashvars = {};

var params = {};
params.quality = "medium";
params.allowscriptaccess = "sameDomain";
params.bgcolor = "#000000";
params.allowFullScreen = "true";
//Flash コンテンツで z-index を有効に windows　safari/IE 対応
params.wmode ="transparent";
var ua = navigator.userAgent;
var isIE = ua.match(/msie/i);
if (!isIE) {
    isIE = ua.match(/rv\:11/i);
}

// FIXME: firefox 40 async plugin init によるバグ
// へのワークアラウンドな対応として、
// setTimeout で swf load を遅らせる

var isFirefox40 = function () {
    var ua = navigator.userAgent.toLowerCase();
    var index = ua.indexOf('firefox');
    if (index === -1) {
        return false
    }
    var version = ua.substring(index + 8, ua.length);
    return parseInt(version.split('.')[0]) === 40;
}

params.allowscriptaccess="always";
var attributes = {};
attributes.id = "ShowRoomLive";
attributes.name = "ShowRoomLive";
attributes.align = "middle";

setTimeout(
    function () {
        swfobject.embedSWF(
                "/assets/swf/v3/ShowRoomLive.swf?v=20160614"
                , "flashContent",
                "640", "360",
                swfVersionStr, xiSwfUrlStr,
                flashvars, params, attributes);
        // JavaScript enabled so display the flashContent div in case it is not replaced with a swf object.
        swfobject.createCSS("#flashContent", "display:block;text-align:left;z-index:0;");
    },
    isFirefox40() ? 3000 : 0
);

function thisMovie(movieName)
{
    if (navigator.appName.indexOf("Microsoft") != -1) {
        return window[movieName];
    } else {
        return document[movieName];
    }
}

