function doLogout() {
  if(window.confirm('Are you sure you want to log out?')){
    $.ajax({
            url: "/user/logout_api",
            type: "POST",
            data: {csrf_token:SrGlobal.csrfToken}, 
        }).done(function(data, status, xhr) {
            if (location.pathname === "/user/mail_auth") {
                location.reload();
            } else {
                location.href = '/';
            }
        });
  }
}

