<!-- Inicio Javascript -->
<script type="text/javascript">
    var moduleFuncs = function() {
        var pathname = window.location.pathname;
        var utm_content = 'portada';
        if(pathname !== '/'){
            var splited_paths = pathname.split('/');
            if(splited_paths[splited_paths.length-1] === ''){
                // subdomain. remove empty spaces
                splited_paths = splited_paths.filter(function(v){return v!==''});
                utm_content = splited_paths.join('-');
            }else{
                // subdomain with file ending
                splited_paths.pop();
                splited_paths = splited_paths.filter(function(v){return v!==''});
                utm_content = splited_paths.join('-');
            }
        }

        var nssource = window.location.hostname;
        document.querySelectorAll('#carrusel-oferplan a').forEach(function(item) {
            var href = item.getAttribute('href');
            if (typeof href !== 'undefined') {
                href = href.replace(/xxmarketingcontentxx/g, utm_content.toLowerCase());
                href = href.replace(/xxnssourcexx/g, nssource.toLowerCase());
                item.setAttribute('href', href);
            }
        });

        if(window.jQuery) {
            var carousel = $("#carrusel-oferplan");
            carousel.jcarousel({
                wrap: 'circular'
            });

            carousel.jcarouselAutoscroll({
                interval: 5000,
                target: '+=1',
                autostart: true
            });

            carousel.on('jcarousel:scroll', function(event, carousel, target, animate) {
                $('#carrusel-oferplan .txt, #carrusel-oferplan .txt2').css({'-webkit-line-clamp':'initial'})
                setTimeout(function(){
                    $('#carrusel-oferplan .txt').css({'-webkit-line-clamp':'2'})
                    $('#carrusel-oferplan .txt2').css({'-webkit-line-clamp':'3'})
                }, 400);
            });

            $('.jcarousel-prev-horizontal').click(function() {
                $('#carrusel-oferplan').jcarousel('scroll', '-=1');
            });

            $('.jcarousel-next-horizontal').click(function() {
                $('#carrusel-oferplan').jcarousel('scroll', '+=1');
            });
        }
    };

    function ready(fn) {
        if (document.readyState !== 'loading') {
            fn()
        } else {
            document.addEventListener('DOMContentLoaded', fn);
        }
    }

    ready(moduleFuncs);
</script>
<!-- Fin Javascript -->

<!-- FIN de la entrega -->
