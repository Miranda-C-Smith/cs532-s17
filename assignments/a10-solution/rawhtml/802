function getCookie_Layer(e){var t=document.cookie;var n=t.indexOf(" "+e+"=");if(n==-1){n=t.indexOf(e+"=")}if(n==-1){t=null}else{n=t.indexOf("=",n)+1;var r=t.indexOf(";",n);if(r==-1){r=t.length}t=unescape(t.substring(n,r))}return t}function setCookie_Layer(e,t,n){var r=new Date;r.setDate(r.getDate()+n);var i=escape(t)+(n==null?"":"; expires="+r.toUTCString());document.cookie=e+"="+i}function CookieLayerVisualizado(e){setCookie_Layer("cookie_20161115",e,365)}function CookieLayerVisto(){setCookie_Layer("cookie_20161115",2,365);document.getElementById("layer_cookie").style.display="none"}var CookieLayer=getCookie_Layer("cookie_20161115");if(CookieLayer!=null&&CookieLayer!=""&&CookieLayer!="0"&&CookieLayer!="1"){document.getElementById("layer_cookie").style.display="none"}else{if(CookieLayer==null)CookieLayer=0;document.getElementById("layer_cookie").style.display="block";window.onscroll=function(e){CookieLayerVisualizado(parseInt(CookieLayer)+2)}}
