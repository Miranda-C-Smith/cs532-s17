<script>
$(window).load(function() {

    function renderBtnLikeFacebook(link)
    {
        var load_like = false;
        var url = $(link).attr('href');
        var title = $(link).attr('title');
        
        $(link).parent().parent().find('.me_gusta').each(function(){
             if(
                $(this).attr('load-facebook') === undefined){
                    var btn_like_facebook = '<a href="http://www.addthis.com/bookmark.php" class="addthis_button_facebook_like" addthis:url="'+url+'" addthis:title="'+title+'" ></a>';
                    $(this).html(btn_like_facebook);
                    $(this).attr('load-facebook' , true);
                    load_like = true;
                }
        });
        return load_like;
    }

    var load_like = false;
    var pestana = $('#1');
    pestana.find('a').each(function(){
        if ( $(this).attr('href').indexOf('.html?') > 0 ) $(this).attr('href', $(this).attr('href').substr(0,$(this).attr('href').indexOf('.html?') + 5));
        else if ( $(this).attr('href').indexOf('.html#') > 0) $(this).attr('href',$(this).attr('href').substr(0,$(this).attr('href').indexOf('.html#') + 5));
        load_like = renderBtnLikeFacebook(this);            
    });
        
    socialNetworks.toolboxAddthis('.btn-like-facebook');
    
    addTagsAnalitycs(); 

    $('a[data-toggle="tab"]').on('show.bs.tab', function (e) {
        var load_like = false;
        var pestana = $(e.target.hash);
        pestana.find('a').each(function(){
            if ( $(this).attr('href').indexOf('.html?') > 0 ) $(this).attr('href', $(this).attr('href').substr(0,$(this).attr('href').indexOf('.html?') + 5));
            else if ( $(this).attr('href').indexOf('.html#') > 0) $(this).attr('href',$(this).attr('href').substr(0,$(this).attr('href').indexOf('.html#') + 5));
            load_like = renderBtnLikeFacebook(this);            
        });
        if(load_like){
            socialNetworks.toolboxAddthis('.btn-like-facebook');       
        }
        
        addTagsAnalitycs();  
    })
    
    function addTagsAnalitycs() {
    
        var positionMod = "col-C";
        var addTags = "#ns_campaign=mod-lo-mas&amp;ns_mchannel=visto-w";
        if (typeof voc_tn != undefined) addTags = addTags + "&amp;ns_source=" + voc_tn;
        if (typeof voc_tc != undefined) addTags = addTags + "&amp;ns_linkname=" + voc_tc;
        addTags = addTags + "&amp;ns_fee=" + positionMod;
    
        $('aside.tabbable.lo_mas div.tab-content div#1.tab-pane ol li h5 a').each(function( index ) {
            if ( $(this).attr('href').indexOf('.html?') > 0 ) $(this).attr('href', $(this).attr('href').substr(0,$(this).attr('href').indexOf('.html?') + 5));
            else if ( $(this).attr('href').indexOf('.html#') > 0) $(this).attr('href',$(this).attr('href').substr(0,$(this).attr('href').indexOf('.html#') + 5));
            $(this).attr('href',$(this).attr('href') + addTags); 
        });
    }
    
});
</script>

