		function loadDefer(){
			if( !deferLoaded ){
				var deferScripts = ['http://a.espncdn.com/redesign/0.346.16/js/espn-defer.js', 'http://a.espncdn.com/redesign/0.346.16/js/espn-defer-low.js'];
