    <script>
      var site_url = "https://www.goodnewsfromindonesia.id/";
      $(document).ready(function () {

        window.twttr = (function(d, s, id) {
          var js, fjs = d.getElementsByTagName(s)[0],
            t = window.twttr || {};
          if (d.getElementById(id)) return;
          js = d.createElement(s);
          js.id = id;
          js.src = "https://platform.twitter.com/widgets.js";
          fjs.parentNode.insertBefore(js, fjs);

          t._e = [];
          t.ready = function(f) {
            t._e.push(f);
          };

          return t;
        }(document, "script", "twitter-wjs"));

        $('.btn-Quote-sharer').on('click', function(e) {
            e.preventDefault();
            $this = $(this);

            $.ajax({
                method: "POST",
                dataType: "json",
                url: site_url + "xhr/sharequote",
                data: { post: $this.data('id'), media: $this.data('media') }
            })
            .done(function(result) {
                console.log(result.message);
            });

        }); // end

        $('.q').keypress(function (e) {
            if (e.which == 13) {
              $('#search_nav').trigger('submit');
            }
        });
        $('[data-toggle="tooltip"]').tooltip()

        $('#search_nav').on('submit', function(e){
          var key = $(this).find('.q').val();
          key = key.split(' ').join('_');
          window.location.href = "/search/" + key;
          return false;
        });

      });

      function gaEventSharer(ShareType){
          ga('send', {
              'hitType': 'event',
              'eventCategory': 'desktop-abtesting',
              'eventAction': 'sharer',
              'eventLabel': ShareType,
              'eventValue': 1
          });
      };
      function gaSharer(Media, Action, Url){
          ga('send', {
              'hitType': 'event',
              'eventCategory': Media,
              'eventAction': Action,
              'eventLabel': Url,
              'eventValue': 1
          });
      };
    </script>
