                            <script type="text/javascript">
                        /* Toggle */
                        $(document).ready(function($){
                            $(".NA_hideBlock").hide();
                            var toggleFlg = "close";
                            $(".NA_toggleBtn").click(function(){
                                $(".NA_hideBlock").slideToggle('fast', 'swing');
                                if(toggleFlg == "close"){
                                    $(this).addClass("NA_toggleOn");
                                    toggleFlg = "open";
                                }else{
                                    $(this).removeClass("NA_toggleOn");
                                    toggleFlg = "close";
                                }
                            });
                        });
                        </script>
                        <script type="text/javascript">
//<![CDATA[
(function() {
    $.getJSON('/stage/news/updatehotnews?id=219891&artist_ids[]=7781&artist_ids[]=7778&artist_ids[]=6531&artist_ids[]=45409&artist_ids[]=7497&artist_ids[]=64858', null, null);
})();
