				<form method="POST" action="http://subs.businessday.co.za/eceapi/Default.aspx?action=addtonewsletteremail" id="PhoenixCall">
					<div class="sub-checklist">
						<input type="hidden" name="actionmethod" value="POST" />
						<input type="hidden" name="publication" value="SDT" />
						<input type="hidden" name="returnurl" value="http://www.timeslive.co.za/?filter=newslettertrue" />
						<ul>
							<li><input name="morning" id="morningcheck" checked="checked" type="checkbox" value="true"><label>AM</label></li>
							<li><input name="afternoon" id="afternooncheck" checked="checked" type="checkbox" value="true"><label>PM</label></li>
							<li><input checked="checked" id="marketingcheck" type="checkbox"><label>Marketing</label></li>
						</ul>
					</div>
					<div class="sub-submit">
						<input type="text" name="email" id="email" onblur="testEmail()" placeholder="Email" onfocus="clearField()">
						<input id="testsend" class="sub-validate" style="cursor:pointer;" onclick="subscribe()" type="button" value="SUBSCRIBE">
					</div>
