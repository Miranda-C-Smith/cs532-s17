<script type="text/javascript">
	function testEmail(){

		var theEmail = $('#email').val()
		if(theEmail == ''){
			$('#email').css("color","red");
			$('#email').val('Enter a valid email address');
		}
		else
		{
			if(isValidEmailAddress(theEmail)){
				$('#testsend').removeAttr('disabled');
			}
			else
			{
				$('#email').css("color","red");
				$('#email').val('Enter a valid email address');
			}
		}



	}

	function clearField(){
		$('#email').css("color","#666");
		$('#email').val('');
	}

	function isValidEmailAddress(emailAddress) {
		var pattern = /^([a-z\d!#$%&'*+\-\/=?^_`{|}~\u00A0-\uD7FF\uF900-\uFDCF\uFDF0-\uFFEF]+(\.[a-z\d!#$%&'*+\-\/=?^_`{|}~\u00A0-\uD7FF\uF900-\uFDCF\uFDF0-\uFFEF]+)*|"((([ \t]*\r\n)?[ \t]+)?([\x01-\x08\x0b\x0c\x0e-\x1f\x7f\x21\x23-\x5b\x5d-\x7e\u00A0-\uD7FF\uF900-\uFDCF\uFDF0-\uFFEF]|\\[\x01-\x09\x0b\x0c\x0d-\x7f\u00A0-\uD7FF\uF900-\uFDCF\uFDF0-\uFFEF]))*(([ \t]*\r\n)?[ \t]+)?")@(([a-z\d\u00A0-\uD7FF\uF900-\uFDCF\uFDF0-\uFFEF]|[a-z\d\u00A0-\uD7FF\uF900-\uFDCF\uFDF0-\uFFEF][a-z\d\-._~\u00A0-\uD7FF\uF900-\uFDCF\uFDF0-\uFFEF]*[a-z\d\u00A0-\uD7FF\uF900-\uFDCF\uFDF0-\uFFEF])\.)+([a-z\u00A0-\uD7FF\uF900-\uFDCF\uFDF0-\uFFEF]|[a-z\u00A0-\uD7FF\uF900-\uFDCF\uFDF0-\uFFEF][a-z\d\-._~\u00A0-\uD7FF\uF900-\uFDCF\uFDF0-\uFFEF]*[a-z\u00A0-\uD7FF\uF900-\uFDCF\uFDF0-\uFFEF])\.?$/i;
		return pattern.test(emailAddress);
	};

	function subscribe(){
		var theEmail = $('#email').val()
		if(theEmail == ''){
			$('#email').css("color","red");
			$('#email').val('Enter a valid email address');
			return;
		}
		else
		{
			if(isValidEmailAddress(theEmail)){
				var morning = $("#morningcheck").is(':checked') == false ? "unsubscribed" : "subscribed";
				var afternoon = $("#afternooncheck").is(':checked') == false ? "unsubscribed" : "subscribed";
				var marketing = $("#marketingcheck").is(':checked') == false ? "unsubscribed" : "subscribed";
				$.post("/?service=tlsub&email=" + theEmail + "&am=" + morning + "&pm=" + afternoon + "&marketing=" + marketing, function( data ) {
					window.location = "http://www.timeslive.co.za/?filter=newslettertrue";
				});
			}
			else
			{
				$('#email').css("color","red");
				$('#email').val('Enter a valid email address');
			}
		}
	}


</script>




