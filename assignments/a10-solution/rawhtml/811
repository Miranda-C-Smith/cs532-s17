        BrightcovePluginFunctions.possibleiframe = $(this).find('iframe');
        if (!BrightcovePluginFunctions.hasBrightcove && BrightcovePluginFunctions.possibleiframe) {
            if (BrightcovePluginFunctions.possibleiframe.attr('src').search('brightcove')){
                document.addEventListener("visibilitychange", BrightcovePluginFunctions.handleVisibilityChange, false);
