                    }, false);
                document.addEventListener('blur', function() {
                        BrightcovePluginFunctions.handleVisibilityChange(false);
                    }, false);
                window.addEventListener('focus', function() {
                        BrightcovePluginFunctions.handleVisibilityChange(true);
                    }, false);
                window.addEventListener('blur', function() {
                        BrightcovePluginFunctions.handleVisibilityChange(false);
                    }, false);

                
