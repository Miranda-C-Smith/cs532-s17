<script>
    var headerbgcolor = '#f4f4f4';
    var InskinControls = {
        InskinControl : {
            BurgerFunction : {},
            inskin_delay : 4500,
            timeSinceWindowLoad : 0,
            actualTimeSinceWindowLoad : 0,
            shouldDelayScroll : true,
            userHasScrolled : false,
            advertDelayCounter : null,
            CssFunction : null,
            SecondayCssFunction : null,
            advertType : null,
            divId : null,
            SecondaryDivId : null,
            backgroundJob : null,
            originalBackground : null,
            hasInskinJoinedUs : false,
            hasPutBack : false,
            hasTemplateAndSuperbanner : 0,
            inskinTop : 0,
            startTime : 0,
            deltaTime : 0,
            deltaDiff : 0,
            adslotChanges : 0,
            DelayScroll : function (divId, type) {
console.log('delayscroll');
             ExpressApp.InskinControl.inskinTop =  0;
             if (ExpressApp.InskinControl.shouldDelayScroll == true) {
                    ExpressApp.InskinControl.advertType = type;
                    if (!ExpressApp.InskinControl.divId) {
                        ExpressApp.InskinControl.divId = divId;
                    } else {
                        ExpressApp.InskinControl.SecondaryDivId = divId;
                    }
                    switch (type) {
                        case 'superbanner':
                            console.log('superbanner');
                            $(window).resize(function(){
                                ExpressApp.InskinControl.Resize();
                            });
                            document.addEventListener('megabanner', function (e) {
                                if($('#' + ExpressApp.InskinControl.divId + ' > div > iframe').length !== 0) {
                                  $('#' + ExpressApp.InskinControl.divId + ' > div > iframe').contents().bind('DOMSubtreeModified', function(el) {
                                    if (el.target.innerHTML.length > 0) {
                                        $iframe = $('#' + ExpressApp.InskinControl.divId + ' > div > iframe');
                                        ExpressApp.InskinControl.adslotChanges++;
                                        if((($iframe.width() === 970 && $iframe.height() === 250) ||
                                            $iframe.width() === 728 && $iframe.height() === 90) && this.actualTimeSinceWindowLoad < 4500) {
                                               console.log('iframe contents have changed');
                                               ExpressApp.InskinControl.StartTheClock();
                                               ExpressApp.InskinControl.shouldDelayScroll = true;
                                               ExpressApp.InskinControl.RefreshTheClock();
                                        }
                                    }
                                  });
                                }
                            }, false);
                            var megaevent = document.createEvent('Event');
                            megaevent.initEvent('megabanner', true, true);
                            document.dispatchEvent(megaevent);

                            $('body').css({backgroundColor: '#f4f4f4'});
                            $('#maincontainer').css({paddingTop: '3px'});
                            $('#maincontainer header').css({borderTop: 'none'});
                            $('.superbanner').css({marginBottom: '0'});
                            ExpressApp.InskinControl.Resize();
                            ExpressApp.InskinControl.hasTemplateAndSuperbanner++;
                            console.log('superbanner');
                            ExpressApp.InskinControl.addShadow();
                            if (!ExpressApp.InskinControl.hasInskinJoinedUs) {
                                ExpressApp.InskinControl.DoTheSticky();
                            }
                            ExpressApp.InskinControl.hasInskinJoinedUs = true;
                            console.log(ExpressApp.InskinControl.hasTemplateAndSuperbanner);
                        break;
                    }
                }
            },
            DoTheSticky : function(){
                console.log('dothesticky');
                if (ExpressApp.InskinControl.shouldDelayScroll == true ) {
                    console.log('doing sticky at ' + this.actualTimeSinceWindowLoad);
                    var tmpfunc = setInterval(function(){
                        ExpressApp.InskinControl.checkForTwoAdverts('superbanner');
                        if (ExpressApp.InskinControl.hasTemplateAndSuperbanner > 1) {
                        }
                        if ($(window).scrollTop() > ExpressApp.InskinControl.inskinTop) {
                           if (!ExpressApp.InskinControl.userHasScrolled) {
                                ExpressApp.InskinControl.startTime =  Date.now();
                           }
                           ExpressApp.InskinControl.userHasScrolled = true;
                           var thediv = ExpressApp.InskinControl.divId;

                           bannerContainerWidth = $('.container').width() + 40;
                           if ($(window).width() < 980) bannerContainerWidth = $('.container').width() + 18;
                           leftpos = (($(window).width() - Math.max($('#' + thediv + ' div iframe').width(), bannerContainerWidth))/2);
                           heightval = $('#' + thediv).height() + 20;
                           $('.superbanner').css({marginBottom: '0'});
                           $('#' + thediv).css({minWidth: bannerContainerWidth + 'px', left: leftpos +'px', top:0, position: 'fixed', zIndex: 9998500, marginBottom: 0, padding: '10px 0', background: '#f4f4f4'}).parent().css({height : heightval + 1});
                           
                                                          if ($.fn.stickyPlayer) {
                                   // send message to the sticky player to set height as heightval + 20;
                                   window.ns_video_player_topPosition = heightval + 20;
                               }
                                                      
                                                   }
                    }, 100);
                    if (ExpressApp.InskinControl.hasTemplateAndSuperbanner > 1) {
                        ExpressApp.InskinControl.SecondayCssFunction = tmpfunc;
                        $('#superbanner').css({marginTop: '10px', position: 'relative',zIndex:6000 });
                        $('.bodywrapper').css({marginTop:  '-' + ($('#superbanner').first('iframe').height() + 10) + 'px'});
                    } else {
                        ExpressApp.InskinControl.removeShadow();
                        ExpressApp.InskinControl.CssFunction = tmpfunc;
                    }
                }
            },
            Resize : function (){
                console.log('resize');
                bannerContainerWidth = $('.container').width() + 40;
                if ($(window).width() < 980) bannerContainerWidth = $('.container').width() + 18;
                $('#' + ExpressApp.InskinControl.divId).css('width', 'auto').css('cssText', $('#' + ExpressApp.InskinControl.divId).attr('style')  + '; margin-bottom:0;  border-bottom:1px solid #d4d4d4; width: ' + bannerContainerWidth + 'px !important;');
                            },
            SplitTheBackground : function(){
            },
            addShadow : function(){
                //$('.headerShell').addClass('shadow');
                //$('#maincontainer').addClass('shadow');
            },
            removeShadow : function(){
                //$('.headerShell').removeClass('shadow');
                //$('#maincontainer').removeClass('shadow');
            },
            checkForTwoAdverts : function(whichTemplate){
                if ($('#div-gpt-ad-319511514470019133-1 div iframe').width() == 0 || !$('#superbanner').is(':visible')) {
                    ExpressApp.InskinControl.hasTemplateAndSuperbanner = 1;
                    ExpressApp.InskinControl.removeShadow();
                }
            },
            SetTheBackground : function(){
            },
            StartTheClock : function(){
                    console.log('StartTheClock');
                if (ExpressApp.InskinControl.timeSinceWindowLoad === 0) {
                    console.log('timeSinceWindowLoad is 0');
                    ExpressApp.InskinControl.startTime =  Date.now();
                    ExpressApp.InskinControl.advertDelayCounter = setInterval(function(){
                            ExpressApp.InskinControl.UpdateSeconds();
                        }, 100);
                }
            },
            RefreshTheClock : function(){
                console.log('RefreshTheClock');
                this.timeSinceWindowLoad = 1;
            },
            UpdateSeconds : function(){
                if (!ExpressApp.InskinControl.hasInskinJoinedUs) {

                }
                if ($(window).scrollTop() > ExpressApp.InskinControl.inskinTop) {
                    deltaTime = Date.now() - ExpressApp.InskinControl.startTime;
                    ExpressApp.InskinControl.deltaDiff = deltaTime - ExpressApp.InskinControl.deltaTime;
                    ExpressApp.InskinControl.deltaTime = deltaTime;
                	this.timeSinceWindowLoad += ExpressApp.InskinControl.deltaDiff;
                	this.actualTimeSinceWindowLoad += ExpressApp.InskinControl.deltaDiff;
                }
                if (this.timeSinceWindowLoad > ExpressApp.InskinControl.inskin_delay && ExpressApp.InskinControl.hasInskinJoinedUs == true) {
                    ExpressApp.InskinControl.PutAdvertBack();
                }
            },
            PutAdvertBack : function (){
                window.clearInterval(ExpressApp.InskinControl.CssFunction);
                window.clearTimeout(ExpressApp.InskinControl.backgroundJob);
                if (ExpressApp.InskinControl.hasTemplateAndSuperbanner > 1 || ExpressApp.InskinControl.SecondayCssFunction) {
                    window.clearInterval(ExpressApp.InskinControl.SecondayCssFunction);
                }
                ExpressApp.InskinControl.SecondayCssFunction = null;
                ExpressApp.InskinControl.shouldDelayScroll = false;
                window.clearInterval(ExpressApp.InskinControl.advertDelayCounter);
                switch (ExpressApp.InskinControl.advertType) {
                    case 'superbanner':
                        if (ExpressApp.InskinControl.hasPutBack == false) {
                            ExpressApp.InskinControl.hasPutBack = true;
                            console.log('putting super banner back at ' + this.timeSinceWindowLoad + 'ms');
                            var thediv = ExpressApp.InskinControl.divId;
                            maxanimation = -1 * $('#' + thediv).height();
                            if (($(window).scrollTop() - $('#superbanner').offset().top) < $('#' + thediv).height()) maxanimation = -1 * ($(window).scrollTop() - $('#superbanner').offset().top);
                            $('#' + thediv).animate({top : maxanimation + 'px'}, 1000, function(){
                               
                                                                   // send message to the sticky player to set height as 0
                                    if ($.fn.stickyPlayer) {
                                        window.ns_video_player_topPosition = 0;
                                    }
                               
                               $('#' + thediv).css({zIndex: 500, position: 'static', padding: '10px 0', background: 'transparent', width: 'auto !important', minWidth: '1px'});
                            });
                                                    }
                    break;
                }
                ExpressApp.InskinControl.shouldDelayScroll = false;
                ExpressApp.InskinControl.SecondaryDivId = null;
            }
        }
    };

    $.extend(ExpressApp, InskinControls);
    $(window).load(function(){
        ExpressApp.InskinControl.StartTheClock();
    });
    $(function(){
        setTimeout(function(){
            ExpressApp.InskinControl.StartTheClock();
        }, 2000);
    });
</script>
<script>
// <![CDATA[
$.extend(ExpressApp.Weather, {
    weatherForecastUrl: '//weather/forecast/location/{LOCATION_ID}/{LOCATION_TITLE}',
    weatherForecastThumb: 'http://cdn.images.express.co.uk/img/weather/png/{ICO_SIZE}/head2_icon_{SKY_CODE}.png',
    GetWeatherData: function() {
        var $object = $(ExpressApp.Weather.weatherWidgetTrigger);
        var cookieLocationCode = $.cookie('metOfficeLocationId');

        $.ajax({
            url: '/weather/get/'+(cookieLocationCode ? cookieLocationCode : '352409'),
            success: function(data) {
                if (data) {
                    ExpressApp.Weather.FillInWeatherWidget(data);
                }
            },
            beforeSend: function() {
                ExpressApp.Preload.Start($object, {left:'auto', right:297});
            },
            complete: function() {
                ExpressApp.Preload.Stop($object);
                $object.find('a').addClass('in');
            }
        });
    },
    FillInWeatherWidget: function(data){
        var $object = $(ExpressApp.Weather.weatherWidgetTrigger);

        $object.find('.weather-info').attr('href', ExpressApp.Weather.weatherForecastUrl.replace('{LOCATION_ID}', data.weatherlocation_id).replace('{LOCATION_TITLE}', $.cookie('metOfficeLocationName')));
        $object.find('a').attr('style', 'padding: 2px 32px 0 35px;background-size: contain;background-image:url("'+ExpressApp.Weather.weatherForecastThumb.replace('{ICO_SIZE}', '52x52').replace('{SKY_CODE}', parseInt(data.weather_type).padLeft(2,'0'))+'")');
        $object.find('.jq-centigrade').text(data.temperature);
        $object.find('.jq-town').text($.cookie('metOfficeLocationName'));
    }
});
$(window).load(function(){
    if ($.cookie('metOfficeLocationId') && $.cookie('metOfficeLocationId') != 352409) {
        ExpressApp.Weather.GetWeather();
    }
});
// ]]>
</script>

