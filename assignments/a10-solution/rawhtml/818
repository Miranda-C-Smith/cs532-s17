<script type="text/javascript">

jQuery(document).ready(function ($) {

	$('a.toggle-header-sub').on('click',function(event) { 
		$('body').toggleClass('toggle-global'); $('html,body').animate({scrollTop: 0}, 700);
		ga('send','event','ananweb-banner','','toggle-header-menu');
	});

	$('li.toggle-header-search').on('click',function(event) { 
		$('body').toggleClass('toggle-search'); $('html,body').animate({scrollTop: 0}, 700);
		ga('send','event','ananweb-banner','','toggle-header-search');
	});	
	
	$("a.tracking").mouseup(function(){
		if($(this).attr('href').toLowerCase() != "javascript:;") {
			ga('send','event','ananweb-banner',document.location.pathname,$(this).attr('href'));
		}
	});

	$("a.sns-tracking").mouseup(function(){
		if($(this).attr('href').toLowerCase() != "javascript:;") {
			if (typeof(ga)!=='undefined') {ga('send','event','sns',document.location.pathname,$(this).attr('class').replace(' sns-tracking',''),$(this).attr('ref'));}
		}
	});

	$("a.related-category").mouseup(function(){
		if($(this).attr('href').toLowerCase() != "javascript:;") {
			if (typeof(ga)!=='undefined') {ga('send','event','related-category',document.location.pathname,$(this).attr('href'));}
		}
	});

	$("a.related-single").mouseup(function(){
		if($(this).attr('href').toLowerCase() != "javascript:;") {
			if (typeof(ga)!=='undefined') {ga('send','event','related-single',document.location.pathname,$(this).attr('href'));}
		}
	});

	$("a.sidebar-pickup").mouseup(function(){
		if($(this).attr('href').toLowerCase() != "javascript:;") {
			if (typeof(ga)!=='undefined') {ga('send','event','sidebar-pickup',document.location.pathname,$(this).attr('href'));}
		}
	});

	$("a.sidebar-ranking").mouseup(function(){
		if($(this).attr('href').toLowerCase() != "javascript:;") {
			if (typeof(ga)!=='undefined') {ga('send','event','sidebar-ranking',document.location.pathname,$(this).attr('href'));}
		}
	});

	$("a.sidebar-column").mouseup(function(){
		if($(this).attr('href').toLowerCase() != "javascript:;") {
			if (typeof(ga)!=='undefined') {ga('send','event','sidebar-column',document.location.pathname,$(this).attr('href'));}
		}
	});

	$("a.sidebar-popular-writer").mouseup(function(){
		if($(this).attr('href').toLowerCase() != "javascript:;") {
			if (typeof(ga)!=='undefined') {ga('send','event','sidebar-popular-writer',document.location.pathname,$(this).attr('href'));}
		}
	});

	$("a[href^='http']").not("a[href*='magazineworld']").not("a[href*='ananweb']").not("a[href*='microad']").attr("target","_blank");
	
	$("a.related-article-honbun").mouseup(function(){
		if($(this).attr('href').toLowerCase() != "javascript:;") {
			if (typeof(ga)!=='undefined') {ga('send','event','related-article-honbun',document.location.pathname,$(this).attr('href'));}
		}
	});
			
$(document).bind("contextmenu",function(e){ return false;});
$("article img").mousedown(function(){return false;});

	

});

</script>

