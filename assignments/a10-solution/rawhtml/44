<script type="text/javascript">
    $(document).ready(function () {

        $("#NewsLetSent").click(function () {

            if ($("#txtNewLetName").val() == "") {
                $(".newsletmsg").html("لطفا نام خود را وارد کنید");
                return;
            }

            if ($("#ctl00_ctl00_VeryTopContentPlaceHolder_NewsLetterReg16_txtNewLetEmail").val() == "") {
                $(".newsletmsg").html("لطفا ایمیل خود را وارد کنید");
                return;
            }

            if ($("#ctl00_ctl00_VeryTopContentPlaceHolder_NewsLetterReg16_SecureCodeTextBox").val() == "") {
                $(".newsletmsg").html("لطفا کد امنیتی را وارد کنید");
                return;
            }

            var securecode = $("#ctl00_ctl00_VeryTopContentPlaceHolder_NewsLetterReg16_Captcha_US1_SecureCodeLabel").val();
            var enterdsecurecode = $("#ctl00_ctl00_VeryTopContentPlaceHolder_NewsLetterReg16_SecureCodeTextBox").val();


            $(".newsletmsg").html("درحال ارسال اطلاعات ...");
            $.ajax({
                type: "Post",
                url: "/NetServices/ajax.aspx/InvoiceModuleMethodJson",
                data: "{modulepath : '/modules/misc/NewsletterReg.ascx' , methodname: 'RegNewsletter', args: ['" + $("#ctl00_ctl00_VeryTopContentPlaceHolder_NewsLetterReg16_txtNewLetEmail").val() + "','" + $("#txtNewLetName").val() + "','','" + getValueUsingClass(".newsletgroups input[type='checkbox']:checked") + "' , '" + securecode + "','" + enterdsecurecode + "'] }",
                contentType: "application/json; charset=utf-8",
                dataType: "json",
                success: function (msg) {

                    if (msg.d.ErrorMessage == null) {
                        if (msg.d.Message == "ok") {
                            $(".newsletmsg").html("با موفقیت ثبت شد.");
                        }
                    } else {
                        $(".newsletmsg").html(msg.d.ErrorMessage);
                        chanagesecurecode(securecode, 'ctl00_ctl00_VeryTopContentPlaceHolder_NewsLetterReg16_Captcha_US1', 'ctl00_ctl00_VeryTopContentPlaceHolder_NewsLetterReg16_Captcha_US1_SecureCodeLabel', 'fa');
                        $("#ctl00_ctl00_VeryTopContentPlaceHolder_NewsLetterReg16_SecureCodeTextBox").val('');

                    }


                },
                error: function () {
                    $(".newsletmsg").html('error: try again!');

                }

            });
        });

    });


    function getValueUsingClass(selector) {
        /* declare an checkbox array */
        var chkArray = [];

        /* look for all checkboes that have a class 'chk' attached to it and check if it was checked */
        $(selector).each(function () {
            chkArray.push($(this).val());
        });

        /* we join the array separated by the comma */
        var selected;
        selected = chkArray.join(',') + ",";

        /* check if there is selected checkboxes, by default the length is 1 as it contains one single comma */
        return selected;
    }

</script>



                
