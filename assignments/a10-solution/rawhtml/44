<script language="javascript">
    $(document).ready(function () {

        $("#ctl00_ctl00_ContentPlaceHolder_ContentPlaceHolder_NewsContent1_Comments_Ctrl1_SendButton").click(function () {
            var NameTextBox = $("#ctl00_ctl00_ContentPlaceHolder_ContentPlaceHolder_NewsContent1_Comments_Ctrl1_NameTextBox").val();
            var EmTextBox = $("#ctl00_ctl00_ContentPlaceHolder_ContentPlaceHolder_NewsContent1_Comments_Ctrl1_EmTextBox").val();
            var bodyTextBox = $("#ctl00_ctl00_ContentPlaceHolder_ContentPlaceHolder_NewsContent1_Comments_Ctrl1_bodyTextBox").val();
            var CommentServiceID = '2529412';
            var ServiceTitle = '';
            var ServiceName = '';
            var lang = 'fa';
            var subject = $("#ctl00_ctl00_ContentPlaceHolder_ContentPlaceHolder_NewsContent1_Comments_Ctrl1_SubjectTextBox").val();
            var talkToTextBox = $("#ctl00_ctl00_ContentPlaceHolder_ContentPlaceHolder_NewsContent1_Comments_Ctrl1_TalkToTextBox").val();
            var NotesTextBox = $("#ctl00_ctl00_ContentPlaceHolder_ContentPlaceHolder_NewsContent1_Comments_Ctrl1_NotesTextBox").val();
            var enterd = $("#ctl00_ctl00_ContentPlaceHolder_ContentPlaceHolder_NewsContent1_Comments_Ctrl1_CaptchaTextBox").val();
            var captcha = $("#ctl00_ctl00_ContentPlaceHolder_ContentPlaceHolder_NewsContent1_Comments_Ctrl1_Captcha_US1_SecureCodeLabel").val();

            if (NameTextBox == "" ) {
                $("#ctl00_ctl00_ContentPlaceHolder_ContentPlaceHolder_NewsContent1_Comments_Ctrl1_lblMessage").text('لطفا گزینه های ستاره دار را حتما وارد نمائید');
                return;
            }

            

            $("#ctl00_ctl00_ContentPlaceHolder_ContentPlaceHolder_NewsContent1_Comments_Ctrl1_lblMessage").html('<span class="LoadingNew">در حال ارسال اطلاعات ...</span>');
            $.ajax({
                type: "Post",
                url: "/NetServices/ajax.aspx/SendComment2",
                data: "{NameTextBox : '" + NameTextBox + "' , EmTextBox: '" + EmTextBox + "' , bodyTextBox : '" + bodyTextBox + "' , CommentServiceID : " + CommentServiceID + " , ServiceTitle:'" + ServiceTitle + "' , ServiceName : '" + ServiceName + "' , lang : '" + lang + "', subject : '" + subject + "', talkTo : '" + talkToTextBox + "' ,notes: '" + NotesTextBox +  "' ,enterd : '" + enterd + "', captcha: '" + captcha + "' }",
                contentType: "application/json; charset=utf-8",
                dataType: "json",
                success: function (msg) {
                    // Replace the div's content with the page method's return.



                    if (msg.d == "ok") {
                        $("#ctl00_ctl00_ContentPlaceHolder_ContentPlaceHolder_NewsContent1_Comments_Ctrl1_lblMessage").html($("#ctl00_ctl00_ContentPlaceHolder_ContentPlaceHolder_NewsContent1_Comments_Ctrl1_Label1").html());
                    }
                    else {

                        $("#ctl00_ctl00_ContentPlaceHolder_ContentPlaceHolder_NewsContent1_Comments_Ctrl1_lblMessage").text(msg.d);
                    }

                    ctl00_ctl00_ContentPlaceHolder_ContentPlaceHolder_NewsContent1_Comments_Ctrl1_Captcha_US1_chanagesecurecode();
                },
                error: function () {

                    $("#ctl00_ctl00_ContentPlaceHolder_ContentPlaceHolder_NewsContent1_Comments_Ctrl1_lblMessage").text('error');

                }

            });



        });
    });

</script>
