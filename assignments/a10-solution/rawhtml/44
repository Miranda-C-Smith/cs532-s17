function ToPersianNumber(number) {
    if (number == "") return number;
    var main = "0123456789";
    var repl = "۰۱۲۳۴۵۶۷۸۹";
    var pNumber = number.toString();

    for (var i = 0; i < main.length; i++) {
        while (pNumber.indexOf(main[i]) != -1) {
            pNumber = pNumber.replace(main[i], repl[i]);
        }
    }
    return pNumber;
}
</script>

