    $(".othermedia, .TabiNewsHomePage .TabbedPanelsTabGroup li:nth-child(2), .TabiNewsHomePage .TabbedPanelsTabGroup li:nth-child(3), .Gozaresh .TabbedPanelsTabGroup li.TabbedPanelsTab:nth-child(3)").tooltip({track: true});

