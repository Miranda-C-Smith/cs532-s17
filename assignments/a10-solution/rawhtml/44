 <script language="javascript">
     function ctl00_ctl00_VeryTopContentPlaceHolder_NewsLetterReg16_Captcha_US1_chanagesecurecode(captcha) {
         var captchaimg = $("#ctl00_ctl00_VeryTopContentPlaceHolder_NewsLetterReg16_Captcha_US1captchaimg").attr("src");
         $("#ctl00_ctl00_VeryTopContentPlaceHolder_NewsLetterReg16_Captcha_US1captchaimg").attr({ "src": "/Theme1/images/Loading2.gif" });
         
         $("#ctl00_ctl00_VeryTopContentPlaceHolder_NewsLetterReg16_Captcha_US1refreshcaptcahimg").hide();
         $.ajax({
             type: "Post",
             url: "/NetServices/captcha.aspx/SecureCode",
             data: "{captcha: '"+captcha+"'}",
             contentType: "application/json; charset=utf-8",
             dataType: "json",
             success: function (msg) {
                 // Replace the div's content with the page method's return.

                 if (msg.d != "") {
                        
                     $("#ctl00_ctl00_VeryTopContentPlaceHolder_NewsLetterReg16_Captcha_US1_SecureCodeLabel").val(msg.d);
                     $("#ctl00_ctl00_VeryTopContentPlaceHolder_NewsLetterReg16_Captcha_US1captchaimg").attr({ "src": "/fa/JpegImage.aspx?action=" + msg.d + "&t=" +   new Date().getTime() });
                     $("#ctl00_ctl00_VeryTopContentPlaceHolder_NewsLetterReg16_Captcha_US1refreshcaptcahimg").show();

                 }
             },
             error: function () {

                 $("#ctl00_ctl00_VeryTopContentPlaceHolder_NewsLetterReg16_Captcha_US1captchaimg").attr({ "src": captchaimg });
                 $("#ctl00_ctl00_VeryTopContentPlaceHolder_NewsLetterReg16_Captcha_US1refreshcaptcahimg").show();
             }

         });
     }

     
     $(document).ready(function() {
        ctl00_ctl00_VeryTopContentPlaceHolder_NewsLetterReg16_Captcha_US1_chanagesecurecode('323e0b71-eeff-43da-bfa9-828770778122');
     });

 </script>
 <img title="Security Code" class="BorderT1" id="ctl00_ctl00_VeryTopContentPlaceHolder_NewsLetterReg16_Captcha_US1captchaimg" src='/fa/JpegImage.aspx?action=323e0b71-eeff-43da-bfa9-828770778122' />
 <input name="ctl00$ctl00$VeryTopContentPlaceHolder$NewsLetterReg16$Captcha_US1$SecureCodeLabel" type="text" id="ctl00_ctl00_VeryTopContentPlaceHolder_NewsLetterReg16_Captcha_US1_SecureCodeLabel" style="display:none" value="323e0b71-eeff-43da-bfa9-828770778122" />
 <a href="javascript:ctl00_ctl00_VeryTopContentPlaceHolder_NewsLetterReg16_Captcha_US1_chanagesecurecode('323e0b71-eeff-43da-bfa9-828770778122')"><img src='/Theme1/images/Refresh1.png' title="کد دیگر" align="top" id="refreshcaptcahimg" /> </a>

 

    <div class="caption">کد امینیتی</div>
    <div class="entry">
