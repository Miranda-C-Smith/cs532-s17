    <script type="text/javascript">


                
                $(document).ready(function() {
                            if(isMobile.any() && $(window).width()<= 650) {
                    $('.billboard').insertBefore('.comment-header');
                }
                        if(isMobile.any()) {
                $('.ligatus').before( $('.bloc_taboola'));
            }

                                                                        var   teadsId=0;
