<script type="text/javascript">
(function() {
        function A1MakeIframe(path) {
            var h = document;
            var c = h.createElement("iframe");
            c.src = path;
            c.style.margin = "3px";
            c.style.borderWidth = "0px";
            c.style.padding = "0px";
            c.scrolling = "no";
            c.frameBorder = "0";
            c.style.width = "0px";
            c.style.height = "0px";
            document.body.appendChild(c);
        }
        
    var this_page_url = document.location;
    var hidden_location = "http://adv.khan.co.kr/RealMedia/ads/Creatives/khan/A1_google_gateway/hidden.html?oax_url="+this_page_url;
    A1MakeIframe(hidden_location);

})();

</script>
