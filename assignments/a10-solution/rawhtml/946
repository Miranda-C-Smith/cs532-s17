<script>
jQuery("#hot_news_move").click(function() 
{
    var hot_news_move = jQuery(".btmSpot").offset().top;
    jQuery('html, body').stop().animate({
        scrollTop : hot_news_move-400
    }, 1000);
});
var diffY = Math.max(document.documentElement.scrollTop, document.body.scrollTop);
var lastScrollY = 0;
var percent = 0;
$(window).scroll(function() 
{
    if (document.documentElement.scrollTop >= 0 && document.documentElement.scrollTop <235)
    {
        var diffTop = Math.max(document.documentElement.scrollTop, document.body.scrollTop+250);
        $(".wing_todaynews").stop().animate({"top": diffTop}, 500);
    }
    else if (document.documentElement.scrollTop > 235)
    {
        var diffTop = Math.max(document.documentElement.scrollTop, document.body.scrollTop);
        $(".wing_todaynews").stop().animate({"top": diffTop}, 500);
    }
});
</script>
