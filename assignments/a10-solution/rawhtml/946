<script type="text/javascript">
function main_go_click() 
{
    parent.location.href = "http://www.khan.co.kr/";
}

function main_eng_click() 
{
    //parent.location.href ="http://english.khan.co.kr/";
    window.open('http://english.khan.co.kr/');
}
</script>


