<script type="text/javascript">
var site = {
    'news': { selector: '.art_body p', page: 'khan' },
    'biz': { selector: '.art_body p', page: 'khan_biz' }
}, pageName = (/^(?:http:\/\/)(\w+)/.exec(document.location.href) || ['', 'news'])[1], info = site[pageName] || {};
    iwmads([info.selector || '#_article br', ':eq(', info.index || 5, ')'].join(''), {mediaId: 'khan', site: ['www.khan.interplay/', info.page || pageName, '_news'].join(''), xButton: info.xButton || true, reNewsId: /([\?&]*art_?id=)(\w+)(&?.*)$/});
</script>
