    scr.type = 'text/javascript';  scr.async = true;
    scr.src = ((pro === 'https:') ? 'https' : 'http') + '://linkback.khan.co.kr/src/lb4khan.min.js';
    scr.charset='utf-8';
    if(!linkback.l){linkback.l=true; tar.insertBefore(scr, tar.firstChild);}
})();
</script>
    <span id="khan_view_icon_ad">
