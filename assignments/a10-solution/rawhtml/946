    {
        jQuery('#header .srch').css('top','228px');
    });
    jQuery('body').css('background-position','0 100px');
}	

function topAdClose(name) 
{
    jQuery('div.ad_top').slideUp();
    khanSetCookie( name , "close", 7, 'm', '.khan.co.kr' );
    jQuery('#header .srch').css('top','128px');
    jQuery('body').css('background-position','0 0');
}
</script>
