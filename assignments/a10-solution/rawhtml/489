function callBack(htmlTag, color, json) {
  var texAdLinkStyle = "display:block;font-weight:bold;color:" + color + ";font-size:10pt;line-height:normal;text-decoration:underline;background-color:transparent;padding:0px;margin:0px;font-style:normal;font-family:arial,helvetica,sans-serif;text-align:left;";
  var texAdTitleStyle = "display:block;font-weight:bold;color:black;font-size:14px;line-height:normal;text-decoration:none;background-color:transparent;padding:0px;margin:0px;font-style:normal;font-family:arial,sans-serif;text-align:right;";
  var texAdDescriptionStyle = "display:block;font-weight:bold;color:" + color + ";font-size:10pt;line-height:normal;text-decoration:underline;background-color:transparent;padding:0px;margin:0px;font-style:normal;font-family:arial,helvetica,sans-serif;text-align:left;";
  var texAdUrlStyle = "display:block;font-weight:normal;white-space:nowrap;text-decoration:underline;font-size:11px;line-height:normal;color:#blue;background-color:transparent;padding:0px;margin:0px;font-style:normal;font-family:arial,sans-serif;text-align:right;";
  var str = "<div style='width:575px;height:14px;border:0px;'>";
  for (var i = 0; i < json.Placements.length; i++) {
    for (var j = 0; j < json.Placements[i].Ads.length; j++) {
	  var ad = json.Placements[i].Ads[j];
	  str += "<div class='hover'><table style='width:100%; padding:2px;'><tbody><tr>";
	  str += "<td style='min-width:570px;'><a href='" + ad.LandingPage + "' target='_blank' style='" + texAdLinkStyle + "'><span style='" + texAdDescriptionStyle + "'>" + ad.Description + "</span>";
	  str += "</a></td>";
	  str += "</tr></tbody></table></div>";
	}
  }
  str+= "</div>";
  document.getElementById(htmlTag).innerHTML = str;
  return true;
}
</script>
