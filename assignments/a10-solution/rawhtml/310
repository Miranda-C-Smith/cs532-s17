function sw(url,winName,w,h) {if (window.open) {if (poppedWindow) { poppedWindow = ''; } windowW = w; windowH = h; var windowX = (screen.width/2)-(windowW/2); var windowY = (screen.height/2)-(windowH/2); var myExtra = "status=no,menubar=no,resizable=yes,toolbar=no,scrollbars=no,addressbar=no"; var poppedWindow = window.open(url,winName,'width='+w+',height='+h+',top='+windowY+',left=' + windowX + ',' + myExtra + '');poppedWindow.focus();setTimeout(poppedWindow,3000);} else { alert('Sharing error. Please be sure your security settings allow popup windows to be opened by our web application.'); }}
