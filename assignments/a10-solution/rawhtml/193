<script type="text/javascript">
	$(function () {
		$('.social-tabs').find('.link-tab-toggle').click(function(){
			var tab		= $(this).parents('li');
			if( !tab.hasClass('active') ){
				tab.addClass('active').siblings('li').removeClass('active')
				tab.parents('.social-tabs').find('.social-tab').removeClass('active').filter(':eq('+ tab.index() +')').addClass('active');
			}
		});
	});
</script>

