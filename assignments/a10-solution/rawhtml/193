<script type="text/javascript">
    $(function(){
        $('#read_comment .tabs .tab').click(function(){
            var i = $(this).data('item');
            $('#read_comment .tabs .tab').removeClass('active');
            $(this).addClass('active');

            $('#read_comment .content-vacw').removeClass('active').eq(i-1).addClass('active');
        });

        $('#read_comment .timer-tabs .tab').click(function(){
            var t = $(this).data('item');
            $('#read_comment .timer-tabs .tab').removeClass('active');
            $(this).addClass('active');
            $('#read_comment .content-vacw.active .viewest').removeClass('active').eq(t-1).addClass('active');
        });
    });
</script>


