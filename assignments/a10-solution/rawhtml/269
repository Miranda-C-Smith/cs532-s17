<script type="text/javascript">
function addFavorite(){
    var favo_id  = 15463;
    var check_id = 'needLogin';
    if(check_id == 'needLogin'){
        $('#ShowErrorAddFavorite').removeClass('hide');
    } else {
        $.ajax({
            type: "POST",
            url: "/saveFavorite",
            data: {action:"ajax", favo_id: favo_id},
            success: function(data) {
            location.reload();
            }
        });
    }
}
function rmFavorite(){
    var favo_id  = 15463;
    var check_id = 'needLogin';
    if(check_id == 'needLogin'){
        $('#ShowErrorAddFavorite').removeClass('hide');
    } else {
        $.ajax({
            type: "POST",
            url: "/removeFavorite",
            data: {action:"ajax", favo_id: favo_id},
            success: function(data) {
            location.reload();
            }
        });
    }
}
function showphone(nhadatId) {        
    if(nhadatId) {          
        $.ajax({
            type: "POST",
            url: "/home/showphone",
            data: {action:"ajax", nhadat_id: nhadatId},
            success: function(data) {  
                var obj = jQuery.parseJSON(data);
                var phone = obj.phone;
                $('.sticky-float-contact').html('<a data-phoneext="'+nhadatId+'" class="btn btn-call-contact btn-action-showphone btn-block" href="tel:'+phone+'"><div class="icon img-call spacer-right-5"></div>'+phone+'</a>');
                $('.phone-contact').html('<a data-phoneext="'+nhadatId+'" class="btn btn-call-contact btn-action-showphone btn-block" href="tel:'+phone+'"><div class="icon img-call spacer-right-5"></div>'+phone+'</a>');
            },
            error: function (result) {
                var errorMsg = "Co loi xay ra !!";
                console.log(errorMsg);
            }
        });
    }
}
</script></div>
