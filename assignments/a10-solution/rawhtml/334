<div class="extras all-100">
<script type="text/javascript">
  var reposition= function(father, son, offset){
  	// window position ( only y is important )
  	var currentPos = Ink.Dom.Element.scroll()[1];

  	// element sizes . Assumes px
  	var father_size = Ink.Dom.Element.elementHeight(father);
 	var son_size    = parseFloat(Ink.Dom.Css.getStyle(son,'font-size')||0);
 	var son_padding = parseFloat(Ink.Dom.Css.getStyle(son,'paddingTop')||0);
 	var father_padding = parseFloat(Ink.Dom.Css.getStyle(father,'paddingTop')||0);

 	// son opacity
 	var opacity = 1-currentPos/(father_size+offset);

 	// son vertical offset. should position the center of the son, roughly in the center of available space
  	var transform_pos = Math.floor( father_size - (father_size-currentPos)/2 - son_padding - father_padding - son_size/2 - offset );

  	// define and set style
var style="opacity:"+opacity +";transform: translate(0px, "+ transform_pos + "px);"+"ms-transform: translate(0px, "+ transform_pos + "px)";  	Ink.Dom.Css.setStyle(son,style);
  };

  Ink.Dom.Event.observe(window, 'scroll', function(ev){
	reposition( Ink.s("#main_header"), Ink.s("#main_header h1"), 16 );
  });
  Ink.Dom.Event.observe(window, 'resize', function(ev){
	reposition( Ink.s("#main_header"), Ink.s("#main_header h1"), 16 );
  });
</script>
