		<script type="text/javascript">
		// キーワードを FreakOut DMP に送る
		var _itm_fout_params = function(){
			var d = document;
			var metas = d.getElementsByTagName('head')[0].getElementsByTagName('meta');
			var params = {};
			var keys = false;
			for(var i = 0; i < metas.length; i++){
				if(metas[i].name == 'keywords' && metas[i].content){
					keys = metas[i].content.split(',').filter(function(e){return e !== '';});
				}
			}
			if(keys){
				for(var i = 0; i < keys.length; i++){
					keys[i] = keys[i].replace(/\s/g,'_').replace(/\t/g,'__');
					params[keys[i]] = 'true';
				}
			}
			// console.log(params);
			return params;
		};
		var _fout_queue = _fout_queue || {}; if (_fout_queue.segment === void 0) _fout_queue.segment = {};
		if (_fout_queue.segment.queue === void 0) _fout_queue.segment.queue = [];
		_fout_queue.segment.queue.push({
		  'user_id': 9303,
		  'params':_itm_fout_params()
		});
		(function() {
		  var el = document.createElement('script'); el.type = 'text/javascript'; el.async = true;
		  el.src = (('https:' == document.location.protocol) ? 'https://' : 'http://') + 'js.fout.jp/segmentation.js';
		  var s = document.getElementsByTagName('script')[0]; s.parentNode.insertBefore(el, s);
		})();
		</script>
	

