<script>
/*
	指定要素 > 子要素の atag を oidcount
*/
function atag2oid(selector_id,oid_name){
	var d = document;
	var wrap = d.getElementById(selector_id);	
	var atags = wrap.getElementsByTagName('a');
	appendEventListener(window,'load',function(e){
		for(var i = 0; i < atags.length; i++){
			appendEventListener(atags[i],'click',function(e){
				var str = masterChannel() + ',' + masterType() + ',' + e.target.innerHTML;
				sc_count_oid(selector_id,str);
			},false);
		}
	},false);
};
atag2oid('globalNavigationGroup0');
</script>

