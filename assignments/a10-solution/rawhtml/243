<script>
(function(){
	var d = document,
			endlink,         /* 関連記事枠 */
			endlink_ul,      /* 関連記事＆関連リンク */
			endlink_art,     /* 関連記事ul */
			endlink_artlist, /* 関連記事li */
			copyright,       /* コピーライト */
			div = d.createElement('div'),
			atag = d.createElement('a'),
			msg_pre, /* preview でのメッセージ用 */
			refURL = getUrl().path_cutpageing_cutparam.replace('.html','_00.html');
	atag.href = refURL;
	atag.innerHTML = 'この記事の関連情報';
	div.appendChild(atag);
	div.id = 'cmsReferenceBtn';
	if (d.getElementById('endlinkConnection')) {
		/* 関連記事枠がある場合 */
		endlink = d.getElementById('endlinkConnection');
		endlink_ul = endlink.getElementsByTagName('ul');
		for (var i=0; i<endlink_ul.length; i++) {
			if (endlink_ul[i].id == 'art') {
				endlink_art = endlink_ul[i];
				endlink_artlist = endlink_art.getElementsByTagName('li');
			}
		}
		/* 関連記事6件目以降を非表示 */
		if(endlink_artlist && endlink_artlist.length > 5) {
			// preview では隠さず、非表示になる旨を表示
			if(getUrl().domain.match(/preview/)) {
				msg_pre = d.createElement('div');
				msg_pre.innerHTML = '※↓6件目以降は公開時に非表示になります↓※';
				msg_pre.style.clear = 'both';
				msg_pre.style.margin = '10px 0px';
				msg_pre.style.fontSize = '16px';
				msg_pre.style.color = '#f75555';
				endlink_art.insertBefore(msg_pre,endlink_artlist[5]);
			} else {
				for (var j=5; j<endlink_artlist.length; j++) {
					endlink_artlist[j].style.display = 'none';
				}
			}
		}
		/* 関連記事がある場合はその下に、無ければ関連記事枠の上に表示 */
		if (endlink_art !== undefined) {
			div.style.margin = '-30px 0 0';
			endlink_art.parentNode.insertBefore(div,endlink_art.nextSibling);
		} else {
			endlink.insertBefore(div,endlink.firstChild);
		}
	} else if (d.getElementById('end') || d.getElementsByClassName('ctrl').length == 0) {
		/* 最終ページもしくは単体ページで、関連記事枠が無い場合 */
		copyright = d.getElementById('cmsCopyright');
		copyright.parentNode.insertBefore(div,copyright);
	}
})();
</script>

