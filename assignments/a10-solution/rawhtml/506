    <script>

        jQuery(document).ready(function($) {



            var transition = 'fade';

            if($(window).width() < 768){transition = 'move';};



            $('.s_bx_slide').royalSlider({

                addActiveClass: true,

                arrowsNav: false,

                controlsInside:true,

                controlNavigation: false,

                globalCaption: false,

                navigateByClick: true,

                sliderDrag: true,

                fadeinLoadedSlide: true,

                allowCSS3: true,

                loop: true,

                transitionType: transition,

                randomizeSlides: false,

                autoPlay: {

                    enabled: true,

                    delay:5000,

                    pauseOnHover: false,

                    stopAtAction: false,

                },

            });



            var space = 0;

            if($(window).width() < 768){space = 15;};



            var arrow = true;

            if($(window).width() < 1185){arrow = false;};    



            $('.servicios .bx_slide').royalSlider({

                addActiveClass: true,

                arrowsNav: arrow,

                controlsInside: false,

                controlNavigation: 'bullets',

                globalCaption: false,

                navigateByClick: true,

                sliderDrag: true,

                loop: true,

                transitionType:'move',

                slidesSpacing: space,

                autoPlay: {

                    enabled: true,

                    delay:4500,

                    pauseOnHover: true,

                    stopAtAction: false,

                },

                keyboardNavEnabled: true,

                visibleNearby: {

                    enabled: true,

                    centerArea: 0.3333,

                    center: true,

                    breakpoint: 768,

                    breakpointCenterArea: 1,

                    navigateByCenterClick: true

                },

            });



        });

    </script>



