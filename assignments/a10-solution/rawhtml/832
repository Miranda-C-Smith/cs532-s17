t-article-post").not(".loaded").last().val();if(nextArticlePost){$(".next-article-post").not(".loaded").addClass("loaded");var hashCode=getHashCodeArray(readListToLoad);$.ajax({url:"/wp-content/themes/cbr/ajax-action.php?action=loadReadListPosts&ajax_post_id="+readListToLoad+"&hashCode="+hashCode,type:"GET",success:function(data){$(".infinite-scroll-wrap").append(data);picturefill();mainMenu();count=count+readListToLoad.length;if(count<=total){nextPostId=generateNextPostId();$(".next-article-post").not(".loaded").last().val(nextPostId);}articles=$('article.article');var size=articleSet.length;for(var i=0;i<readListToLoad.length;i++){articleSet.push({index:size,post:readListToLoad[i],progress:5,article:$('#post-'+readListToLoad[i])});size++;}if(callback){callback();}}});}};$('.read-list-articles li').on('click',function(){if(userDevice==='mobile'||$(window).width()<=1023){var link=$(this).find('.img-wrapper a').attr("href");window.location.href=link;return false;}var postToLoad=$(this).attr('data-post-id');var jumpOffset=75;if(currentPosts.indexOf(postToLoad)==-1){var start=getReadListStartIndex();var end=readlistPosts.indexOf(postToLoad)+1;var readListToLoad=readlistPosts.slice(start,end);currentPosts=currentPosts.concat(readListToLoad);loadReadListPosts(readListToLoad,function(){$('html, body').animate({scrollTop:$("article#post-"+postToLoad).offset().top-jumpOffset},200);var loadingScroll=setInterval(function(){$('html, body').animate({scrollTop:$("article#post-"+postToLoad).offset().top-jumpOffset},200);if(($(window).scrollTop()>=($("article#post-"+postToLoad).offset().top-(jumpOffset+2)))&&($(window).scrollTop()<=($("article#post-"+postToLoad).offset().top-(jumpOffset-2)))){clearInterval(loadingScroll);}},300);});}else{$(window).scrollTop($("article#post-"+postToLoad).offset().top-jumpOffset);var scroll=setInterval(function(){$(window).scrollTop($("article#post-"+postToLoad).offset().top-jumpOffset);if(($(window).scrollTop()>=($("article#post-"+postToLoad).offset().top-(jumpOffset+2)))&&($(window).scrollTop()<=($("article#post-"+postToLoad).offset().top-(jumpOffset-2)))){clearInterval(scroll);}},300);}return false;});$('.mobile-progress-wrap .i-arrow').on('click',function(){if($(this).hasClass('is-active')){$('html').addClass('read-list-open');}else{$('html').removeClass('read-list-open');}});});</script>
