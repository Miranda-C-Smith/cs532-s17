<script type="text/javascript">
<!--//--><![CDATA[//><!--
(function($) {
var deviceWidth = window.matchMedia( "(min-width: 768px)" ).matches;

//If it is mobile then stick the leaderboard to bottom.
if(!deviceWidth) {
 $('.site-nav__leaderboard').addClass('site-nav__scroll');
}

// If it is desktop then stick the leaderboard to top
if(deviceWidth) {
    $(window).scroll(function() {
          if ($(window).scrollTop() > 60) {
            $('.site-nav__leaderboard').addClass('site-nav__leaderboard-sticky');
         }
         if ($(window).scrollTop() <= 60) {
             $('.site-nav__leaderboard').removeClass('site-nav__leaderboard-sticky');
          }
    });
}
})(jQuery);
//--><!]]>
</script>
