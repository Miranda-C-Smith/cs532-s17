			var Terminos = $("#marcar").val();
			if($("form.comentario").valid()){
				$.post("/ajax/", { request: "saveComment", NewsId : NewsId, Name : Name, Email : Email, Comentario : Comentario,  Terminos : Terminos }, function(){$("#enviar").attr("disabled", ""), $("#comment").append("<div id='msg-comment' style='display:none'>Gracias por su comentario.</div>"); $("#msg-comment").fadeIn();});
				document.getElementById("comment").reset();
			}
			return false;
		});			
	});
	
	$(document).ready(function() {
		$("form.comentario").validate();
	});
</script>
