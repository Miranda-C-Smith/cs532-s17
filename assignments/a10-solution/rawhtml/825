    </div>
<div class="clear"></div>
  <script type="text/javascript">
  (function($){
      $(document).ready(function(){
          var hastUrl, pathUrlEspecial, UrlEspecial ;
          pathUrlEspecial = window.location.pathname.toString().split("/");
          hastUrl =pathUrlEspecial.pop();
          if(hastUrl!=''){
              jQuery(this).removeClass('active');
              jQuery('#menu-especiales-wrapper ul#menu li a').each(function(){
              var UrlActivate = jQuery(this).attr('href').split('/').pop();
              
              if( UrlActivate === hastUrl ){
                  jQuery(this).addClass('active');
              
                }
              })
           }
      });  //Termina ready
  })(jQuery);
  </script>

</div>
