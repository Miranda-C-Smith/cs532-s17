<script>
  var gptran = false;
  function gptrun() {
    if (gptran) {
      return;
    }
    gptran = true;    

    for(var z in fastlane.slots ){
      rubicontag && rubicontag.setTargetingForGPTSlot && rubicontag.setTargetingForGPTSlot(fastlane.slots[z] );
    }
    googletag.pubads().refresh();
  };


  var rubicontag = rubicontag || {};
  rubicontag.cmd = rubicontag.cmd || [];
  var adslots = [];

  rubicontag.cmd.push(function(){
    var index = 1;
    for(var i in rubicon_extends ){   

      var adunit = rubicon_extends[i].adunit;
      var size   =  JSON.parse(rubicon_extends[ i ].size );
      var placeholder_id = rubicon_extends[ i ].placeholder_id;
      var adunit_hitory = rubicon_extends[ i ].adunit_hitory;
      var position = 'atf';
      if(index  >= 4){
        position = 'btf';
      }
      rubicontag.defineSlot(adunit,size,placeholder_id  ).setPosition( position).setFPI('adunit',adunit_hitory.replace('35139216\/','')).setFPI('position', position );     
      index ++;
    }// end for      

    try{ 
      var a = new Array('gender', 'age', 'education', 'interest', 'product', 'income', 'marital', 'custom', 'cluster', 'career', 'brand', 'industry', 'everybuyer', 'connection');    
      for (nvg_i in a){
        var b = '';
        var c = 'nvg_' + a[nvg_i];
        c = c.substring(0, 10);
        var d = nvg41021.getSegment(a[nvg_i]);
        if (d.search(',') != -1){
          rubicontag.setFPV(c,d.join(','));
        } else {
          rubicontag.setFPV(c,d);
        }
      }
    } catch(e){ console.log('Rubicon - error  Navegg'); };
    rubicontag.run(gptrun);
  });
  </script>
