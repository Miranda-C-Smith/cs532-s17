$(document).ready(function() {

  if($(window).width() > 1039) {
    $('.fb-comments').attr('data-width', 710);
  } else if($(window).width() > 767) {
    $('.fb-comments').attr('data-width', 574);
  } else if($(window).width() > 339) {
    $('.fb-comments').attr('data-width', 336);
  } else {
    $('.fb-comments').attr('data-width', 300);
  }
});
</script>
