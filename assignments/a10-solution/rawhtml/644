  <script type="text/javascript">var MIN_LENGTH = 3;
$(document).ready(function() {
  $("#connected_search_box_ac").keyup(function() {
    var keyword = $("#connected_search_box_ac").val();
    if (keyword.length >= MIN_LENGTH) {
      $('#connected_search_box_ac_results').show();
      $.get("/store/connectedcart_search_autocomplete.php", {keyword: keyword}).done(function(data) {
        $('#connected_search_box_ac_results').html('');
        var results = jQuery.parseJSON(data);
        $(results).each(function(key, value) {
          $('#connected_search_box_ac_results').append('<div class="connected_search_box_ac_results_item">' + value + '</div>');
        })
        $('.connected_search_box_ac_results_item').click(function() {
          var text = $(this).contents(":not(:empty)").first().text();
          $('#connected_search_box_ac').val(text);
        })
      });
    }
    else {
      $('#connected_search_box_ac_results').html('');
    }
  });
  $("#connected_search_box_ac").blur(function() {
    $("#connected_search_box_ac_results").fadeOut(500);
  })
  .focus(function() {
    $("#connected_search_box_ac_results").show();
  });
});</script></head>
