$.cookie('sb-installed', null, {path: '/'});

if ( UA.match(/iPhone|iPod|iPad/i) ) {
config.force = 'ios';
}


if (UA.toLowerCase().indexOf('ucbrowser') < 0) {
$.smartbanner(config);
}

