<script>
    document.addEventListener("WebComponentsReady", function() {
      var cartApi = document.querySelector("#cart-api");
      if (cartApi) {
        cartApi.addEventListener("cartitem:added", function(e) {
          var cartDetail = JSON.parse(e.detail.responseText);
          update_cart_count(cartDetail.count);
        });
      }
    });
    $(function () {
        $(".js-product-list").imagesLoaded(function () {
            $(".js-product-list").masonry({
                itemSelector: '.js-product-box'
            });
        });
        $(".js-category-list").imagesLoaded(function () {
            $(".js-category-list").masonry({
                itemSelector: '.js-category-box'
            });
        });

        
        var periscope_icons = {
          'white': $(
            '.header-social-media img[src$="/social-icon-periscope-white.svg"]'
          ).attr('src').slice(0, -9),
        };

        periscope_icons.black = periscope_icons.white + 'black.svg';
        periscope_icons.pink = periscope_icons.white + 'pink.svg';
        periscope_icons.white += 'white.svg';

        if ($('body').hasClass('body-dark')) {
          $(
            '.header-social-media img[src="' + periscope_icons.white + '"]'
          ).attr(
            'src',
            periscope_icons.black
          );

          $('.replicated-rep-social-media .periscope-icon').hover(
            function() {
              $(this).attr('src', periscope_icons.white);
            }, function() {
              $(this).attr('src', periscope_icons.pink);
            }
          );
        }
        else {
          $('.replicated-rep-social-media .periscope-icon').hover(
            function() {
              $(this).attr('src', periscope_icons.black);
            }, function() {
              $(this).attr('src', periscope_icons.pink);
            }
          );
        }
        
    });
</script>
