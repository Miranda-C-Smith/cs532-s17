function moveScroller2() {
    var move2 = function() {
        var s2 = $("#share");
        var st2 = $(window).scrollTop();
        var ot2 = $("#fixsharetop").offset().top - 105;
        var bt2 = $("#fixsharebottom").offset().top - s2.height() - 124;

        if(st2 > ot2) {
			var vtop = 65;
			if($(".main-menu").css("opacity") == "1") vtop = 65 + $(".main-menu").height();
            s2.css({
                position: "fixed",
                top: vtop,

            });	
        } 

		if(st2 > bt2 ) {
            s2.css({
                position: "absolute",
				top: "inherit",
				bottom	: "15px",
            });
        }

		if(st2 < ot2) {
            s2.css({
                position: "relative",
                top: "0px",
            });
       	}

    };
    $(window).scroll(move2);
    move2();
}
