function processCriteo365464 (json) {
	console.log('API response: ',json);
	var div = document.getElementById('criteo-365464');
	if (json.response_status != 0) {
		/* call passback function here*/
		return;
	}
	var htmlcode = '';
	var products_count = 5;
	htmlcode += '<div class="criteo-title f22 tsa2 tap pa15 red bsht f300" >BELI SEKARANG</div>';
	htmlcode += '<div class="criteo-products" style="padding-right:0px!important">';
	htmlcode += '<div class="criteo-privacy">';
	htmlcode += '<a href="'+ json.privacy.optout_click_url +'" target="_blank"><img src="'+ json.privacy.optout_image_url + '"></a>';
	htmlcode += '</div>';
	for (var i in json.products) {
		if (i >= products_count) continue;
		htmlcode += '<div class="criteo-product fl mr10 artohtitm ovh">';
		htmlcode += '<div class="criteo-product-image">';
		htmlcode += '<a href="'+ json.products[i].click_url +'" target="_blank"><img src="'+ json.products[i].image.url +'"></a>';
		htmlcode += '</div>';
		htmlcode += '<div class="criteo-product-title">';
		htmlcode += '<a href="' + json.products[i].click_url + '" target="_blank">'+json.products[i].title+'</a>';
		htmlcode += '</div>';
		htmlcode += '<!--<div class="criteo-product-description">';
		htmlcode += '<a href="' + json.products[i].click_url + '" target="_blank">'+json.products[i].description+'</a>';
		htmlcode += '</div>-->';
		htmlcode += '<div class="criteo-advertiser">';
		htmlcode += '<a href="' + json.advertiser.logo_click_url + '" target="_blank">'+ json.advertiser.domain +'</a>';
		htmlcode += '</div>';
		htmlcode += '</div>';
	}
	htmlcode += '<div style="clear:both"></div>';
	htmlcode += '</div>';
	for (i in json.impression_pixels) {
		htmlcode += '<img src="' + json.impression_pixels[i].url + '" width="0px" height="0px" style="display:none" >';
	}
	div.innerHTML = htmlcode;
	div.style.display = 'block'; 
};
</script>
