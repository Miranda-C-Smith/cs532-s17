<!-- Error messages -->
<div id="alerts"></div>
<script type="text/javascript" src="/assets/js/cookie.js"></script>
<script type="text/javascript">
$(document).ajaxSuccess(function(e, xhr, settings) {
    if (settings.success)
        return;

    if (settings.dataType === 'html') {
        if (typeof settings.onSuccess === 'function')
            return settings.onSuccess(xhr.responseText, xhr, settings);
        else
            return;
    }

    var res = xhr.responseJSON;
    if (!res) {
        if (xhr.responseText)
            alertError(xhr.responseText, 'html');
        else if (xhr.responseText === 'abort')
            alertError('Cancelled.');
        else
            alertError('Unknown error.');
        if (typeof settings.onError === 'function')
        	settings.onError(e, xhr, settings);
    } else if (res.errors && res.errors.length) {
        res.errors.forEach(alertError);
        if (typeof settings.onError === 'function')
        	settings.onError(e, xhr, settings);
    } else {
        if (settings.successAlert !== false) {
            if (res.messages && res.messages.length)
                res.messages.forEach(alertSuccess);
            else
                alertSuccess('Success!');
        }
        if (typeof settings.onSuccess === 'function')
            settings.onSuccess(res, xhr, settings);
    }
});

$(document).ajaxError(function(e, xhr, settings, err) {
    if (settings.error)
        return;
    alertError(err);

    if (typeof settings.onError === 'function')
        settings.onError(e, xhr, settings, err);    
});

function alertError(err, type) {
	console.log(err);
    var alert = $('<div>').addClass('alert alert-danger')[type === 'html' ? 'html' : 'text'](err).appendTo('#alerts');
    setTimeout(function() { alert.remove(); }, 5000);
};
function alertSuccess(msg) {
    var alert = $('<div>').addClass('alert alert-success').text(msg).appendTo('#alerts');
    setTimeout(function() { alert.remove(); }, 5000);
};
$(function() {
	var alert = function(msg, type) { return (type == 'success' ? alertSuccess : alertError)(msg); };
	if (Cookies.get('__messages')) {
		var messages = Cookies.get('__messages').split('|||');
		for(var i = 0; i < messages.length; i++)
			setTimeout(alert(messages[i].replace(/\+/g, ' '), 'success'), i * 250);
	}
	if (Cookies.get('__errors')) {
		var errors = Cookies.get('__errors').split('|||');
		for(var j = 0; j < errors.length; j++)
			setTimeout(alert(errors[j].replace(/\+/g, ' '), 'danger'), (i + j) * 250);
	}
	Cookies.remove('__messages');
	Cookies.remove('__errors');
});
</script>
