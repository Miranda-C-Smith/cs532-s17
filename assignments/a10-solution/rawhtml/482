<script type="text/javascript">
function clickRecommend(event) {
    var item = event.data;
    item.rank = item.rank.toString(10);
    item.track_id = item.track_id.toString(10);
    i3("create", "clickRec");
    i3("clickRec.set", "detail", {
        via_info: "recommend",
        via_option: item.track_id,
        via_view_rank: item.rank
    });
    i3("clickRec.add", "detail", {
        app_id: item.app_id
    });
    i3("clickRec.send", "click", "recommend", function(){
        window.open = item.link;
    });
}

//<![CDATA[
$(function() {
    var recommend_area = $("#ntg-recommend-data");
    var recommend_item_area = $("#ntg-recommend-item-data");
    var recommend_user_area = $("#ntg-recommend-user-data");

    var viewRecommend  = function(recommends) {
        try {
            if (!recommends || 0 == recommends.length) {
                recommend_item_area.remove();
                return false;
            }
            if ("string" == typeof recommends) {
                recommends = JSON.parse(recommends);
            }
            if (0 == recommends.length) {
                recommend_item_area.remove();
                return false;
            }
            tracking_id = recommends[0].track_id;
            item_cnt    = recommends.length;
            i3("create", "rec");
            i3("rec.set", "recommend", {
                type: tracking_id,
                hit_count: item_cnt,
                page_number: 1,
                limit: item_cnt,
                show_count: item_cnt
            });
            recommend_item_area.show();
            var recommend_template = $("#recommend-data-item-template");
            for (var i = 0; i < recommends.length; i ++) {
                i3("rec.add", "recommend", {app_id: recommends[i].app_id});
                var recommend_item = recommend_template.clone();
                recommend_item.removeAttr("id");
                recommend_item.find("img[name=recommend_item_thumb]").attr({
                    "src" : recommends[i].image,
                    "alt" : recommends[i].title,
                });
                recommend_item.find("a[name=recommend_item_link]").attr({
                    "href" : recommends[i].link,
                });
                recommend_item.find("span[name=recommend_item_title]").text(recommends[i].title);
                recommend_item.click(recommends[i], clickRecommend);
                recommend_item_area.find("ul").append(recommend_item);
            }
            i3("rec.send", "show", "recommend");
            recommend_template.remove();
            recommend_area.show();
        } catch(e) {
            recommend_item_area.remove();
        }
    };


    var viewUserRecommend  = function(recommends) {

        try {

            if (!recommends || 0 == recommends.length) {
                recommend_user_area.remove();
                return false;
            }
            if ("string" == typeof recommends) {
                recommends = JSON.parse(recommends);
            }
            if (0 == recommends.length) {
                recommend_user_area.remove();
                return false;
            }

            tracking_id = recommends[0].track_id;
            item_cnt    = recommends.length;
            i3("create", "rec");
            i3("rec.set", "recommend", {
                type: tracking_id,
                hit_count: item_cnt,
                page_number: 1,
                limit: item_cnt,
                show_count: item_cnt
            });
            recommend_user_area.show();
            var recommend_template = $("#recommend-data-user-template");
            for (var i = 0; i < recommends.length; i ++) {
                i3("rec.add", "recommend", {app_id: recommends[i].app_id});
                var recommend_item = recommend_template.clone();
                recommend_item.removeAttr("id");
                recommend_item.find("img[name=recommend_user_thumb]").attr({
                    "src" : recommends[i].image,
                    "alt" : recommends[i].title,
                });
                recommend_item.find("a[name=recommend_user_link]").attr({
                    "href" : recommends[i].link,
                });
                if(recommends[i].newgame == 'new') {
                    recommends[i].newgame = 'ntg-ttl-cardlist ellipsis-row01 new';
                } else {
                    recommends[i].newgame = 'ntg-ttl-cardlist ellipsis-row01';
                }
                recommend_item.find("div[name=recommend_new_icon]").attr({
                    "class" : recommends[i].newgame,
                });
                recommend_item.find("span[name=recommend_user_title]").text(recommends[i].title);
                recommend_item.click(recommends[i], clickRecommend);
                recommend_user_area.find("ul").append(recommend_item);
            }
            i3("rec.send", "show", "recommend");
            recommend_template.remove();
            recommend_area.show();
        } catch(e) {
            recommend_user_area.remove();
        }
    };

    var title_url = "symphony";

    $.ajax({
        type  : "POST",
        url   : "/netgame_s/" + title_url + "/ajax-getrecommend/",
        data  : {
            "app_id" : 750145,
            "lang"   : navigator.languages ? navigator.languages[0] : (navigator.language || navigator.userLanguage),
            "type"   : "item"
        },
        dataType : "json"
    })
    .done(viewRecommend)
    .fail(function() {
        recommend_item_area.remove();
    });

    $.ajax({
        type  : "POST",
        url   : "/netgame_s/" + title_url + "/ajax-getrecommend/",
        data  : {
        	"app_id" : 750145,
            "lang" : navigator.languages ? navigator.languages[0] : (navigator.language || navigator.userLanguage),
            "type" : "user"
        },
        dataType : "json"
    })
    .done(viewUserRecommend)
    .fail(function() {
        recommend_user_area.remove();
    });

});
//]]>
</script>
