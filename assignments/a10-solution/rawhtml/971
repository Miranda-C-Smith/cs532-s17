        <script type="text/javascript">
    $(function() {
        var homepageVotingRedirectStart = $.countdown.UTCDate(
            0,
2017,
1,
22,
19,
40,
0,
0);

        var britishVideoDates = [{"StartDate":"2017-01-14T18:24:00Z","EndDate":"2017-01-20T12:00:00Z"},{"StartDate":"2017-01-20T15:00:00Z","EndDate":"2017-01-27T12:00:00Z"},{"StartDate":"2017-01-27T15:00:00Z","EndDate":"2017-02-03T12:00:00Z"},{"StartDate":"2017-02-03T15:00:00Z","EndDate":"2017-02-10T12:00:00Z"},{"StartDate":"2017-02-10T15:00:00Z","EndDate":"2017-02-17T12:00:00Z"},{"StartDate":"2017-02-22T19:40:00Z","EndDate":"2017-02-22T21:07:00Z"}];

        var getUTCNow = function() {
            var now = new Date();
            return $.countdown.UTCDate(0, now.getUTCFullYear(), now.getUTCMonth(), now.getUTCDate(), now.getUTCHours(), now.getUTCMinutes(), now.getUTCSeconds());
        };

        var getUTCDate = function(date) {
            return $.countdown.UTCDate(0,
                date.getUTCFullYear(),
                date.getUTCMonth(),
                date.getUTCDate(),
                date.getUTCHours(),
                date.getUTCMinutes(),
                date.getUTCSeconds());
        };


        if (getUTCNow() >= homepageVotingRedirectStart) {
            window.location.href = "/#voting-british-video";
        }

        var parseDate = function(date) {
            var utcDate = getUTCDate(new Date(Date.parse(date)));
            return utcDate;
        };

        var britishVideoModel = {
            britishVideoStartDateTime: null,
            britishVideoEndDateTime: null,
            update: function() {
                var utcNow = getUTCNow();

                var britishVideoState = "";

                if (utcNow < parseDate(britishVideoDates[0].StartDate)) {
                    this.votingStatus = "notStarted";
                    this.britishVideoStartDateTime = parseDate(britishVideoDates[0].StartDate);
                    this.britishVideoEndDateTime = parseDate(britishVideoDates[0].EndDate);
                }
                else if (utcNow > parseDate(britishVideoDates[britishVideoDates.length - 1].EndDate)) {
                    this.britishVideoStartDateTime = parseDate(britishVideoDates[britishVideoDates.length - 1].StartDate);
                    this.britishVideoEndDateTime = parseDate(britishVideoDates[britishVideoDates.length - 1].EndDate);
                    this.votingStatus = "finished";
                }
                else {
                    this.britishVideoStartDateTime = null;
                    this.britishVideoEndDateTime = null;

                    for (var i = 0; i < britishVideoDates.length; i++) {
                        if (utcNow >= parseDate(britishVideoDates[i].StartDate) && utcNow <= parseDate(britishVideoDates[i].EndDate)) {
                            this.britishVideoStartDateTime = parseDate(britishVideoDates[i].StartDate);
                            this.britishVideoEndDateTime = parseDate(britishVideoDates[i].EndDate);
                            this.votingStatus = "started";
                            break;
                        }
                    }

                    if (!this.britishVideoStartDateTime) {
                        this.votingStatus = "temporaryClosed"

                        for (var i = 0; i < britishVideoDates.length; i++) {
                            if (utcNow < parseDate(britishVideoDates[i].StartDate)) {
                                this.britishVideoStartDateTime = parseDate(britishVideoDates[i].StartDate);
                                this.britishVideoEndDateTime = parseDate(britishVideoDates[i].EndDate);
                                break;
                            }
                        }
                    }
                }
            },
            votingStatus: ""
        };

        britishVideoModel.update();

        var updateBritishVideoCountdowns = function() {
            if (britishVideoModel.votingStatus == "started") {
                $("<div />").countdown({
                    until: britishVideoModel.britishVideoEndDateTime,
                    onExpiry: function() {
                        setTimeout(updateBritishVideoVotingView, 2000);
                    }
                });
            }
            else {
                //if (britishVideoModel.votingStatus == "notStarted" || britishVideoModel.votingStatus == "temporaryClosed") {
                //    $("<div />").countdown({
                //        until: britishVideoModel.britishVideoStartDateTime,
                //        onExpiry: function() {
                //            setTimeout(updateBritishVideoVotingView, 2000);
                //        }
                //    });
                //} else if (britishVideoModel.votingStatus == "finished") {
                    window.location.href = "/nominees";
                //}
            }
        };

        var updateBritishVideoVotingView = function() {
            britishVideoModel.update();
            updateBritishVideoCountdowns();
        };

        updateBritishVideoVotingView();
    });
        </script>

