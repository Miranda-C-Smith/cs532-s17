    <script type="text/javascript">
        // check CSS3 and cookies support
        if (!Modernizr.borderradius || !are_cookies_enabled()) {
            window.location.href = "/oldbrowser.html";
        }

        function are_cookies_enabled() {
            var cookieEnabled = (navigator.cookieEnabled) ? true : false;

            if (typeof navigator.cookieEnabled == "undefined" && !cookieEnabled) {
                document.cookie = "britstestcookie";
                cookieEnabled = (document.cookie.indexOf("britstestcookie") != -1) ? true : false;
            }
            return (cookieEnabled);
        }
    </script>
