  function initTitleIcons() {
    //script by http://lctree.fc2web.com/manner/

    var titleIcons = document.getElementsByTagName('span');
    var titleIconsNum = titleIcons.length;
    for (var i = 0; i < titleIconsNum; i++) {
        var im = titleIcons[i];
        if (im.name == null || im.name.indexOf('titleIcon') == -1) {
            continue;
        }
        
        im.className = iconStyle(im.title);
    }
  }
//-->
