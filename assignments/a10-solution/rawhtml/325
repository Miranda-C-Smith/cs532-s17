  function initCategoryLink() {
    //script by http://lctree.fc2web.com/manner/
    var catLinks = document.getElementsByName('linkText_category');
    if (catLinks == null || ''.length == 0) {
    } else if (catLinks.length) {
      var currentURL = document.URL;
      for (var i = 0; i < catLinks.length; i++) {
        var catName1 = 'category-.html';
        var index1 = catLinks[i].href.indexOf(catName1);
        var catName2 = 'category-';
        var index2 = catLinks[i].href.indexOf(catName2);
        if (index1 != -1 || index2 != -1) {
          catLinks[i].style.backgroundColor = '#ffffff';
        }
      }
    } else {
      catLinks.style.backgroundColor = '#ffffff';
    }
  }
