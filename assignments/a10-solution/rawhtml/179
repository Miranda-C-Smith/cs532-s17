<script type="text/javascript" async>
$(window).load(function(){
  var $pTopBtn = $(".pagetop"), 
      $winDef = $(window),
      $defBottom = $pTopBtn.offset().top,
      $pagePath = location.pathname,
      $matchStr = $pagePath.match("/[a-z].+/[0-9]{4}/[a-z].+_[0-9].+_[0-9].+.html?$"),
      followHeight = 150,
      outBrainFlg = 0;
      
  $winDef.scroll(function(){ scrollTopBtn(); });
  function scrollTopBtn(){
    if($matchStr){
      if($('.OUTBRAIN.ob-shrink').length > 0 && outBrainFlg == 0){
         followHeight = followHeight + 250;
         outBrainFlg = 1;
      }
      $(".pagetop").css({'bottom': $defBottom-($winDef.scrollTop()+$winDef.height()-followHeight)});
      if($winDef.scrollTop() > 300){
        $(".pagetop").css('opacity', 0.8);
      }else{
        $(".pagetop").css('opacity', 0);
      }
    }    
  }
});
</script>
