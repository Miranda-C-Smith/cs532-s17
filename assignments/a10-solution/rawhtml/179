<script async>
if(document.getElementById("twitter_cnt_text") != null || document.getElementById("twitter_cnt_text_top") != null){
	$.ajax({
		type: 'GET',
		url: '//jsoon.digitiminimi.com/twitter/count.json?url=' + encodeURI(location.href),
		dataType: 'jsonp',
		timeout: 30000,
        success: function(json){
			if(json.count){
				if(document.getElementById("twitter_cnt_text") != null){
					$("#twitter_cnt_text").append(json['count']);
				}
				if(document.getElementById("twitter_cnt_text_top") != null){
					$("#twitter_cnt_text_top").append(json['count']);
				}
			}else{
				if(document.getElementById("twitter_cnt_text") != null){
					$("#twitter_cnt_text").append(0);
				}
				if(document.getElementById("twitter_cnt_text_top") != null){
					$("#twitter_cnt_text_top").append(0);
				}
			}
     	}
	});
	if(document.getElementById("twitter-cnt") != null){
		document.getElementById("twitter-cnt").href="https://twitter.com/search?q="+encodeURI(location.href)+"&src=typd";
	}
	if(document.getElementById("twitter-cnt_b") != null){
		document.getElementById("twitter-cnt_b").href="https://twitter.com/search?q="+encodeURI(location.href)+"&src=typd";
	}
	if(document.getElementById("twitter-link") != null){
		document.getElementById("twitter-link").href="https://twitter.com/intent/tweet?text="+encodeURIComponent(document.title)+"&url="+encodeURI(location.href);
	}
	if(document.getElementById("twitter-link_b") != null){
		document.getElementById("twitter-link_b").href="https://twitter.com/intent/tweet?text="+encodeURIComponent(document.title)+"&url="+encodeURI(location.href);
	}
}
</script>
