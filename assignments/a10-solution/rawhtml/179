<script async>
if(document.getElementById("fb_cnt_text") != null || document.getElementById("fb_cnt_text_top") != null){
	$.ajax({
		type: 'GET',
		url: 'https://graph.facebook.com/v2.7/?id=' + encodeURI(location.href) + '&access_token=1473288276265783|lub4zk4sNN-ipjkUgVbvhk3-BqI',
		dataType: 'jsonp',
		timeout: 30000,
        success: function(json){
			if('share' in json){
				if(document.getElementById("fb_cnt_text") != null){
					$("#fb_cnt_text").append(json.share.share_count);
				}
				if(document.getElementById("fb_cnt_text_top") != null){
					$("#fb_cnt_text_top").append(json.share.share_count);
				}
			}else{
				if(document.getElementById("fb_cnt_text") != null){
					$("#fb_cnt_text").append('0');
				}
				if(document.getElementById("fb_cnt_text_top") != null){
					$("#fb_cnt_text_top").append('0');
				}
			}
     	}
	});
	if(document.getElementById("fb-link_b") != null){
		document.getElementById("fb-link_b").href="http://www.facebook.com/sharer.php?u="+encodeURI(location.href)+"&amp;t="+encodeURIComponent(document.title);
	}
