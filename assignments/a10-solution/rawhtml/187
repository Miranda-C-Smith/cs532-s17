jindo.$Fn(function() {
	try{		
		 if (jindo.$$.getSingle('.u_cbox_comment_none')) {
			 	var commentArea = jindo.$Element(jindo.$$.getSingle('.u_cbox_btn_view_comment'));
			 	commentArea.hide();
		 }
	} catch(e){
	}
}).delay(1);

</script>

