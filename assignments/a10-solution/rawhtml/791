<script type="text/javascript">

    $().ready(function () {
        $("#CloseMore").click(function () {
            $("#lmMore_8").hide();
        });
        //$("#comboCategories").append(new Option("more", "More"));

        $("#comboSex").change(function () {
            document.location.replace($("#UrlData").text() + "/" + $(this).val());
        });

        $('#hlpFilter select').selectbox({ inputClass: "selectbox selectbox_sm" }).change(function (e, el, val, text) {
            hlShowBy = val;
            hlPagerMin = 0; hlPagerMax = 0; hlDirection = 0; hlShift = 0; hlPage = 1;
            if (!reset_all) {
                getHLPage();
            }
            if (val == 0) text = '';
            setShowByNiche(text);
        });
        $('#hlpRegions select').selectbox().change(function (e, el, val, text) {
            hlRegion = val;
            hlPagerMin = 0; hlPagerMax = 0; hlDirection = 0; hlShift = 0; hlPage = 1;
            if (!reset_all) {
                getHLPage();
            }
            if (val == 0) text = '';
            setRegonNiche(text);
        });
        $('#hlpLanguages select').selectbox({ inputClass: "selectbox selectbox_sm" }).change(function (e, el, val, text) {
            hlLng = val;
            hlPagerMin = 0; hlPagerMax = 0; hlDirection = 0; hlShift = 0; hlPage = 1;
            if (!reset_all) {
                getHLPage();
            }
            if (val == 0) text = '';
            setLangNiche(text);
        });
        $('#hlpSex select').selectbox({ inputClass: "selectbox selectbox_sm" }).change(function (e, el, val, text) {
            hlLng = val;
            hlPagerMin = 0; hlPagerMax = 0; hlDirection = 0; hlShift = 0; hlPage = 1;
            if (!reset_all) {
                getHLPage();
            }
            if (val == 0) text = '';
            if (typeof (setSexNiche) == 'function') {
                setSexNiche(text);
            }
        });
        $('#hlpCategories select').selectbox({ inputClass: "selectbox selectbox_sm" }).change(function (e, el, val, text) {

            if (val == "more") {
                e.preventDefault();
                $("#lmMore_8").show();
                return;
            } else {
                hlLng = val;
                hlPagerMin = 0;
                hlPagerMax = 0;
                hlDirection = 0;
                hlShift = 0;
                hlPage = 1;
                if (!reset_all) {
                    getHLPage();
                }
                if (val == 0) text = '';
                if (typeof (setCatNiche) != "undefined") {
                    setCatNiche(text);
                }
            }
        });
        $('#hlpRooms select').selectbox({ inputClass: "selectbox selectbox_sm" }).change(function (e, el, val, text) {
            if (val.indexOf("q") == 0) {
                hlQueryId = val.substring(1, val.length);
            }
            else {
                hlRoomId = val;
            }
            hlPagerMin = 0; hlPagerMax = 0; hlDirection = 0; hlShift = 0; hlPage = 1;
            if (!reset_all) {
                getHLPage();
            }
        });
        $('#hlpHosts select').selectbox({ inputClass: "selectbox selectbox_sm" }).change(function (e, el, val, text) {
            hlHostStatus = hlpHosts;
            hlPagerMin = 0; hlPagerMax = 0; hlDirection = 0; hlShift = 0; hlPage = 1;
            if (!reset_all) {
                getHLPage();
            }
        });

        if (typeof (initFilterClickMeClass) != "undefined") {
            initFilterClickMeClass();
        }
        if (typeof (InitFiltersClick) != "undefined") {
            InitFiltersClick();
        }

    });
    function updateShowBy(showBy) {
        $("#hlpFilter ul li#_input_" + showBy).click();
        setNicheTotal();
    }

    function updateLang(lang) {
        $("#hlpLanguages ul li#_input_" + lang).click();
        setNicheTotal();
    }

    function updateRegion(reg) {
        $("#hlpRegions ul li#_input_" + reg).click();
        setNicheTotal();
    }
</script>




