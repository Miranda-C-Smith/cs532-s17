<script type="text/javascript">

    var add2Cart_res = {
        add2cart: 'Add me to your Quick List',
        removeFromCart: 'Remove from Quick List',
        added: 'Added to Quick List',
        removed: 'Removed from Quick List',
        add2fav: 'Add to Favorites',
        removeFav: 'Remove from Favorites',
        notify: 'Get notified when I`m online',
        stopNotify: 'Stop online notification',
        onlyForMembers: 'To enjoy this feature and many others,<br/>sign up easily and quickly!',
        onlyForMembersBtn: 'Sign up Now!',
        onlyForMembers_favorites: 'Become a member to add $nickname$ to your favorites',
        buttons: {
            cancel: 'Cancel',
            ok: 'OK'
        },
        alreadyMemberLink: {
            already_member: 'Already a $SITE_NAME$ member?',
            login_now: 'Login now'
        },
        signupUrl: '/signup'
    };

    $().ready(function () {
        $(".add2Cart_li").click(function (e) {
            add2Cart_add2Favorite($("a", this)[0]);
            e.preventDefault();
            e.stopPropagation();
        });
    });

    function rv_incCounter(c, selector) {
        var $$ = $(selector);
        var count = $$.text() / 1 + c;
        count = count > 30 ? 30 : count;
        $$.text(count);
    }

    function add2Cart(item, ignoreTexts) {
        var $$ = $(item);
        var action = $$.attr("act");
        if (action[0] != '_') {
            add2Cart_send("action=" + action + "&storage=MyCart&id=" + $$.attr("hid"), function (r) {
                if (r.responseText == "OK") {
                    if (action == "remove") {
                        $$
                        .attr("act", "add")
                        .removeClass("add2Cart_" + action)
                        .addClass("add2Cart_add");
                        $$.attr("title", add2Cart_res.add2cart);
                        if (!ignoreTexts) {
                            $$.html(add2Cart_res.add2cart);
                        }
                        rv_incCounter(-1, "span.mcCount");
                    }
                    else {
                        $$
                        .attr("act", action == "add" ? "_remove" : "_add")
                        .removeClass("add2Cart_" + action)
                        .addClass("add2Cart_wait");
                        $$.attr("title", action == "add" ? add2Cart_res.added : add2Cart_res.removed);
                        if (!ignoreTexts) {
                            $$.html(action == "add" ? add2Cart_res.added : add2Cart_res.removed);
                        }
                        //                        setTimeout(function () {
                        //                            add2Cart_setSatus($$, ignoreTexts);
                        //                        }, 5000);
                        rv_incCounter(1, "span.mcCount");
                    }
                }
            });
        }
    }

    function add2Cart_add2Favorite(item, layout) {
        var $$ = $(item);
        add2Cart_send("action=add2Fav&storage=MyCart&id=" + $$.attr("hid") + "&hn=" + $$.attr("hn") + "&gfav=" + $$.attr("gfav"), function (r) {
            var msg = r.responseText;
            var sp = msg.split(';');
            add2Cart_openOverlayMessage(sp[1], layout);
            if (sp[0] / 1 < 0) {
                return;
            }
            if ($$.attr("nt") != "noText") {
                $$.html(sp[0] == "0" ? add2Cart_res.add2fav : add2Cart_res.removeFav);
            } else {
                $$.attr("title", sp[0] == "0" ? add2Cart_res.add2fav : add2Cart_res.removeFav);
            }
            $$.attr("gfav", sp[0] == "0" ? "0" : "1");
            rv_incCounter(sp[0] == "0" ? -1 : 1, "span.mfCount");
            $$.removeClass(sp[0] == "0" ? "add2Fav_remove" : "add2Fav_add")
            .addClass(sp[0] == "0" ? "add2Fav_add" : "add2Fav_remove");
            $$.parent().removeClass(sp[0] == "0" ? "add2Fav_li_remove" : "add2Fav_li_add")
            .addClass(sp[0] == "0" ? "add2Fav_li_add" : "add2Fav_li_remove");
        });
    }

    function add2Cart_add2Favorite_Link(item) {
        var $$ = $(item);
        add2Cart_send("action=add2Fav&storage=MyCart&id=" + $$.attr("hid") + "&hn=" + $$.attr("hn") + "&gfav=" + $$.attr("gfav"), function (r) {
            var msg = r.responseText;
            var sp = msg.split(';');
            add2Cart_openOverlayMessage(sp[1]);
        });
    }

    function add2Cart_removeFavorite(item) {
        var $$ = $(item);
        add2Cart_send("action=add2Fav&storage=MyCart&id=" + $$.attr("hid") + "&hn=" + $$.attr("hn") + "&gfav=" + $$.attr("gfav"), function (r) {
            var msg = r.responseText;
            var sp = msg.split(';');
            add2Cart_openOverlayMessage2(sp[1], 'location.href = location.href;');
        });
    }


    function add2Cart_removeFromChatted(itemid) {
        var $$ = $('#' + itemid);
        add2Cart_send("action=removeFromChatted&storage=MyCart&id=" + $$.attr("hid") + "&hn=" + $$.attr("hn"), function (r) {
            var msg = r.responseText;
            var sp = msg.split(';');
            add2Cart_openOverlayMessage2(sp[1], 'location.href = location.href;');
        });
    }

    function add2Cart_send(_data, _complete) {
        $.ajax({
            type: "GET",
            url: "/services/newonlinelover/rvcontent.ashx",
            data: _data,
            cache: false,
            complete: function (r) {
                if (typeof (_complete) != "undefined") {
                    _complete(r);
                }
            },
            onerror: function () {
                alert('err');
            }
        });
    }

    function notifyOnlineLink(l_hostName, l_hostId) {
        // first add to favorites
        add2Cart_send("action=add2Fav&storage=MyCart&id=" + l_hostId + "&hn=" + l_hostName + "&gfav=0", function (r) {
            var msg = r.responseText;
            var sp = msg.split(';');
            var _params = { "ActionId": 6, "host": l_hostName, "fid": sp[0], "en": "0", "hid": l_hostId };
            // and then set notification
            notifyOnline_send($.param(_params), function (msg2) {
                var sp2 = msg2.split(';');
                add2Cart_openOverlayMessage(sp2[2]);
            });
        });

    }

    function notifyOnline_send(_data, _complete) {
        $.ajax({
            type: "POST",
            url: '/services/HostProfileHandler.ashx',
            data: _data,
            success: function (msg) {
                if (typeof (_complete) != "undefined") {
                    _complete(msg);
                }
            }
        });
    }

    function add2Cart_setSatus(item, ignoreTexts) {
        var action = item.attr("act").substring(1);
        item
            .attr("act", action)
            .removeClass("add2Cart_wait")
            .addClass("add2Cart_" + action);
        $$.attr("title", action == "add" ? add2Cart_res.add2cart : add2Cart_res.removeFromCart);
        if (!ignoreTexts) {
            item.html(action == "add" ? add2Cart_res.add2cart : add2Cart_res.removeFromCart);
        }
    }

    function add2Cart_openOverlayMessage(msg, layout) {
        doAlert('onlyForMembers',
            msg,
            { css: 'bo msgBox' },
            [{ id: 'okBtn', css: 'buttonRed', style: 'width:60px; padding:3px 0;', value: add2Cart_res.buttons.ok, click: '$.modal.close();' }], null
            /*layout || ['50%', '60%']* changed to null*/);
        //setTimeout('$.modal.close();', 5000);
    }


    function add2Cart_openOverlayMessage2(msg, onClose) {
        doAlert('onlyForMembers',
            msg,
            { css: 'bo msgBox' },
            [{ id: 'okBtn', css: 'buttonRed', style: 'width:60px; padding:3px 0;', value: add2Cart_res.buttons.ok, click: '$.modal.close();' + onClose }],
           null);
        // setTimeout('$.modal.close();' + onClose, 5000);
    }

    function add2Cart_openOverlayMessageDial(msg, onOk) {
        doAlert("onlyForMembers",
            msg,
            { css: "bo msgBox" },
            [{ id: "okBtn", css: "buttonRed", style: "width:100px", value: add2Cart_res.buttons.ok, click: "$.modal.close();" + onOk },
                { id: "okCencel", css: "buttonRed", style: "width:100px", value: add2Cart_res.buttons.cancel, click: "$.modal.close();" }], null
            /*['50%', '45%'] changed to null*/);
        //setTimeout('$.modal.close();', 5000); // ??? TODO - do we need to hide ok/cancel dialog after a timeout? Katy
    }

    function onAddToFavClicked(nickname, portalAlias) {
        window.event.stopPropagation();

                  
        doAlert('onlyForMembers',
            //'Become a member to add ' + nickname + ' to your favorites',
            add2Cart_res.onlyForMembers_favorites.replace("$nickname$", nickname),
            { css: 'bo msgBox wide' },
            [{ id: 'okBtn', css: 'buttonRed', style: 'width:200px; padding:3px 0;', value: add2Cart_res.onlyForMembersBtn, click: 'onSignupBtnClicked();' },
                { id: 'cancelBtn', css: 'buttonRed', style: 'width:100px; padding:3px 0;', value: add2Cart_res.buttons.cancel, click: 'onClickedBtnClicked();' },
                { id: 'alredyMember', css: 'btnMember', style: '', value: add2Cart_res.alreadyMemberLink.already_member.replace("$SITE_NAME$", portalAlias) + " ", click: '' },
                { id: 'alredyMemberBtn', css: 'btnLogin', style: 'color:#aaccff;', value: add2Cart_res.alreadyMemberLink.login_now, click: 'onAlreadymemberBtnClicked();' }],
           /* ['50%', '45%'] changed to null*/ null);
    }

    // next 3 methods moved from RVGalleryPage and HostListPage - Katy
    function onSignupBtnClicked() {
        location.href = '/signup';
    }
    function onClickedBtnClicked() {
        $.modal.close();
    }
    function onAlreadymemberBtnClicked() {
        location.href = '/login';
    }

</script>

