    function updatePageVals(pagerMin, pagerMax, fMin, myParentClass) {
        if ($('.hpHostListContainer:not([data-special-class=""]) .hpArrowsPager').length == 0 || myParentClass == "")
            return;

        var $parent = $('.hpHostListContainer.' + myParentClass + ' .hpArrowsPager');
        $parent.attr("data-pager-min", pagerMin);
        $parent.attr("data-pager-max", pagerMax);
        $parent.attr("data-total-items", fMin);
    }

