	<script>	
	
		$(document).ready(function(){
		$("#btn_local").append('<div class="local_list"><ul><li><a href="/list_local.php?ncid=L57">부산</a></li><li style="color:#ababab;font-weight:normal;"> | </li><li><a href="/list_local.php?ncid=L52">대구경북</a></li><li style="color:#ababab;font-weight:normal;"> | </li><li><a href="/list_local.php?ncid=L55">광주전남</a></li><li style="color:#ababab;font-weight:normal;"> | </li><li><a href="/list_local.php?ncid=L56">울산</a></li><li style="color:#ababab;font-weight:normal;"> | </li><li><a href="/list_local.php?ncid=L50">강원</a></li><li style="color:#ababab;font-weight:normal;"> | </li><li><a href="/list_local.php?ncid=L51">경남</a></li><li style="color:#ababab;font-weight:normal;"> | </li><li><a href="/list_local.php?ncid=L54">전북</a></li></div>');
			$("#btn_local").hover(function(){
				$(".local_list").show();		
				$(this).css("border-bottom","2px solid #fff");
				$(this).css("border-left","1px solid #333");
				$(this).css("border-right","1px solid #333");
				$(this).find("a:eq(0)").css("margin-right","10px");
				$(this).find("a:eq(0)").css("margin-left","10px");				
				$(this).prev().css("margin-right","12px");
				$(this).css("margin-right","12px");				
			},function(){			
				reset();
			});	

			
			$("#btn_visual").append('<div class="visual_list"><ul><li><a href="http://www.focus.kr/photo_main.php">포토</a></li><li style="color:#ababab;font-weight:normal;"> | </li><li><a href="http://www.focus.kr/media_view.php">영상</a></li><li style="color:#ababab;font-weight:normal;"> | </li><li><a href="http://www.focus.kr/visual/GraphicNews_Main.php">그래픽</a></li><li style="color:#ababab;font-weight:normal;"> | </li><li><a href="http://www.focus.kr/media_view.php?fnm=vr">VR뉴스</a></li></ul></div>');			
			$("#btn_visual").hover(function(){
				$(".visual_list").show();		
				$(this).css("border-bottom","2px solid #fff");
				$(this).css("border-left","1px solid #333");
				$(this).css("border-right","1px solid #333");				
				$("#btn_visual").css("padding-right","11px");
				$("#btn_visual").css("padding-left","11px");				
				//$(this).prev().css("margin-right","12px");
				//$(this).css("margin-right","12px");				
			},function(){			
				reset_visual();
			});	
			
		
		});	
		
		
		function reset(){
				$(".local_list").hide();				
				$("#btn_local").css("border-width","0px");
				$(".gnb .wrap ul.la li").css("margin-right","23px");
				$("#btn_local a").css("margin","0px");
		}
		function reset_visual(){
				$(".visual_list").hide();				
				$("#btn_visual").css("border-width","0px");
				$("#btn_visual").css("padding-left","12px");	
				$("#btn_visual").css("padding-right","12px");	
				//$(".gnb .wrap ul.ra li").css("margin-right","23px");
				//$("#btn_visual a").css("margin","0px");
		}
		
	</script>		
