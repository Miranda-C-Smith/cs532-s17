  );

  // ticket #228 - index exchange desires a unique value per slot, so we generate
  // a sequential id and use it as the "tile" targeting param.  98% correct.
  var tile = 0;
  wbads.defineCallback('post.slot.define', function(evt, slot) {
    tile++;

    if (slot.slot_data.tile) {
      tile = slot.slot_data.tile + 1;
      return;
    }

    slot.slot_data.tile = tile;
    slot.setTargetingParam('tile', tile);
  });
});
</script>


