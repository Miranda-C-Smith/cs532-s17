background:#FFF;
border-top:3px #DDD solid;
border-bottom:3px #DDD solid;
}
.colBoxShareboxThumb {
display:table-cell;
width:50%;
height:100%;
background-size:cover;
background-position:center center;
}
.colBoxShareboxBtn {
display:table-cell;
color:#FFF;
vertical-align:middle;
}
.colBoxSharebox_bgw .colBoxShareboxBtn {
color:#000;
font-weight:bold;
}
#cmsBody .colBoxShareboxBtn p {
margin:0 0 15px;
font-size:16px;
}
.colBoxShareboxBtnFb {
margin-bottom:13px;
}
.colBoxShareboxBtnTw {
margin-bottom:5px;
}
-->
</style>
<div id="thirdPartyWidget">
<div class="colBoxClear h20px"></div>
</div>
<script>
(function(){
	var d = document;
	var target = d.getElementById('thirdPartyWidget'); // SNSパーツ設置位置
		var meta, // meta タグ
				extraid, // EXIRA ID
				disuse_ogimage = false, // OG 画像二次利用不可フラグ
				sharebox, // SNSパーツ
				sharebox_thumb = imgSrv() + '/nl/images/nl_fb_coverimage.jpg', // SNSパーツ サムネイル画像（デフォルト：カバー画像）
				sharebox_text = 'この記事が気に入ったら<br>ねとらぼに「いいね！」しよう', // SNSパーツ テキスト
				sharebox_class = '', // SNSパーツclass指定
				fb_like, // FB Like Button
				tw_follow; // TW Follow Button
	// ねとらぼ Facebook & Twitter
	fb_like = '<div class="fb-like" data-href="https://www.facebook.com/ITmediaNlab/" data-layout="button_count" data-action="like" data-size="small" data-show-faces="false" data-share="false"></div>';
	tw_follow = '<a href="https://twitter.com/itm_nlab" class="twitter-follow-button" data-show-count="false" data-lang="ja">@itm_nlabさんをフォロー</a>';
	extraid = getMETA('extraid').content;
	if(extraid) {
		if(extraid.match(/kw-neco/)){
			// ねとらぼ生物部 Facebook & Twitter
			fb_like = '<div class="fb-like" data-href="https://www.facebook.com/ITmediaNlabZoo" data-layout="button_count" data-action="like" data-size="small" data-show-faces="false" data-share="false"></div>';
			tw_follow = '<a href="https://twitter.com/itm_nlab" class="twitter-follow-button" data-show-count="false" data-lang="ja">@itm_nlabさんをフォロー</a>';
			sharebox_thumb = imgSrv() + '/nl/images/nl_fb_coverimage_zoo.jpg';
			sharebox_text = 'この記事が気に入ったら<br>ねとらぼ生物部に「いいね！」しよう'; // SNSパーツ テキスト
		} else if(extraid.match(/kw-entertainment/)) {
			// ねとらぼエンタ Facebook & Twitter
			fb_like = '<div class="fb-like" data-href="https://www.facebook.com/ITmediaNlabEnta" data-layout="button_count" data-action="like" data-size="small" data-show-faces="false" data-share="false"></div>';
			tw_follow = '<a href="https://twitter.com/itm_nlabenta" class="twitter-follow-button" data-show-count="false" data-lang="ja">@itm_nlabentaさんをフォロー</a>';
			sharebox_thumb = imgSrv() + '/nl/images/nl_fb_coverimage_ent.jpg';
			sharebox_text = 'この記事が気に入ったら<br>ねとらぼエンタに「いいね！」しよう'; // SNSパーツ テキスト
		}
		if(extraid.match(/kw-disuse_ogimage/)) {
			// OG 画像二次利用不可
			disuse_ogimage = true;
		}
	}
	if (disuse_ogimage == false) {
		meta = d.getElementsByTagName('meta');
		for(var i = 0; i < meta.length; i++){
			if(meta[i].getAttribute("property") == 'og:image'){
				sharebox_thumb = meta[i].content.replace(/\/l_(.*\.)(gif|GIF|jpg|JPG|png|PNG)/,'/$1$2');
				break;
			}
		}
	}
	sharebox = d.createElement('div');
	sharebox.id = 'colBoxSharebox';
	sharebox.className = 'colBoxSharebox';
	sharebox.innerHTML  = '<div class="colBoxShareboxThumb" style="background-image:url(' + sharebox_thumb + ');"></div>';
	sharebox.innerHTML += '<div class="colBoxShareboxBtn"><p>' + sharebox_text + '</p><div class="colBoxShareboxBtnFb">' + fb_like + '</div><div class="colBoxShareboxBtnTw">' + tw_follow + '</div></div>';
	target.insertBefore(sharebox,target.firstChild);
})();
</script>


<!--EDIT END-->
</div></div>

<script>endlinkAll();</script>

