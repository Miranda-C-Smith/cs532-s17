	function cbs_run_ad_listenlive() { 
		var is_refresh = false;
		if (ads_cluster["ad_listenlive"]) {
			googletag.cmd.push(function() { (ads_cluster["ad_listenlive"]).clearTargeting(); });
			is_refresh = true;
		} else {
			googletag.cmd.push(function() { ads_cluster["ad_listenlive"] = googletag.defineSlot("/4128/CBS.NY/" + cbs_ads_zone + "", [[88,31]], "ad_listenlive").addService(googletag.pubads())});
		}
		googletag.cmd.push(function() { (ads_cluster["ad_listenlive"]).setTargeting("tag", cbs_ads_tags.concat(["listenliveoverlay"])).setTargeting("pos", 37).setTargeting("type", "post").setTargeting("title", "njsixthgraderseesgirlscoutcookiesalessoarwithherbrutallyhonestreviews").setTargeting("refresh", "0").setCollapseEmptyDiv(true); });
		if (is_refresh) googletag.cmd.push(function() { googletag.pubads().refresh([ads_cluster["ad_listenlive"]]); });
		else googletag.cmd.push(function() { googletag.display("ad_listenlive"); });
	}
