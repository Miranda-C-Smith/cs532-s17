<script>

// $(document).ready(function() {
	showPromo();

	new AdaptiveHelper().onBpChange(function() {
		showPromo();
	});

	function showPromo()
	{
		switch (new AdaptiveHelper().getBreakpointName())
		{
			case 'mobile':
				$('.breaking__imageWrapp').html($('#tpl_image_s').html())
			break;

			case 'tablet':
				$('.breaking__imageWrapp').html($('#tpl_image_l').html())
			break;

			case 'desktop':
				$('.breaking__imageWrapp').html($('#tpl_image_m').html())
			break;

			default:
				$('.breaking__imageWrapp').html($('#tpl_image_l').html())
			break;
		}
	}


	// })

</script>




