<script>
	var subscribe_news_form_block = false;

	function sendAnalytics(current) {
		var data = $(current).data().newslettertrack,
			dataValue = $(current).data().newslettervalue,
			category = data.category || '',
			action = data.action || '',
			label = data.label || '',
			value = dataValue || '';

			console.log(value);
			TvrainAnalytics.event(category, action, label, value, true);
	}

	
	if (!localStorage.getItem("subscribe-news-hide"))
	{
		$(".mailing").show();
	}
	$(function() {
		$(".subscribe_news-button-hide").click(function()
		{
			$(".mailing").hide();
			localStorage.setItem("subscribe-news-hide", true);
		});

		$(".subscribe_news-button").click(function(event) {
			var _this = event.currentTarget;

			if (subscribe_news_form_block == false)
			{
				fbq('track', 'Lead');
				$(".subscribe_news-button").css("background-color", "#ccc");
				subscribe_news_form_block = true;
				$(".subscribe_news-error").text("").addClass('form-field__error--hidden');

				
				var s_email = $(".subscribe_news-email").val();
				if (!s_email)
				{
					$(".subscribe_news-error").text("Необходимо заполнить Email").removeClass('form-field__error--hidden');
					subscribe_news_form_block = false;
					return false;
				}
				if (!validateEmail($(".subscribe_news-email").val()))
				{
					$(".subscribe_news-error").text("Неверный Email").removeClass('form-field__error--hidden');
					subscribe_news_form_block = false;
					return false;
				}

				var types = [];
				$(".f-types:checked").each(function() {
					types.push($(this).attr("name"));
				});
				$(_this).attr('data-newsLetterValue', types.join(','));

				console.log(types);

				if (types.length == 0)
				{
					$(".subscribe_news-error").text("Необходимо выбрать хотя бы одну рассылку").removeClass('form-field__error--hidden');
					subscribe_news_form_block = false;
					return false;
				}

				$.post("/user/subscribenews/", {
					email         : s_email,
					types         : types,
					agreement     : true
				}, function(data) {
					if (data)
					{
						$(".mailing__success").removeClass('mailing__success--hidden').text('Пожалуйста, для подтверждения подписки следуйте инструкциям, отправленным на адрес ' + s_email);
						$("#margin_form").hide();
						sendAnalytics(_this);
					}
					else
					{
						$(".subscribe_news-error").text('Произошла непредвиденная ошибка').removeClass('form-field__error--hidden');
					}
				}, 'json')
				.fail(function() {
					$(".subscribe_news-error").text('Произошла непредвиденная ошибка').removeClass('form-field__error--hidden')
				});
			}
		});
	});

	function validateEmail(email) {
		var re = /^([\w-]+(?:\.[\w-]+)*)@((?:[\w-]+\.)*\w[\w-]{0,66})\.([a-z]{2,6}(?:\.[a-z]{2})?)$/i;
		return re.test(email);
	}
</script>
			
