	<div id="tiles-ab">
	<script>

				$.ajax("https://relap.io/api/v2/similar_pages_jsonp.js", {
					jsonp: "callback",
					dataType: "jsonp",
					data: {
						url: window.location.href,
						limit: 100,
						with_description: 0
					},
				}).done(function (response) {
					var ids = [], Map = {};
					response.recs.forEach(function (item) {
						var result = item.url.match( /-([0-9]+)\//i );
						Map[result[1]] = item.rec_id;
						ids.push(result[1]);
					});

					var tracking = 1;
					$(document).scroll(function () {
						if (tracking) {
							var current = $(window).scrollTop();
							if (current + 150 > $('#tiles-ab').offset().top) {
								var track = new Image();
								track.src = "https://relap.io/api/v2/pixel.gif?rgid=" + response.rec_group_id;
								tracking = 0;
							}
						}
					});
					$(document).on('click', '[data-rid]', function () {
						var track = new Image();
						track.src = "https://relap.io/api/v2/pixel.gif?rid=" + $(this).data('rid');
					});

					abload('/recommended/detailtilesrelap/', {id: 427137, ids: ids, rids: Map, controllerView: 'detail', YII_CSRF_TOKEN: 'd49c0474b76ff61fb43346527cb2245785ef8bbe'});
				});


		$(document).on('click', '[data-statab-track-link]',function (e) {
			var data = $(e.currentTarget).data().gaParams;
			TvrainAnalytics.event(data.category + ' relap', 'click', data.label, data.value);
		});

		function abload(recUrl, data) {
			$.ajax(recUrl, {
				type: 'POST',
				data: data,
				dataType: 'HTML',
				success: function (response) {
					$('#tiles-ab').html(response);
					window.bLazy.revalidate();
					if (rekl && $('#dfp_banner_lc2').length) {
						rekl.runDFP('dfp_banner_lc2');
					}
					$(window).on('load', function () {
						new TVRAIN.StickyBehavior({
							start: '#rc3_banner_trigger',
							target: '#rc3_banner',
							area: '.main-col',
							classProgress: 'banner--fixed',
							classEndpoint: 'banner--abs'
						});
						window.SvgHelper().apply();
					});
				}
			});
		}

	</script>
