    <div class="col-md-8 wrapper">
        
        <article itemscope itemtype="http://schema.org/NewsArticle">
            <div class="date">
                <div class="pull-right">
                    <p itemprop="datePublished" content="2017-02-07T07:00:00+0100">
                        <i class="fa fa-calendar"></i> 07.02.17
                        <i class="fa fa-clock-o"></i> 07:02:00
                    </p>
