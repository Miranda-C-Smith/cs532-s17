<script type="text/javascript">
	// CAFESUS-7781 by laziel
	var goFitter;
	var gnFitted = 0;

	function fitFrame(){
		if(gnFitted < 10){ // 0.5ÃÊ¾¿ 10È¸
			parentResizeIframe('cafe_main');
			goFitter = window.setTimeout('fitFrame()', 500);
			gnFitted++;
		} else {
			stopFitFrame();
		}
	}

	function stopFitFrame(){
		if(goFitter){
			Event.unregister(window, 'load', fitFrame);
			window.clearTimeout(goFitter);
			goFitter = gnFitted = null;
		}
	}

	Event.register(window, 'load', fitFrame);
	Event.register(window, 'unload', stopFitFrame);

	var bTablet = false;
	var oTabletHandler = new TabletHandler(bTablet);


var cafenameTitle = "¼Ò·® ±ÂÁî¸¸µå´Â ¸Ó±¸";
LH.add(changeTitle(cafenameTitle + " : " +  "³×ÀÌ¹ö Ä«Æä"));
LH.add(initTopLayerQuery());



</script>



<script type="text/javascript">
function showDenyNamelessLayer() {
	
	var layerScrollTop = document.getElementsByTagName('HTML')[0].scrollTop;
    var layerClientHeight = document.getElementsByTagName('HTML')[0].clientHeight;
    var layerPosition = layerScrollTop + (layerClientHeight * 0.5);
    document.getElementById("denyNamelessLayer").style.top = layerPosition + "px";
	document.getElementById("denyNamelessLayer").style.display = "";
}

function hideDenyNamelessLayer() {
	document.getElementById("denyNamelessLayer").style.display = "none";
}

function redirectToCertPage() {
	toCertPage(encodeURIComponent(document.location.href), encodeURIComponent('http://cafe.naver.com/bebemug'));
}
</script>
