$(document).ready(function(){
  var ran_yet = false;
  $("a,body,input").live('click', function() {
    // Don't show camgasm popup for affiliate referred users.
    if ($.cookie('fromaffiliate')) {
      return true;
    }
    if (ran_yet) {
      return true;
    }
    ran_yet = true;
    var cookiename = 'cg1';
    var val = $.cookie(cookiename);
    if (!val) {
      val = 0;
    } else {
      val = parseInt(val);
    }
    var d = new Date;
    // millisecs * seconds * minutes * hours
    d.setTime(d.getTime() + 1000 * 60 * 60 * 8);
    $.cookie(cookiename, val + 1, {expires: d, path: '/'});
    if (val != 9) {
      return true;
    }
    var windowprops = "width=1024,height=768,location=yes,toolbar=yes,menubar=yes,status=yes,scrollbars=yes,resizable=yes,top=1,left=1";

    var cg_url = 'http%3A%2F%2Fcamgasm.com%2Faffiliates%2Fin%2F%3Ftrack%3Dbestconverting%26tour%3Dhr8m%26campaign%3Dmna6V';
    if (Math.random() >= 0.5) {
        cg_url = 'http%3A%2F%2Fcamgasm.com%2Faffiliates%2Fin%2F%3Ftrack%3Dhomepage%26tour%3Dgrq0%26campaign%3Dmna6V';
    }
    var url = "/sitestats/openwindow/L4SE0H7dgo9hzdBU/?url=" + cg_url;

    var myWin = window.open(url, "", windowprops);
    myWin.blur();
    window.focus();
    if (typeof mySurveyWin !== 'undefined') {
        mySurveyWin.focus();
    }
    return true;
  });
});


