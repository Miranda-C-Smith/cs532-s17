$(document).ready(function() {
  var displayedOverlay = false;
  $("a:not(a.nooverlay,[href='#'])").live('click', function(){
    if (displayedOverlay) {
      return true;
    }
    //noinspection ReuseOfLocalVariableJS
    displayedOverlay = true;
    $(document).scrollTop(1);
    if (!$("#register_lightbox").length > 0) {
      return true;
    }
    $('#register_lightbox a.next').attr('href', $(this).attr('href'));
    $('#register_lightbox .iframe_join').contents().find('a.next').attr('href', $(this).attr('href'));
    $('#register_lightbox .iframe_join').contents().find('#id_username').focus();
    $('#overlay').show();
    $('#register_lightbox').show();
    $('#overlay').click(function() {
      $('#overlay').hide();
      $('#register_lightbox').hide();
    });
    return false;
  });
});


