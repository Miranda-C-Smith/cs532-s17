<script type="text/javascript">

$.show_login_box = function(){
    $("#login-overlay").show();
    $("#login-box").show();
    $("#id_username").focus();
    $("#id_username").select();
    if ($.mydefchatconn) {
        $.mydefchatconn('disable_html5_video_controls');
    }
}

$.hide_login_box = function(){
    $("#login-overlay").hide();
    $("#login-box").hide();
    if ($.mydefchatconn) {
        $.mydefchatconn('enable_html5_video_controls');
    }
}

$(document).ready(function(){
    $.hide_login_box();
    $("a.login-link").click( function(){
	$.show_login_box();
	return false;
    });
    $("#login-overlay").click(function(){
	$.hide_login_box();
	return false;
    });

    $("#login-box .close").click(function(){
	$.hide_login_box();
    });
    
});
</script>
<script type="text/javascript">
var mobi = ['opera', 'iemobile', 'webos', 'android', 'blackberry', 'ipad', 'safari'];
var midp = ['blackberry', 'symbian'];
var ua = navigator.userAgent.toLowerCase();
if ((ua.indexOf('midp') != -1) || (ua.indexOf('mobi') != -1) || ((ua.indexOf('ppc') != -1) && (ua.indexOf('mac') == -1)) || (ua.indexOf('webos') != -1)) {
  document.write('<link rel="stylesheet" href="https://ssl-ccstatic.highwebmedia.com/css/allmobile.css" type="text/css" media="all"/>');
  if (ua.indexOf('midp') != -1) {
    for (var i = 0; i < midp.length; i++) {
      if (ua.indexOf(midp[i]) != -1) {
	document.write('<link rel="stylesheet" href="https://ssl-ccstatic.highwebmedia.com/css/' + midp[i] + '.css" type="text/css"/>');
      }
    }
  }
  else {
    if ((ua.indexOf('mobi') != -1) || (ua.indexOf('ppc') != -1) || (ua.indexOf('webos') != -1)) {
      for (var i = 0; i < mobi.length; i++) {
	if (ua.indexOf(mobi[i]) != -1) {
	  if ((mobi[i].indexOf('blackberry') != -1) && (ua.indexOf('6.0') != -1)) {
	    document.write('<link rel="stylesheet" href="https://ssl-ccstatic.highwebmedia.com/css/' + mobi[i] + '6.0.css" type="text/css"/>');
	  }
	  else {
	    document.write('<link rel="stylesheet" href="https://ssl-ccstatic.highwebmedia.com/css/' + mobi[i] + '.css" type="text/css"/>');
	  }
	  break;
	}
      }
    }
  }
}
if ((navigator.userAgent.indexOf('iPhone') != -1) || (navigator.userAgent.indexOf('iPad') != -1)) {
  document.write('<meta name="viewport" content="width=device-width" />');
}

</script>
