    $('.signupModal__open--freeTrial, a[href="#signupModal--freeTrial"]').fancybox({
        padding: 0,
        helpers: {
            overlay: {
                locked: true
            }
        },
        maxWidth: 800,
        maxHeight: '90%'
    });

    $('.signupModal__authorizeApp').click(function(){

        $.post('/wp-content/themes/evolutionwp/ajax/authorizeApp.php', {twitter_handle: $('#twitter_handle').val()}).done(function(){
            jQuery('#sendToSecure').submit();
        });

    });

});


function signupModalSubmit_freeTrial(){
    jQuery('#signupModal__form--freeTrial .signupModal__message').removeClass('signupModal__message--error');
    jQuery('#signupModal__form--freeTrial .signupModal__label--error').removeClass('signupModal__label--error');
    jQuery('#signupModal__form--freeTrial .signupModal__message').html('');
    var error = '';

    jQuery('#signupModal__form--freeTrial .signupModal__step1 .signupModal__requiredField').each(function(){
        if (jQuery(this).val() == ''){
            tmp_label = jQuery(this).closest('.signupModal__formInput').find('label');
            tmp_label.addClass('signupModal__label--error');
            error += 'Missing '+tmp_label.html()+'<br />';
        }
    });

    if (error != ''){
        jQuery('#signupModal__form--freeTrial .signupModal__message').addClass('signupModal__message--error').html(error);
    }
    else {

        jQuery('#signupModal__form--freeTrial .signupModal__message').addClass('signupModal__message--sending').html('Processing Details, please wait..');

        jQuery.post('/wp-content/themes/evolutionwp/ajax/captureDetails--freeTrial.php', jQuery('#signupModal__form--freeTrial').serialize()).done(function(data){
            console.log(data);
            results = data.split('|');
            if (results[0] == '1'){
                jQuery('.signupModal__message').hide();
                jQuery('#sendToSecureEmail').val(jQuery('#email_address').val());
                jQuery('#sendToSecureHandle').val(jQuery('#twitter_handle').val());
                jQuery('.signupModal__steps--active').removeClass('signupModal__steps--active');
                jQuery('.authStep').addClass('signupModal__steps--active');
                jQuery('.signupModal__step1').hide();
                jQuery('.signupModal__step3').fadeIn();
            }
            else {
                jQuery('#signupModal__form--freeTrial .signupModal__message').addClass('signupModal__message--error').html(results[0]);
            }
        });

    }
}</script> <link rel="stylesheet" href="https://www.socialquant.net/wp-content/themes/evolutionwp/css/custom-style.css" /></body></html>