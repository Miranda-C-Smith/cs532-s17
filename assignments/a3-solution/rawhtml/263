    function validateEmail(email) {
        var re = /^(([^<>()[\]\\.,;:\s@\"]+(\.[^<>()[\]\\.,;:\s@\"]+)*)|(\".+\"))@((\[[0-9]{1,3}\.[0-9]{1,3}\.[0-9]{1,3}\.[0-9]{1,3}\])|(([a-zA-Z\-0-9]+\.)+[a-zA-Z]{2,}))$/;
        return re.test(email);
    }

    function validate() {
        $("#result").text("");
        var email = $("#email_newsletter").val(); 
      
        if (validateEmail(email)) {
            $.ajax({
                type: "POST",
                url: "newsletter/save",
                data: {email: email},
                dataType: 'json',
                beforeSend: function() {
                    $('#result').html("<img src="+ajax_loader+" /><br><br>");
                },
                success: function(response){
                    $("#result").text(response);
                    $("#result").css("color", "#3A539B");
                },
                error: function(response) {
                    $("#result").text(response);
                    $("#result").css("color", "#EC644B");
                }
            });
        } else {
            $("#result").text("Format email tidak valid");
            $("#result").css("color", "#EC644B");
        }
        return false;
    }

    $("#form-email").bind("submit", validate);

    //define variabel
    var base_url = "http://www.kompasiana.com/";
    var ajax_loader = "http://assets-a1.kompasiana.com/statics/kompasiana2015/images/loader.gif";
    var ajax_test= "http://assets-a1.kompasiana.com/statics/kompasiana2015/images/loading-small.gif";
    var isKompasianer = "";
    var notifAjax = false;

</script>

<script type="text/javascript">
    //define variabel
    var base_url = "http://www.kompasiana.com/";
    var ajax_loader = "http://assets-a1.kompasiana.com/statics/kompasiana2015/images/loader.gif";
    var ajax_test= "http://assets-a1.kompasiana.com/statics/kompasiana2015/images/loading-small.gif";
    var isKompasianer = "";

    $(function(){
            $('.skin-left .close, .skin-right .close').click(function () {
                $('.kcm-skin-ads').hide();
                return false;
            });

            if ($('#div-gpt-ad-162687188723508243-oop_ad_container').length) {
                $('.pol-ads .pol-banner').show(0).delay(20000).hide(0);
            } else $('.pol-ads .pol-banner').hide();

            $(".kompasianer button").click(function() {
                var e = $.Event("keypress");
                e.which = 13; // # Some key code value
                $('.search-kompasianer').trigger(e);
            });

            $(".search-box-in").keypress(function(e) {
                if (e.which == 13) {
                    window.location.href = "http://www.kompasiana.com/search?q="+$('#search-col').val();
                }
            });

            $('.search-kompasianer').keypress(function(e) {
                if(e.which == 13) {
                    window.location.href = "http://www.kompasiana.com/search/kompasianer?q="+$(this).val();
                }
            });
    });

    
    $( document ).ready(function() {
        // s : First Notification
        firstNotification(function(count) {
            if(count != 0)
                $('title').prepend('(' + count + ') ');
        });
        // e : First Notification
    });

    // s -- notification
    setInterval(function(){
        if( notifAjax == false ) {
            notifAjax = true;
            $.ajax({
                url : "/ajax/origin/notification",
                type: "GET",
                dataType : "json",
                success : function(data){  
                    var total_notification = 0;
                    if("messages" in data) {
                        // conversation notification
                        if(data.messages.total != 0) {
                            $("#notification").html(data.messages.total);
                        } else {
                            $("#notification").html('');
                        }
                        
                        total_notification = total_notification + parseInt(data.messages.total);
                    }
                    
                    // latest notification
                    if(data.latest.latest.notification != 0) {
                        $("#notification-latest").html(data.latest.latest.notification);
                    } else {
                        $("#notification-latest").html('');
                    }
                    
                    total_notification = total_notification + parseInt(data.latest.latest.notification);
                    
                    // count total notification
                    if (total_notification != 0) {
                        $('title').html('(' + total_notification + ') Pembenahan Rusun ala Djarot - KOMPASIANA.com');
                    } else {
                        $('title').html('Pembenahan Rusun ala Djarot - KOMPASIANA.com');
                    }
                    
                    notifAjax = false;
                },
            })
        }
    }, 30000 );
    // e -- notification

</script>

