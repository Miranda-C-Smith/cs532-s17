    var ec = new evercookie({
      history: false,
      java: false,
      tests: 10,
      silverlight: false,
      baseurl: '',
      asseturi: '/assets',
      phpuri: '/ajax/origin/ec',
      authPath: false,
      pngCookieName: 'ec_png',
      pngPath: '/png',
      etagCookieName: 'ec_etag',
      etagPath: '/etag',
      cacheCookieName: 'ec_cache',
      cachePath: '/cache'
    });
    // ec.get("SID", function(best, all) {
    //   if (typeof best === 'undefined') {
    //     ec.set("SID", "58abaf5b11664f8570335118");
