<script type="text/javascript">
startTime();
function startTime() {
	var today = new Date();
	var h = today.getHours();
	var m = today.getMinutes();
	var s = today.getSeconds();
	h = checkTime(h);
	m = checkTime(m);
	s = checkTime(s);
	document.getElementById('lbl_indotime').innerHTML = h+':'+m+':'+s;
	if(s=='00') {
		setcitytime_shanghai();
		setcitytime_paris();
		setcitytime_singapore();
		setcitytime_hongkong();
		setcitytime_newyork();
		setcitytime_tokyo();
	};
	var t = setTimeout(startTime, 500);
};
function checkTime(i) {
	if (i < 10) {i = "0" + i};
	return i;
};

function setcitytime_shanghai() {
	var today = new Date();
	today.setHours(today.getHours()+1);
	var h = today.getHours();
	var m = today.getMinutes();
	h = checkTime(h);
	m = checkTime(m);
	document.getElementById('lbl_shanghai').innerHTML = h+':'+m;
};
function setcitytime_paris() {
	var today = new Date();
	today.setHours(today.getHours()-6);
	var h = today.getHours();
	var m = today.getMinutes();
	h = checkTime(h);
	m = checkTime(m);
	document.getElementById('lbl_paris').innerHTML = h+':'+m;
};
function setcitytime_singapore() {
	var today = new Date();
	today.setHours(today.getHours()+1);
	var h = today.getHours();
	var m = today.getMinutes();
	h = checkTime(h);
	m = checkTime(m);
	document.getElementById('lbl_singapore').innerHTML = h+':'+m;
};
function setcitytime_hongkong() {
	var today = new Date();
	today.setHours(today.getHours());
	var h = today.getHours();
	var m = today.getMinutes();
	h = checkTime(h);
	m = checkTime(m);
	document.getElementById('lbl_hongkong').innerHTML = h+':'+m;
};
function setcitytime_newyork() {
	var today = new Date();
	today.setHours(today.getHours()-11);
	var h = today.getHours();
	var m = today.getMinutes();
	h = checkTime(h);
	m = checkTime(m);
	document.getElementById('lbl_newyork').innerHTML = h+':'+m;
};
function setcitytime_tokyo() {
	var today = new Date();
	today.setHours(today.getHours()+2);
	var h = today.getHours();
	var m = today.getMinutes();
	h = checkTime(h);
	m = checkTime(m);
	document.getElementById('lbl_tokyo').innerHTML = h+':'+m;
};
