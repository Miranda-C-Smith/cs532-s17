<script type="text/javascript">
(function() {

  function yad_notifyEvent(div_id, type) {
    try {
      var w = window;
      var data = {'pid' : div_id, 'type' : type};
      if (typeof w.CustomEvent == 'function') { /* modern browsers */
        event = new w.document.defaultView.CustomEvent('banner', { 'bannerData': data });
        event.bannerData = data;
      } else if (typeof w.Event == 'function') {
        event = new w.Event('banner');
        event.bannerData = data;
      } else if (typeof w.document.createEvent == 'function') { /* IE 9+ */
        event = w.document.createEvent('Event');
        event.initEvent('banner', true, true);
        event.bannerData = data;
      }
      if (event) w.document.dispatchEvent(event);
    } catch(e) {}
  }

  function yad_hide(div) {
    if (div) { try {
      $(div).closest('.banners__wrap-article').hide();
    } catch(e) {} }
  }

  var div = document.getElementById("yandex_rtb_R-A-123194-30");
  if (div) {
    var rnd = Math.round(Math.random()*1000000000);
    div.id += "-" + rnd;
    (function(r, w, d, n, s, t) {
        w[n] = w[n] || [];
        w[n].push(function() {
            Ya.Context.AdvManager.render(
                { blockId: "R-A-123194-30", renderTo: "yandex_rtb_R-A-123194-30-" + r, async: true, pageNumber: r }
            );
        });
        t = d.getElementsByTagName("script")[0];
        s = d.createElement("script");
        s.type = "text/javascript";
        s.src = "//an.yandex.ru/system/context.js";
        s.async = true;
        t.parentNode.insertBefore(s, t);
    })(rnd, this, this.document, "yandexContextAsyncCallbacks");

   (function() {
      var div_counter = 0;
      setTimeout(function() {
        if (div.innerHTML.length) {
          yad_notifyEvent(div.id, 'show');
          return;
        } else if (++div_counter < 10){
          setTimeout(arguments.callee, 500);
          return;
        } else {
          yad_hide(div);
          yad_notifyEvent(div.id, 'empty');
          return;
        }
      }, 500);
    })();
  }
})();

