<script>

function video_interno(){
		$(".multimedia_cont").removeClass('disabled');
	  if($("#video_lsr_ultimo").attr('video')==undefined) return;
 	  var video=$("#video_lsr_ultimo").attr('video');
	  var imagen=$("#video_lsr_ultimo").attr('imagen');

      jwplayer('video_lsr_ultimo').setup({ 
      file: video, 
      image:imagen, 
      width: 370, 
      height: 224 
      });

}
jQuery(document).ready(function($) {
	video_interno();
});

$(".thmb").click(function(event) {
	$(".multimedia_cont").addClass('disabled');
	if($(this).attr('tipo') == "foto"){
		$(".multimedia_cont").load('http://lasillarota.com/modulo/multimedia/fotogaleria_loultimo/'+$(this).attr('id'),'',function(){
			 $(".multimedia_cont").removeClass('disabled');
			 $(".fancygaleria").fancybox({
			  titleShow:"false"
			  });
			
		});
		$.ajax({ url: base_url+'modulo/multimedia/sumar_click_fg/'+ $(this).attr('id') });
	}else if($(this).attr('tipo') == "video"){
		$(".multimedia_cont").load('http://lasillarota.com/modulo/multimedia/video_loultimo/'+$(this).attr('id'),video_interno);
	}
//$("#contenedor").load('');
});
</script><div class="row">
