<script>
var soldados_cuarteles = ''; $(document).ready(function() {
soldados_cuarteles  = $('.soldados-cuarteles').bxSlider({
auto:true,pause: 6000,slideWidth: 300,
minSlides: 1, maxSlides: 1, slideMargin: 0, moveSlides:1,
pager: false, video: true, useCSS: false, controls:false,
onSliderLoad: function(indice,slider){
				$('.bx-clone object').prepend('<param name="wmode" value="transparent">');
				impresor(slider,'soldados-cuarteles');
			},
onSlideAfter: function(){
				impresor(soldados_cuarteles,'soldados-cuarteles');
				indice = soldados_cuarteles.getCurrentSlide();
				indice = (indice*1) + 1;
				pause = $('.soldados-cuarteles .slide').eq(indice).attr('pause');
				soldados_cuarteles.setPause(pause);
			}
});
});
</script>
