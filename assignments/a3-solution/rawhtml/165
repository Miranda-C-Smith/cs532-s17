<script>
;(function(){
  $(document).ready(function(){
    var header = $('.ds-metro-header');
    $([
      '<ul id="ds-lens-main-nav" style="position:absolute;left:-200px">',
        '<li class="ds-metro-link"><a href="http://do512.com" class="ds-ga-event" data-ds-ga-event-name="LENS_SWAP" data-ds-ga-action="CLICK" data-ds-ga-label="HOME">Do512<span></span></a></li>',
 '<li class="ds-articles-link"><a href="http://do512.com/p/what-to-do-in-austin" class="ds-ga-event" data-ds-ga-event-name="LENS_SWAP" data-ds-ga-action="CLICK" data-ds-ga-label="what_to_do">What to Do<span></span></a></li>',
 '<li class="ds-freeweek-link"><a href="http://freeweek.do512.com" class="ds-ga-event" data-ds-ga-event-name="LENS_SWAP" data-ds-ga-action="CLICK" data-ds-ga-label="free_week">Free Week<span></span></a></li>',
      '</ul>'
    ].join('')).insertAfter(header);
    var hideTimer, lensNav = $('#ds-lens-main-nav');
    lensNav.css({left:'-'+(lensNav.width()-35)+'px'});
    lensNav.on('mouseover', function(){
      var el = $(this);
      if (hideTimer) clearTimeout(hideTimer);
      el.addClass('on');
    })
    lensNav.on('mouseout', function(){
      var el = $(this);
      if (hideTimer) clearTimeout(hideTimer);
      hideTimer = setTimeout(function(){
        el.removeClass('on');
      }, 250);
    })
  });
})();
</script>

