    <!--DFP-->
<script>
  var googletag = googletag || {}; googletag.cmd = googletag.cmd || [];
  /**
   * Closure pour permettre de relancer l'affichage des pubs jusqu'à l'acceptation des cookies.
   *
   * refusedOnce à FALSE pour permettre la recursivité.
   */
  ;(function(refusedOnce) {
    ;(function () {
      // Fonction stockée pour recursivité.
      var self = arguments.callee;

      // Si le cookie consent est posé.
      if (window.cookieconsent) {
        cookieconsent("consent", "advertising", function (accept) {

          // Si le client accepte, on lance les scripts.
          if (accept) {

            // Initialisation googletag
            var dfp_vars = Drupal.settings.dfp_vars;
            if ((dfp_vars instanceof Object) && (dfp_vars['usedfp'] === true)) {
              (function () {
                if (getDeviceDfp(dfp_vars) === "desktop") {
                  initProxistore();
                }
                var gads = document.createElement('script');
                gads.async = true;
                gads.type = 'text/javascript';
                var useSSL = 'https:' == document.location.protocol;
                gads.src = (useSSL ? 'https:' : 'http:') + '//www.googletagservices.com/tag/js/gpt.js';
                var node = document.getElementsByTagName('script')[0];
                node.parentNode.insertBefore(gads, node);
                isDfpInTest(dfp_vars);
              })();
              setGoogleTag(googletag, advSizes, getDeviceDfp(dfp_vars), dfp_vars, false);
            }
          }

          // S'il a refusé
          else {
            if(!refusedOnce){
              refusedOnce = true;
            }

            setTimeout(self, 1000);
          }
        });

      // Le cookie consent n'a pas été encore validé.
      } else {
        // On boucle chaque seconde.
        setTimeout(self, 1000);
      };
    })();
  })(false);
</script>
