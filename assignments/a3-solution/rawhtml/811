BrightcovePluginFunctions.getHiddenPropertyName = function(prefix){
    return (prefix ? prefix + 'Hidden' : 'hidden');
}

BrightcovePluginFunctions.getVisibilityEvent = function(prefix){
    return (prefix ? prefix : '') + 'visibilitychange';
}

BrightcovePluginFunctions.getBrowserPrefix = function(){
    for (var i = 0; i < BrightcovePluginFunctions.browserPrefixes.length; i++) {
        if(BrightcovePluginFunctions.getHiddenPropertyName(BrightcovePluginFunctions.browserPrefixes[i]) in document) {
            // return vendor prefix
            return BrightcovePluginFunctions.browserPrefixes[i];
        }
    }
    
    // no vendor prefix needed
    return null;
}

BrightcovePluginFunctions.browserPrefix = BrightcovePluginFunctions.getBrowserPrefix();
BrightcovePluginFunctions.hiddenPropertyName = BrightcovePluginFunctions.getHiddenPropertyName(BrightcovePluginFunctions.browserPrefix);
BrightcovePluginFunctions.visibilityEventName = BrightcovePluginFunctions.getVisibilityEvent(BrightcovePluginFunctions.browserPrefix);

BrightcovePluginFunctions.onVisible = function() {
    // prevent double execution
    if(BrightcovePluginFunctions.isVisible) {
        return;
    }
    if (BrightcovePluginFunctions.hasPlayed) {
        $('.htmlappend').each(function(){
            possibleiframe = $(this).find('iframe');
            if (possibleiframe.attr('src').search('brightcove')){
                win = possibleiframe.get(0).contentWindow;
                win.postMessage("restartAfterHidden","http://players.brightcove.net");
            }
        });
    } 
    // change flag value
    BrightcovePluginFunctions.isVisible = true;
}


BrightcovePluginFunctions.onHidden = function() {
    // prevent double execution
    if(!BrightcovePluginFunctions.isVisible) {
        return;
    }
    
    $('.htmlappend').each(function(){
        possibleiframe = $(this).find('iframe');
        if (possibleiframe.attr('src').search('brightcove')){
            win = possibleiframe.get(0).contentWindow;
            win.postMessage("pauseWhenHidden","http://players.brightcove.net");
        }
    });
    
    // change flag value
    BrightcovePluginFunctions.isVisible = false;
}


BrightcovePluginFunctions.handleVisibilityChange = function(forcedFlag) {
  // forcedFlag is a boolean when this event handler is triggered by a
  // focus or blur eventotherwise it's an Event object
  if(typeof forcedFlag == "boolean") {
    if(forcedFlag) {
      return BrightcovePluginFunctions.onVisible();
    }
    return BrightcovePluginFunctions.onHidden();
  }

  if(document[BrightcovePluginFunctions.hiddenPropertyName]) {
    return BrightcovePluginFunctions.onHidden();
  }

  return BrightcovePluginFunctions.onVisible();
}

$(window).on('load', function(){
    //BrightcovePluginFunctions.checkForBrightcoveVideo();
    //alert($('#vjs_video_3 .vjs-control-bar button.vjs-fullscreen-control').html());
    //$('<button class="vjs-facebook vjs-control vjs-button " type="button" aria-live="polite"><span class="vjs-control-text">Facebook</span></button><button class="vjs-twitter vjs-control vjs-button " type="button" aria-live="polite"><span class="vjs-control-text">Twitter</span></button><button class="vjs-share2 vjs-control vjs-button " type="button" aria-live="polite"><span class="vjs-control-text">Share</span></button>').insertAfter('#vjs_video_3 .vjs-control-bar button.vjs-fullscreen-control');
});


