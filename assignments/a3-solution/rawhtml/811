<script type="text/javascript">

$.extend(ExpressApp, {
    RelatedVideoModal: {
        modalWindow: null,
        modal: null,
        video: null,
        videocontent: null,
        stop: false,

        Start: function(url) {
            ExpressApp.RelatedVideoModal.video = url;
            ExpressApp.RelatedVideoModal.InitModal();
            ExpressApp.RelatedVideoModal.Listener();
        },
        InitModal: function() {
            ExpressApp.RelatedVideoModal.modalWindow = $('.reveal-dynamic').length == 2;
            ExpressApp.RelatedVideoModal.modal = ExpressApp.RelatedVideoModal.modalWindow
                ? $('.reveal-dynamic:hidden').last()
                : $('<div>').addClass('reveal-modal reveal-dynamic GallerySlideshowModal videoModal').appendTo('body');

            var modalBody = ExpressApp.RelatedVideoModal.GetModalContent({
                    photoSrc: null,
                }, false);
                console.log('modalBody:' + modalBody);
            if (ExpressApp.RelatedVideoModal.modalWindow && ExpressApp.RelatedVideoModal.modal.is(':visible')) {
                ExpressApp.RelatedVideoModal.modal.bind('reveal:closed.reveal', function(){
                    ExpressApp.RelatedVideoModal.modal.empty().html(modalBody).reveal();
                });
            } else {
                //alert('no here');
            }

            ExpressApp.RelatedVideoModal.Listener();
            ExpressApp.RelatedVideoModal.modal.empty().html(modalBody).reveal();
        },
        ReplaceModalBody: function(modalBody) {
            console.log('modalBody:' + modalBody);
            ExpressApp.RelatedVideoModal.modal.empty().html(modalBody);
        },
        GetModalContent: function(content, isButtonPaused) {
                console.log('modalBody:' + ExpressApp.RelatedVideoModal.video);
            $.ajax({
                url : ExpressApp.RelatedVideoModal.video,
                dataType : 'html',
                success: function(data){
                    ExpressApp.RelatedVideoModal.ReplaceModalBody(tmpl("tmpl-relatedvideo-slideshow", {
                        videoContent: data,
                    }));
                }
            });
        },
        Listener: function() {
            // Click on the button "Play slideshow" in the gallery


            // Click on the "Close" button on the modal slideshow
            $(document).on('click', '.GallerySlideshow [data-button="close"]', function() {
                ExpressApp.RelatedVideoModal.modal.trigger('reveal:close');
                setTimeout(function() {
                    $('.videoModal').remove();
                }, 500);
            });
        }
    }
});

$(function () {
    $(document).on('click', '.three-related-videos .Videos a', function (e) {
        e.preventDefault();
        var url = $(this).attr('href');
        ExpressApp.RelatedVideoModal.Start(url);
    });
});

BrightcovePluginFunctions = {};
BrightcovePluginFunctions.hasPlayed = false;
BrightcovePluginFunctions.win = null;
BrightcovePluginFunctions.possibleiframe = null;
BrightcovePluginFunctions.isVisible = true;
BrightcovePluginFunctions.hasBrightcove = false;
BrightcovePluginFunctions.browserPrefixes = ['moz', 'ms', 'o', 'webkit'];
