<script type="text/javascript">
<!--
$(function() {
    var changeThumb = function(arrow, change) {
        // 押された矢印の横のMM/NNの文字列を取得
        var current = arrow.siblings(".sendPageNum").children(".molecular");
        var max = arrow.siblings(".sendPageNum").children(".denominator");
        var currentNum = parseInt(current.text());
        var maxNum = parseInt(max.text());
        var afterNum = currentNum + change;

        // 変更後の値の整合性をチェック
        if (0 >= afterNum || maxNum < afterNum) {
            return false;
        }

        // 画像の切り替え
        var currentImg = arrow.parent().siblings(".PhotosContainerMain:visible");
        if (change === 1 && currentImg.next(".PhotosContainerMain").length !== 0) {
            currentImg.hide();
            currentImg.next(".PhotosContainerMain").show();
        } else if (change === -1 && currentImg.prev(".PhotosContainerMain").length !== 0) {
            currentImg.hide();
            currentImg.prev(".PhotosContainerMain").show();
        } else {
            return false;
        }

        // カウンターの文字を変更する
        current.text(afterNum);

        // 矢印の処理
        var prevArrow = arrow.parent().children("a.prev");
        var nextArrow = arrow.parent().children("a.next");
        if (afterNum === 1) {
            prevArrow.removeClass("validPrev");
            prevArrow.addClass("disabledPrev");
        } else {
            prevArrow.removeClass("disabledPrev");
            prevArrow.addClass("validPrev");
        }
        if (afterNum === maxNum) {
            nextArrow.removeClass("validNext");
            nextArrow.addClass("disabledNext");
        } else {
            nextArrow.removeClass("disabledNext");
            nextArrow.addClass("validNext");
        }

        return true;
    };

    $(".sendPage > .prev").each(function() {
        var arrow = $(this).click(function(e){
            if (changeThumb(arrow, -1) !== true) {
                return false;
            }
            YJNEWS.RD('/media/news/articles/thumb/previous/');
            return false;
        });
    });

    $(".sendPage > .next").each(function() {
        var arrow = $(this).click(function(e){
            if (changeThumb(arrow, 1) !== true) {
                return false;
            }
            YJNEWS.RD('/media/news/articles/thumb/next/');
            return false;
        });
    });

    $(".sendPage").each(function() {
        $(this).toggle();
    });
});
//-->
</script>
