     <div class="wp-conten-center-detail">
     <!-- detail -->
    <div class="wp-conten-detail">
    <div class="breadcome">
        
        <a href="http://www.republika.co.id/">Home</a> > 
        <!--
                <a href=""></a> &gt; 
