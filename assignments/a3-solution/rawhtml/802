<script type="text/javascript">
    if (!Array.prototype.filter)
    {
        Array.prototype.filter = function(fun /*, thisp */)
        {
            "use strict";

            if (this === void 0 || this === null)
                throw new TypeError();

            var t = Object(this);
            var len = t.length >>> 0;
            if (typeof fun !== "function")
                throw new TypeError();

            var res = [];
            var thisp = arguments[1];
            for (var i = 0; i < len; i++)
            {
                if (i in t)
                {
                    var val = t[i]; // in case fun mutates this
                    if (fun.call(thisp, val, i, t))
                        res.push(val);
                }
            }

            return res;
        };
    }
</script>
<script type="text/javascript">
    (function($) {
        $(function() {
            $("#carrusel-oferplan").jcarousel({
                wrap: 'circular',
            });
            $('#carrusel-oferplan').on('jcarousel:visiblein', 'li', function(event, carousel) {$(carousel._target).find(".imagenofertas").attr('src',$(carousel._target).find(".imagenofertas").attr('alt'));});
            $("#carrusel-oferplan").jcarouselAutoscroll({
                interval: 5000,
                target: '+=1',
                autostart: true,
            });

            $('.jcarousel-prev-horizontal').click(function() {
                $('#carrusel-oferplan').jcarousel('scroll', '-=1');
            });

            $('.jcarousel-next-horizontal').click(function() {
                $('#carrusel-oferplan').jcarousel('scroll', '+=1');
            });
            var pathname = window.location.pathname;
            var utm_content = 'portada';
            if(pathname != '/'){
                var splited_paths = pathname.split('/');
                if(splited_paths[splited_paths.length-1] == ''){
                    // subdomain. remove empty spaces
                    splited_paths = splited_paths.filter(function(v){return v!==''});
                    utm_content = splited_paths.join('-');
                }else{
                    // subdomain with file ending
                    splited_paths.pop();
                    splited_paths = splited_paths.filter(function(v){return v!==''});
                    utm_content = splited_paths.join('-');
                }
            }
            var nssource = window.location.hostname;
            $('#slider-oferplan a').each(function(index, elem){
                var href = $(this).attr('href');
                if (typeof href != 'undefined') {
                    href = href.replace(/xxmarketingcontentxx/g, utm_content.toLowerCase());
                    href = href.replace(/xxnssourcexx/g, nssource.toLowerCase());
                    $(this).attr('href', href);
                }
            });

        });
    })(jQuery);
</script>

