		 */
		if(window.location.hash == '#comments') {
			commentsShow.showInNews(commentsShowParams);
		}
		$('.tooltips, .comenta a').bind( "click", function() {
			commentsShow.showInNews(commentsShowParams);
		});
	});
}
</script><div class="container subwrapper ">
				
					
