<script>
$(window).load(function() {

    function renderBtnLikeFacebook(link)
    {
        var load_like = false;
        var url = $(link).attr('href');
        var title = $(link).attr('title');
        
        $(link).parent().parent().find('.me_gusta').each(function(){
             if(
                $(this).attr('load-facebook') === undefined){
                    var btn_like_facebook = '<a href="http://www.addthis.com/bookmark.php" class="addthis_button_facebook_like" addthis:url="'+url+'" addthis:title="'+title+'" ></a>';
                    $(this).html(btn_like_facebook);
                    $(this).attr('load-facebook' , true);
                    load_like = true;
                }
        });
        return load_like;
    }

    var load_like = false;
    var pestana = $('#1');
    pestana.find('a').each(function(){
        load_like = renderBtnLikeFacebook(this);            
    });
    socialNetworks.toolboxAddthis('.btn-like-facebook');

    $('a[data-toggle="tab"]').on('show.bs.tab', function (e) {
        var load_like = false;
        var pestana = $(e.target.hash);
        pestana.find('a').each(function(){
            load_like = renderBtnLikeFacebook(this);    
        });
        if(load_like){
            socialNetworks.toolboxAddthis('.btn-like-facebook');        
        }
    })
});
</script>

