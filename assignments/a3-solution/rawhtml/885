<script>


$(document).ready(function(){
	init_pushstate();
	init_fecha_hace();
	init_af();
init_botones();
init_tips();
$('.autogrow').autogrow();



		$('.comu_caja_opc p').mouseover(function()
			{
			var estado_sobre  = $(this).attr('estado-sobre');
			$(this).html(estado_sobre);

			var div = $(this).next('div:first');
			var estado_sobre  = div.attr('estado-sobre');
			var estado_normal = div.attr('estado-normal');

			div.removeClass(estado_normal).addClass(estado_sobre);
			}
		).mouseout(function()
			{
			var estado_normal = $(this).attr('estado-normal');
			$(this).html(estado_normal);

			var div = $(this).next('div:first');
			var estado_sobre  = div.attr('estado-sobre');
			var estado_normal = div.attr('estado-normal');

			div.removeClass(estado_sobre).addClass(estado_normal);
			});
	autocomplete('#bus_superior','buscar-juegos');	comu_init_menu();
	textarea();
});

