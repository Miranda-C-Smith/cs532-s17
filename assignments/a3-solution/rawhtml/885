	<script>
		setTimeout(function() {
			if ($('#pie_video').css('display')!='block') {
				set_cookie('adblock', 'true', '999', '/', '', '' );
			}
		}, 999);
		var cargado_video_pie = 0;
		$(window).scroll(scroll_pie_video);

		function isInViewPie(elem) {
			var ele = $(elem), xtra=40;
			var lBound = $(window).scrollTop(), uBound = lBound + $(window).height(), top = ele.offset().top-xtra, bottom = top + ele.outerHeight(true)+xtra;
			return (top > lBound && top < uBound) || (bottom > lBound && bottom < uBound) || (lBound >= top && lBound <= bottom) || (uBound >= top && uBound <= bottom);
		}

		function scroll_pie_video()	{
			if (isInViewPie('#pie_video')) {
				if (cargado_video_pie==0) {
					cargado_video_pie = 1;
					$('#pie_pub_google').hide();
					$('#pie_video_inte').show().load('/modulos/gamespace/videos.php?get=player_video&id_video=40654&zona=articulos&autostart=true&audio_foco=1&show_votos=0&show_super_title=1');
				}
			} else {
				if (cargado_video_pie==1) {
					if (screenfull.isFullscreen==false) {
						$(window).unbind('scroll', scroll_pie_video);
						jwplayer('jwvideo_40654').setVolume(0);
						$('#pie_video_inte').hide();

						setTimeout(function() {
							jwplayer('jwvideo_40654').remove();
						}, 4000);

					}
				}
			}
		}

