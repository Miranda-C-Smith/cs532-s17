<script>
        function usuarioOfflineModalCtrl($scope, $modal, $timeout) {
                $scope.openmsg = function () {
                        $timeout(function () {
                                $modal.open({
                                        templateUrl: 'modalUserOff.html',
                                        controller: false,
                                        backdrop: true,
                                        size: 'lg'
                                });
                        }, 1000);
                };
        }

        function clickRedirectOff(urlOff) {
                $("#modal-livro-conteudo").hide();
                $("#modal-livro-loading").show();
                window.location.replace(urlOff);
        }
</script>    </div>

