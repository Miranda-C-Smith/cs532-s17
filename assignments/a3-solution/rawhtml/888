function imgButton(mode){

	var checkEula = $("input:checkbox:checked").length;

	if(mode==0){

if(0==3 && 0==1){
    // 楽天確認
    alert('この商品は、楽天会員では購入できません。');
}else{
        // 利用規約確認
		if(checkEula>0){
			if(0!=1){
                var quantity = $('#quantity_n').val();
                $('#quantity').val(quantity);
				document.form1.submit();
			}else{
				alert('この商品は、現在カートに入っている商品と同時に購入は出来ません。');
			}
	    }else{
	        alert('「利用規約に同意して購入する」にチェックして下さい。');
	    }
}
    }else if(mode==1){
        // 利用規約確認済みor利用規約未使用
    	if(0==3 && 0==1){
            // 楽天確認
			alert('この商品は、楽天会員では購入できません。');
		}else{
            $('#seihon_pflg').val(0);
                var quantity = $('#quantity_n').val();
                $('#quantity').val(quantity);
            $('#form1').submit();
			//document.form1.submit();
		}
    }else if(mode==2){
        // 製本
        $('#seihon_pflg').val(1);
                var quantity = $('#quantity_s').val();
                $('#quantity').val(quantity);
        $('#form1').submit();
        //document.form1.submit();
    }else if(mode==3){
        // 利用規約確認
		if(checkEula>0){
            if(0!=1){
                $('#seihon_pflg').val(1);
                var quantity = $('#quantity_s').val();
                $('#quantity').val(quantity);
                $('#form1').submit();
            }else{
                alert('この商品は、現在カートに入っている商品と同時に購入は出来ません。');
            }
        }else{
            alert('「利用規約に同意して購入する」にチェックして下さい。');
        }
    }else if(mode==4){
        // DVD
        $('#dvd_sflg').val(1);
                var quantity = $('#quantity_d').val();
                $('#quantity').val(quantity);
        $('#form1').submit();
        //document.form1.submit();
    }else if(mode==5){
        // 利用規約確認
		if(checkEula>0){
            if(0!=1){
                $('#dvd_sflg').val(1);
                var quantity = $('#quantity_d').val();
                $('#quantity').val(quantity);
                $('#form1').submit();
            }else{
                alert('この商品は、現在カートに入っている商品と同時に購入は出来ません。');
            }
        }else{
            alert('「利用規約に同意して購入する」にチェックして下さい。');
        }
    }
}
