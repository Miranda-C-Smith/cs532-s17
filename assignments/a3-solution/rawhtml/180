var wbq = wbq || [];
wbq.push(function mediaTogglerInit_wbw_58abb17159549() {
  require(['jquery'], function ($) {
    $('#wbw-58abb17159549 .panel-content').find('article').each(function() {
      var $elm = $(this);
      var index = $elm.index();
      if (index === 0) {
        $elm.find('.media-left').addClass('active');
      }

      $elm.not(':last-child').find('.media-body').on('mouseover', function() {
        var $item = $(this);
        $item.parent().find('.media-left').addClass('active');
        $item.parent().siblings().find('.media-left').removeClass('active');
      });
    });
  });
});
</script>
