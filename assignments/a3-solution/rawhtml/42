<script>

var ratingHistoryData = [{ key:"Rating (moving average)", values:[], color: '#80AF3F' }];
var chart = stats.graph.createDefaultChartObject();
var tooltip = d3.select("body")
	.append("div")
	.style("position", "absolute")
	.style("z-index", "10")
	.classed("app-changelog-type-box app-changelog-type-box-update", true)
	.style("visibility", "hidden");
	
updateRatingChart = function() {
  chart.update();
  var annotationElements = d3.select("#ratinghistory-graph > svg .nv-interactive").selectAll(".annotation").data(
      ratingHistoryData[0].values.filter(function(d) { return d.length > 2; }));  
  annotationElements.enter().append("circle");  
  annotationElements.transition().duration(250)
  	.attr("class", "annotation")
  	.attr("cx", function(d) {
  		return chart.xScale()(Date.parse(d[0]));
	}).
  	attr("cy", function(d) { return chart.yScale()(d[1]); }).
  	attr("style", "fill:#e6a509;fill-opacity:0.8").
  	attr("stroke-width", 1).
  	attr("stroke", "#fff").
  	attr("r", "5");
  annotationElements.
  	on("mouseover", function(d){return tooltip.style("visibility", "visible").text(d[2].annotationText);}).
	on("mousemove", function(){return tooltip.style("top", (d3.event.pageY-20)+"px").style("left",(d3.event.pageX+10)+"px");}).
	on("mouseout", function(){return tooltip.style("visibility", "hidden");});
  annotationElements.exit().remove();
}

nv.addGraph(function() {
	chart.margin({left: 30})
		.x(function(d) { return Date.parse(d[0]) })
		.y(function(d) { return d3.round(d[1], 2) });
	chart.showLegend(false);
		
	chart.xScale(d3.time.scale());
	chart.xAxis
		.tickFormat(function(d) { return d3.time.format('%b %e, %Y')(new Date(d)) })
		.showMaxMin(false);
	
	chart.yAxis
		.tickValues([1,2,3,4,5])
		.showMaxMin(false);
	
	chart.forceY([1, 5]);
	
	d3.select("#ratinghistory-graph > svg")
		.datum(ratingHistoryData)
		.transition().duration(300).call(chart);
	
	nv.utils.windowResize(updateRatingChart);
	updateRatingChart();
	return chart;
});



</script>
