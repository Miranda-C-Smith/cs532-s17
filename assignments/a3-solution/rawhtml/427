            function siwp_genid() {
                var cid = '', chars = 'abcdefghijklmnopqrstuvwxyz0123456789';
                for (var c = 0; c < 40; ++c) { cid += chars.charAt(Math.floor(Math.random() * chars.length)); }
                return cid;
            };
