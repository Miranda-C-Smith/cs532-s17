            function siwp_refresh(id, audioObj) {
                var cid = siwp_genid();
                document.getElementById('input_siwp_captcha_id_' + id).value = cid;
                document.getElementById('siwp_captcha_image_' + id).src = 'http://toptdawl.com/wp-content/plugins/securimage-wp/lib/siwp_captcha.php?id=' + cid;
                if (null !== audioObj) {
                    audioObj.refresh(cid);
                }
            
