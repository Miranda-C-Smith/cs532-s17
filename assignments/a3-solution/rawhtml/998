    <script type="text/javascript" src="//platform.twitter.com/widgets.js"></script>
<script type="text/javascript">
//<![CDATA[
var g_Tweets;
$(document).ready(function($) {
    // Define Tweets Class
    Tweets = {
        page : 1
        ,posts : {}
        ,next_page : {}
        ,news_id : 219891        ,tweets_block : $('ul#tweets')
        ,loader_block : $('div#tweets-loader')
        ,more_block : $('div#tweets-more')
        ,init : function()
        {
            g_Tweets = this;
            this._request(1);
        }
        ,_request : function(page)
        {
            this.loader_block.show();
            this.more_block.hide();
            // jsonp request
            this.page = page;
            var script = document.createElement('script');
            script.type = 'text/javascript';
            script.src = 'http://satellite1.natalie.mu/tweets/jsonp/news/' + this.news_id + '/ipp/30/page/' + this.page;
            document.body.appendChild(script);
        }
        ,_render : function(res)
        {
                this.loader_block.hide();
            // is responses exists?
            if (this.page == 1 && typeof res === 'undefined') {
                $('div#NA_res').hide();
                return;
            }
            // Set comment list
            if (typeof res !== 'undefined') {
                this.more_block.show();
                this.posts = res.posts;
                this.next_page = res.next_page;
                if (this.page == 1) {
                    // append list
                    this._appendList(0, 5);
                    // set first next-page event
                    if (this.posts.length > 5) {
                        var _this = this;
                        $('a', this.more_block).unbind('click');
                        $('a', this.more_block).click(function () {
                            // append list
                            _this._appendList(5, 25);
                            // set next-page event
                            _this._attachNextPageEvent(_this.next_page);
                            return false;
                        });
                    } else {
                        this.more_block.hide();
                    }
                } else {
                    // append list
                    this._appendList(0, this.posts.length);
                    // set next-page event
                    this._attachNextPageEvent(this.next_page);
                }
            }
        }
        ,_attachNextPageEvent : function(page)
        {
            if (!!page) {
                var _this = this;
                $('a', this.more_block).unbind('click');
                $('a', this.more_block).click(function () {
                    _this._request(page);
                    return false;
                });
            } else {
                this.more_block.hide();
            }
        }
        ,_appendList : function(offset, count)
        {
            var max = count + offset;
            for (var i = offset; i < max; i++) {
                if (!!this.posts[i]) {
                    if ($('#tw_'+this.posts[i].tweet_id).length<1) this.tweets_block.append(this._generateTweetRow(this.posts[i]));
                }
            }
        }
        ,_generateIcon : function(post)
        {
            return $('<li><a href="https://twitter.com/' + post.screen_name + '" target="_blank"><img src="' + post.profile_image_url + '" width="24" height="24" /></a></li>');
        }
        ,_generateTweetRow : function(post)
        {
            var src = [];
            // Generate html code
            row = [];
            row.push('<li id="tw_'+ post.tweet_id +'">');
            row.push('<div class="NA_twIcon""><a href="https://twitter.com/' + post.screen_name + '" target="_blank"><img src="' + post.profile_image_url + '" width="48" height="48" /></a></div>');
            row.push('<div class="NA_twAttr">');
            row.push('<p class="NA_twName"><a href="https://twitter.com/' + post.screen_name + '" target="_blank">' + post.name + ' <span class="res-tw-id">@' + post.screen_name + '</span></a></p>');
            row.push('<p class="NA_twDate"><a href="https://twitter.com/' + post.screen_name + '/statuses/' + post.tweet_id + '" target="_blank" title="' + this._getAbsoluteTime(post.created_at) + '">' + this._getRelativeTime(post.created_at) + '</a></p>');
            row.push('</div>');
            row.push('<div class="NA_twUI">');
            row.push('<a class="NA_twReply" href="https://twitter.com/intent/tweet?in_reply_to=' + post.tweet_id + '" target="_blank">返信</a>');
            row.push('<a class="NA_twRT" href="https://twitter.com/intent/retweet?tweet_id=' + post.tweet_id + '" target="_blank">リツイート</a>');
            row.push('<a class="NA_twFav" href="https://twitter.com/intent/favorite?tweet_id=' + post.tweet_id + '" target="_blank">お気に入り</a>');
            row.push('</div>');
            row.push('<p class="NA_twBody">' + post.tweet_text + '</p>');
            row.push('</li>');
            src.push(row.join(''));
            return $(src.join(''));
        }
        ,_getRelativeTime : function(post_time)
        {
            var sec_diff = Math.round(new Date().getTime() / 1000) - post_time;
            if (sec_diff < (60 * 60)) {
                return Math.floor(sec_diff / 60) + '分';
            } else if (sec_diff < (60 * 60 * 24)) {
                return Math.floor(sec_diff / (60 * 60)) + '時間';
            } else {
                return this._getAbsoluteTime(post_time);
            }
        }
        ,_getAbsoluteTime : function(post_time)
        {
            var post_date = new Date(post_time * 1000);
            return post_date.getFullYear() + '年' + (post_date.getMonth() + 1) + '月' + post_date.getDate() + '日 ' + post_date.getHours() + ':' + (post_date.getMinutes() < 10 ? '0' : '') + post_date.getMinutes();
        }
    };
    // Do First Request
    Tweets.init();
});

function getTweets_callback(json)
{
    g_Tweets._render(json);
}

//]]>
</script>
