                            <script type="text/javascript">
                        /* Toggle */
                        $(document).ready(function($){
                            $(".NA_hideBlock").hide();
                            var toggleFlg = "close";
                            $(".NA_toggleBtn").click(function(){
                                $(".NA_hideBlock").slideToggle('fast', 'swing');
                                if(toggleFlg == "close"){
                                    $(this).addClass("NA_toggleOn");
                                    toggleFlg = "open";
                                }else{
                                    $(this).removeClass("NA_toggleOn");
                                    toggleFlg = "close";
                                }
                            });
                        });
                        </script>
