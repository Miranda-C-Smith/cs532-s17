        var activo = false;
        var MenuFade = "";
        $(function(){
                $("#div_menu").click(function(){
                        if(activo == false){
                                $("ol#MMEnu_").stop(true).fadeIn(200);
                                activo = true;
                        }else{
                                $("ol#MMEnu_").fadeOut(100);
                                activo = false;
                        };
                });
                $("ol#MMEnu_").hover(function(){
                                clearTimeout(MenuFade);
                },function(){
                                MenuFade = setTimeout(function(){
                                        $("#div_menu").click();
                                }, 1500);
                });
        });
        //Obteniendo Fecha
        F = new Date();
        Dia = F.getDate();
        M = ["ENERO", "FEBRERO", "MARZO", "ABRIL", "MAYO"]
        Mes = M[F.getMonth()]
        $("div#Consulta p").html(Dia + " DE " + Mes + " DE 2016")
</script>


