                                    $("#live-bfm-tv-head").html("<img class='lazy img-responsive-l' src='http://static.bfmtv.com/ressources/img/mediaplayer/player-bfmtv_fin_head.png'>");
                                    $("#live-bfm-business-head").html("<img class='img-responsive-l' src='http://static.bfmtv.com/ressources/img/mediaplayer/player-bfmbusiness_fin_head.png'>");
