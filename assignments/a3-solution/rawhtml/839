	(function(){
		var deferLoaded = false;
		function loadDefer(){
			if( !deferLoaded ){
				var deferScripts = ['http://a.espncdn.com/redesign/0.336.5/js/espn-defer.js', 'http://a.espncdn.com/redesign/0.336.5/js/espn-defer-low.js'];
