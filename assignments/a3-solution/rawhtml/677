<script src="//s.yimg.jp/images/approach/jslib/deeplink-2.0.5.js"></script>
<link rel="stylesheet" href="//s.yimg.jp/images/riff/1.1.0/styles/riff-scoped.min.css">
<link rel="dns-prefetch" href="//approach.yahooapis.jp" />


<script>
(function () {
    if(typeof YAHOO === 'undefined'){
        YAHOO = {};
    }
    if(typeof YAHOO.JP === 'undefined'){
        YAHOO.JP = {};
    }
    if(typeof YAHOO.JP.yw === 'undefined'){
        YAHOO.JP.yw = {};
    }

})();


YAHOO.JP.yw.web2app = (function () {
    var _key = '';
    var _hasLocalStorage;
    var _onShow = function() {};
    var _onHide = function() {};

    var closeFlag = false;
    var openFlag = false;

    var approach;
    var option;
    var aprParams;

    // ローカルストレージが利用できるかの判定
    if (typeof localStorage === "undefined"
        || typeof Storage === "undefined"
        || localStorage instanceof Storage === false) {
        _hasLocalStorage = false;
    } else {
        _hasLocalStorage = true;
    }



    return {
        init : function() {

            approach = new YAHOO.JP.Approach({yjdnEAppId: "524qviatmbwRhaNYUqNL4M2Bm0pUgQ3hyfZvZ8gNw1W8V51e5iaf2yXn5iw2DaxyIqKw9hdpO1M3yslV7WIo8_gPneQ_1GSz9w6niEYXnSo05PY-", testMode: false});
            option   = YAHOO.JP.Approach.OPT_APP_OPEN | YAHOO.JP.Approach.OPT_STORE_OPEN;
            aprParams = {};
            aprParams.ios = {};
            aprParams.ios.universalLinks = {};
            aprParams.android = {};

            aprParams.isLoggedInd = true;
            aprParams.ios.universalLinks.enable = true;
            aprParams.ios.universalLinks.ignoreFallbackCap = false;

            var event = "touchstart";

            var target = $(".aprch");
            target.each(function() {
                $(this).on(event, function() {
                    var url   = $(this).data("url");
                    var rd    = $(this).data("rd");
                    var pos   = $(this).data("pos");
                    var ylk   = $(this).data("ylk");
                    var param = $(this).data("param");
                    var referrer = $(this).data("referrer");

                    var ylks = ylk.split(";");
                    var id;
                    $.each(ylks, function(index, element) {
                        if (element.indexOf("sec:") > -1) {
                            id = (element.split(":"))[1];
                        }
                    });

                    // id指定がない場合はw2aデフォルトのものを使用
                    if (id === undefined || id.length === 0) {
                        id = "stcftw2a";
                    }

                    ins.beaconClick(id, "android", pos);

                    YAHOO.JP.yw.web2app.deepLink(url, referrer, rd, pos, param);
                    return;
                });
            });

        },
        deepLink : function(url, referrer, rd, pos, param) {

            aprParams.ios.storeUrl = rd;
            aprParams.android.installReferrer = 'utm_source%3Dyahoo%26utm_medium%3Dweather%26utm_campaign%3D' + referrer;
            aprParams.params = {};
            aprParams.params['from'] = referrer;
            if (param !== null && param.length > 0) {
                var params = param.split(":");
                aprParams.params[params[0]] = params[1];
            }

            approach.replaceAppScheme(url, option, aprParams);
        }
    }
})();
</script>



