			if (ev.target.value.length > 45 || ev.target.value.split('\n').length > 1) {
				// Si le texte fait plus de 45 caractères ou a au moins un retour à la ligne, on redimensionne le bloc
				ev.target.style.height = 'auto';
				ev.target.style.height = (ev.target.scrollHeight + 40) + 'px';
			} else {
				// Si le texte est trop court, on ne redimensionne pas le bloc
				ev.target.style.height = null;
			}
		}

		function delayedResize(ev) {
			// On attend le prochain cycle pour être sûr d'avoir le nouveau texte
			setTimeout(function() { resize(ev) }, 0);
