        function OnClientRated10452(sender, args)
        {
            var txtRate = $('#ctl00_cphMiddle_Sampa_Web_View_RemarkUI_RemarkListEditControl20cphMiddle_10452_reRemarkEdit_txtRemarkRate');
            if (txtRate)
            {
                txtRate.val(sender.get_value());
                var validator = $('#ctl00_cphMiddle_Sampa_Web_View_RemarkUI_RemarkListEditControl20cphMiddle_10452_reRemarkEdit_rfvRemarkRate');
                if (validator) {
                    ValidatorValidate(validator[0]);
                }                
            }
        }

        function ShowRemaining10452(txtBox, lblRemainedId, maxLength) {
            $('#' + lblRemainedId).text((maxLength - txtBox.value.length).toString());
        }
            
        function SetParentId10452(parentId) {
            $('#ctl00_cphMiddle_Sampa_Web_View_RemarkUI_RemarkListEditControl20cphMiddle_10452_reRemarkEdit_txtParentId').val(parentId);
        }
            
        function RefreshParent10452()
        {
            if (window.RefreshRemarkList10452_)
            {
                window.RefreshRemarkList10452_();
            }
        }

