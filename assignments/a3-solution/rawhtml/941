        var ctl00_cphMenu_Sampa_Web_View_GeneralUI_MegaMenuControl00cphMenu_10461_pnlMenuContainerAppendix;
        var secondMenuContainerClass = '';
        if (secondMenuContainerClass != '') {
            ctl00_cphMenu_Sampa_Web_View_GeneralUI_MegaMenuControl00cphMenu_10461_pnlMenuContainerAppendix = $('.' + secondMenuContainerClass + ' .menu3dmega > ul').html();
        } else {
            ctl00_cphMenu_Sampa_Web_View_GeneralUI_MegaMenuControl00cphMenu_10461_pnlMenuContainerAppendix = '';
        }


        $('#ctl00_cphMenu_Sampa_Web_View_GeneralUI_MegaMenuControl00cphMenu_10461_pnlMenuContainer').menu3d({
            responsive: true,
            //clickable: false,
            skin: 'skin-gray',
            speed: 600,
            responsiveAppendix: ctl00_cphMenu_Sampa_Web_View_GeneralUI_MegaMenuControl00cphMenu_10461_pnlMenuContainerAppendix,
            appendixWidth: 768,
            vertical: false,
            easing: 'default',
            hoverIn: 'fadeIn',
            hoverOut: 'fadeOut',
            rtl: true,
            offCanvasInResponsive: false,
            rnText: '',
            mainMenuContainerId: 'ctl00_cphMenu_Sampa_Web_View_GeneralUI_MegaMenuControl00cphMenu_10461_pnlMegaMenu',
            mainPageContainerId: 'generalMainWrap'
        });
    });
    var isFixed = false;
    $(window).scroll(function () {
        if (isFixed) {
            if ($(window).width() > 767) {
                if ($(document).scrollTop() > $('.mainHeaderWrap').height()) {
                    if (($('.mainContentWrap').height() > 400)) {
                        $('.mainHeaderWrap').addClass('fixedMenu');
                    }
                } else {
                    $('.mainHeaderWrap').removeClass('fixedMenu');
                }
            }
        }
    });
</script>


			</div>
		</div>
	</div>
