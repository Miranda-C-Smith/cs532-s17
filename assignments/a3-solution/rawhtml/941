    <script type="text/javascript">
        try {
            if ($telerik.$) {
                window.$ = $telerik.$;
            }
        }
        catch (err) {
        }

        function ShowRemarkEdit10452(parentId, remarkId) {
            if ($('#' + parentId).is(":visible")) {
                $('#' + parentId).slideUp("slow");
                $('#ctl00_cphMiddle_Sampa_Web_View_RemarkUI_RemarkListEditControl20cphMiddle_10452_pnlRemarkEdit').detach().appendTo('#ctl00_cphMiddle_Sampa_Web_View_RemarkUI_RemarkListEditControl20cphMiddle_10452_pnlComment');
                $('#ctl00_cphMiddle_Sampa_Web_View_RemarkUI_RemarkListEditControl20cphMiddle_10452_pnlComment').slideDown("slow");
                $('#ctl00_cphMiddle_Sampa_Web_View_RemarkUI_RemarkListEditControl20cphMiddle_10452_pnlShowMainComment').hide();
                SetParentId10452('');
            } else {
                $('#ctl00_cphMiddle_Sampa_Web_View_RemarkUI_RemarkListEditControl20cphMiddle_10452_pnlRemarkEdit').parent().slideUp("slow");
                $('#ctl00_cphMiddle_Sampa_Web_View_RemarkUI_RemarkListEditControl20cphMiddle_10452_pnlRemarkEdit').detach().appendTo('#' + parentId);
                $('#' + parentId).slideDown("slow");
                if (parentId != 'ctl00_cphMiddle_Sampa_Web_View_RemarkUI_RemarkListEditControl20cphMiddle_10452_pnlComment') {
                    $('#ctl00_cphMiddle_Sampa_Web_View_RemarkUI_RemarkListEditControl20cphMiddle_10452_pnlShowMainComment').show();
                } else {
                    $('#ctl00_cphMiddle_Sampa_Web_View_RemarkUI_RemarkListEditControl20cphMiddle_10452_pnlShowMainComment').hide();
                }
                SetParentId10452(remarkId);
            }
            $('#ctl00_cphMiddle_Sampa_Web_View_RemarkUI_RemarkListEditControl20cphMiddle_10452_pnlRemarkEdit' + ' div.LastMessageSuccess').hide();
        }
        
        function DisableReplies10452() {
            $('#ctl00_cphMiddle_Sampa_Web_View_RemarkUI_RemarkListEditControl20cphMiddle_10452_pnlRemarkListSimple' + ' div.reply').hide();
            $('#ctl00_cphMiddle_Sampa_Web_View_RemarkUI_RemarkListEditControl20cphMiddle_10452_pnlShowMainComment').hide();
        }
    </script>

<div id="ctl00_cphMiddle_Sampa_Web_View_RemarkUI_RemarkListEditControl20cphMiddle_10452_pnlRemarkListSimple" class="noPrint">
							
    <div class="RadAjaxPanel" id="ctl00_ctl00_cphMiddle_Sampa_Web_View_RemarkUI_RemarkListEditControl20cphMiddle_10452_rlsRemarkList_ucMessageBox_rttMessageBoxPanel">

							</div>

    <script language="javascript" type="text/javascript">
        try {
            if ($telerik.$) {
                window.$ = $telerik.$;
            }
        }
        catch (err) {
        }

        function SetLoadingPanel10452(updatedControlId, visible) {
            var loadingPanel = $find("ctl00_ralpGeneral");
            if (loadingPanel != null) {
                if (visible) {
                    loadingPanel.show(updatedControlId);
                } else {
                    loadingPanel.hide(updatedControlId);
                }
            }
        }

        function RemarkSubmitLike10452(remarkId) {
            SetLoadingPanel10452('pnlLike' + remarkId, true);
            $('#ctl00_cphMiddle_Sampa_Web_View_RemarkUI_RemarkListEditControl20cphMiddle_10452_rlsRemarkList_txtRemarkId').val(remarkId);
            $('#ctl00_cphMiddle_Sampa_Web_View_RemarkUI_RemarkListEditControl20cphMiddle_10452_rlsRemarkList_btnLike').click();
        }

        function RemarkSubmitDislike10452(remarkId) {
            SetLoadingPanel10452('pnlLike' + remarkId, true);
            $('#ctl00_cphMiddle_Sampa_Web_View_RemarkUI_RemarkListEditControl20cphMiddle_10452_rlsRemarkList_txtRemarkId').val(remarkId);
            $('#ctl00_cphMiddle_Sampa_Web_View_RemarkUI_RemarkListEditControl20cphMiddle_10452_rlsRemarkList_btnDislike').click();
        }

        function RemarkUpdateCount10452(remarkId, likeCount, dislikeCount) {
            if (remarkId != '') {
                if (typeof likeCount !== "undefined") {
                    $('#lblLike' + remarkId).text(likeCount);
                }
                if (typeof dislikeCount !== "undefined") {
                    $('#lblDislike' + remarkId).text(dislikeCount);
                }
                SetLoadingPanel10452('pnlLike' + remarkId, false);
            }
        }

        function RemarkNextPage10452(parentId, nextPageIndex, parentClientId) {
            SetLoadingPanel10452(parentClientId, true);
            $('#ctl00_cphMiddle_Sampa_Web_View_RemarkUI_RemarkListEditControl20cphMiddle_10452_rlsRemarkList_txtNextPage').val(parentId + "," + nextPageIndex + "," + parentClientId);
            $('#ctl00_cphMiddle_Sampa_Web_View_RemarkUI_RemarkListEditControl20cphMiddle_10452_rlsRemarkList_btnNextPage').click();
        }

        function RemarkUpdateNextPage10452(parentClientId) {
            $('#' + parentClientId + ' .remarkButton').remove();
            $('#ctl00_cphMiddle_Sampa_Web_View_RemarkUI_RemarkListEditControl20cphMiddle_10452_rlsRemarkList_pnlNextPageResult > div.remarkWrapper').detach().appendTo('#' + parentClientId + ' > div.moreResult');
            SetLoadingPanel10452(parentClientId, false);
            $('#' + parentClientId + ' > div.moreResult').slideDown("slow");
        }

