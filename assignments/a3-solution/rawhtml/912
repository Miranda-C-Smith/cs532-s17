<script type="text/javascript" src="/js/video.js"></script>
<script type="text/javascript">
jQuery(function($) {
	$(document).ready(function() {

        $('.review-pagination a').click(function(e) {
            e.preventDefault();


            if ($(this).hasClass("previous")) {
                var page = $('.review-pagination li.active').prev().index();
            } else if ($(this).hasClass("next")) {
                var page = $('.review-pagination li.active').next().index();
            } else {
                var page = $(this).parent().index();
            }

            $('.review-pagination li.active').removeClass('active');
            $('.review-pagination li').eq(page).addClass('active');
            $('.review-pagination li.disabled').removeClass('disabled');

            if (page == 1) $('.review-pagination li').eq(0).addClass('disabled');
            if (page == ($('.review-pagination li:last-child').index() - 1)) $('.review-pagination li:last-child').addClass('disabled');

            $('.review-container:not(.review-page-' + page +')').hide();
            $('.review-page-' + page).show();

            $('html, body').animate({
                scrollTop: $(".review-stars-overall").offset().top
            }, 500);
        });

        $('#write_it').click(function() {
            $('.write-review-wrapper').addClass('visible').show();
        });
        $(document).on('click','.mousetrap, #zoom1', function(e) {
            var newWidth = Math.min(($(document).width()*.9), 600);
            var images = [{src: $('#zoom1').attr('href')}];

            $('#more_product_images a').each(function(){
                if ($(this).attr('href') != $('#zoom1').attr('href')) {
                    images.push({src: $(this).attr('href')});
                }
            });

            $('.popupVideo').each(function(){
                var content = $(this).html();
                var width = $(this).width()-10;
                var height = $(this).find('.video-js').height();

                if (height == null) {
                    height = $(this).find('iframe').attr('height');
                }

                hregex = new RegExp(height,"g");
                var ratio = width/height;
                height = newWidth/ratio;

                if (content.search("iframe") >= 0) {
                     images.push({src: $(content.replace(/290/g, newWidth).replace(hregex, height)), type: 'inline'});
                } else {
                    $(this).find('.video-js').css('width', newWidth).css('height', height).parent().css('width', newWidth);
                    images.push({src: $(this).css('width', newWidth+10).show(), type: 'inline'});
                }
            });
            $.magnificPopup.open({
                items:  images,
                gallery: {
                    enabled: true
                },
                type: 'image'
            });
            return false;
        });

        		$('.btn-oos-notify').click(function() {
			if ($('.btn-oos-notify').html() == 'Submit') {
				$.post('http://www.bestvibrator.info/product/oosNotify', {
					"email": $('#oos_email_addr').val(),
					"sku": 'CNVNAL-55034'
				}, function(data) {
					if (data.response != 'success') {
						$('#oos_email_error').html(data.message != undefined? data.message: 'There was a problem saving your notifcation. Please try again later or contact Support');
					}
					else {
						$('#email_oos_notify').hide();
						$('#email_oos_notify_success').show();
					}
				}, "json");

			} else {
				$('#email_oos_notify_details').show();
				$('.btn-oos-notify').html('Submit').css('width','100px');
			}
		});
                

	});

});
</script>
