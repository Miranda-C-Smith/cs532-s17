        var node = document.getElementsByTagName('script')[0];
        node.parentNode.insertBefore(gads, node);
    })();
</script>
<script type="text/javascript">
    // Saftey timer just in case our CDN breaks for any reason
    window.setTimeout(function() {
        for (var i = 0; i < window.Headertag_defer_queue.length; i++) {
            var qf = window.Headertag_defer_queue[i];
            if (typeof qf === 'function') {
                qf();
            }
        }
        window.Headertag_defer_queue = {};
        window.Headertag_defer_queue.push = function(foo) {
            if (typeof foo === 'function') foo();
        };
    }, 500);
</script>
