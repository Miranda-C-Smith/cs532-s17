<script language="JavaScript" type="text/javascript">
    s.channel= 'news:article';
    s.pageName = 'Vanessa Hudgens, the Queen of Coachella, Gushes About Pregnant Beyonce Headlining the Show';
    s.server = 'www.etonline.com';
