<script type="text/javascript">window.BMR.run('http://sp.naver.com/sp');</script>
<script type="text/javascript">
	function redirectMobile() {
		setCookie("NTC_PC_MODE", "false");
		var sRedirectPcUrl = "";
		
		if (location.search === "") {
			sRedirectPcUrl = 'http://m.tv.naver.com'+ location.pathname;
		} else {
			sRedirectPcUrl = 'http://m.tv.naver.com' +
							  location.pathname +
	        				  location.search.split("&")[0];
		}
		location.href = sRedirectPcUrl;
	}
	
	function setCookie(name, value) {
		var domain = ".tv.naver.com";
		var path = "/";
		document.cookie = name + "=" + value + "; path=" + path + "; domain=" + domain;
    }
</script>
