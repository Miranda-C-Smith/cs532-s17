<script> var e = document.createElement("script"); e.src = 'http://d03.dyn.topstarnews.net/topstarnews?_as_L='+document.lastModified+'&_as_T='+document.title+'&_as_R='+document.referrer; e.type="text/javascript"; document.getElementsByTagName("head")[0].appendChild(e); </script>
	<!-- /// jquery totop-->
	<!-- /// easing plugin ( optional ) /// -->
	<!-- /// UItoTop plugin /// -->
	<!-- /// Starting the plugin /// -->
<script type="text/javascript">
    $(document).ready(function() {
        /*
         var defaults = {
         containerID: 'toTop', // fading element id
         containerHoverID: 'toTopHover', // fading element hover id
         scrollSpeed: 1200,
         easingType: 'linear'
         };
         */

//        $().UItoTop({easingType: 'easeOutQuart'});

        $.ajax({
            url: 'http://d01.dyn.topstarnews.net/dnib_get_contents.php?dyn_type=hot&func=dnibcallback1',
            type: 'get',
            dataType: 'jsonp',
            cache: true,
            jsonpCallback: 'dnibcallback1',
//            error: function() {
//                alert('Loading failure');
//            },
            success: function(response) {

                layer_begin = "<table width=\"100%\" border=\"0\" cellpadding=\"0\" cellspacing=\"0\"><tbody>";
                layer_end = "</tbody></table>";
                layer_body = "<tr><td valign=\"top\"><table border=\"0\" cellpadding=\"0\" cellspacing=\"0\"><tbody><tr><td><div style=\"padding:3px;\"></div>&nbsp;&nbsp;<i class=\"icon-double-angle-right\"></i>&nbsp;&nbsp;<a href=\":::1:::\">:::2:::</a><div style=\"padding:4px;\"></div></td></tr></tbody></table></td></tr>";

                $(layer_begin).appendTo("#dnib_hoti_1");
                $(layer_begin).appendTo("#dnib_hoti_3");
                var i = 0;
                n = 0;
                $.each(response, function(i) {
                    news = this;
                    link_url = "detail.php?number=" + news.N + "&thread=" + news.C;
                    if (i < 8) {
                        tmp = layer_body.replace(":::1:::", link_url);
                        news_title = news.T;
                        news_title3 = news_title.substr(0,25);
                        tmp1 = tmp.replace(":::2:::",news_title+"…");
                        tmp3 = tmp.replace(":::2:::", news_title3+"…");
                        $(tmp1).appendTo("#dnib_hoti_1");
                        $(tmp3).appendTo("#dnib_hoti_3");
                    }
                    else {
                        if (n == 0) {
                            $(layer_end).appendTo("#dnib_hoti_1");
                            $(layer_end).appendTo("#dnib_hoti_3");
                            $(layer_begin).appendTo("#dnib_hoti_2");
                        }

                        tmp = layer_body.replace(":::1:::", link_url);
                        tmp = tmp.replace(":::2:::", news.T+"…");
                        $(tmp).appendTo("#dnib_hoti_2");
                        n = n + 1;
                    }
                    i = i + 1;

                });
                
                $(layer_end).appendTo("#dnib_hoti_2");

            }
        });
    });
</script>

