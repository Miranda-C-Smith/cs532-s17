        $(document).ready(function() {
            $(".view_comment").each(function(){
            var post_id = $(this).attr("data-postid");
            var blog_id = $(this).attr("data-blogid");
            $.getJSON('https://comment.detik.com/v2/?callback=?', {count:1, key:post_id, group:blog_id, prefix:'blog'}, function(response){
                    $("#comment_"+ post_id + "_" + blog_id).html(response);
