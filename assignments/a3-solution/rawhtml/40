<script type="text/javascript">

    if (('') == '110456') {
        $j(".ci").find(".menu05").css("display", "none")
    }

    var sTLPage = '0';

    $j(document).ready(function () {
        var jvtpage = $j.url().attr("file").toLowerCase();
        var jvtparam = ($j.url().attr("query") == null ? "" : $j.url().attr("query").toLowerCase().replace("genre=", ""));

        if (jvtpage == "genremain.aspx" && jvtparam != '') {
            jommenu = $j("div.ci > ul.gnb");
            if (jvtparam == "15456") jommenu.find("a.menu01").addClass("on");
            if (jvtparam == "15457") jommenu.find("a.menu02").addClass("on");
            if (jvtparam == "15458") jommenu.find("a.menu03").addClass("on");
            if (jvtparam == "15459") jommenu.find("a.menu04").addClass("on");
            if (jvtparam == "999") jommenu.find("a.menu05").addClass("on");
            if (jvtparam == "15460") jommenu.find("a.menu06").addClass("on");
        }


        $j('#header a.accessCheck').unbind('click.accessCheck').bind('click.accessCheck', function (e) {
            e.preventDefault();

            if (('False').toLowerCase() == 'false') {
                $j("#dialogAlert").jAlert({ "msg": "로그인을 해야 합니다.", "loc": jsf_base_GetYes24Login() });
            } else {
                $j(location).attr('href', $j(this).attr("href"));
            }

        });

        $j('div#header div.toolmenu ul.hd_left li.fst ul.sub').hide();
        $j('div#header div.toolmenu ul.hd_left li.fst').hover(function () {
            $j('ul:not(:animated)', this).slideDown('fast');
        },
        function () {
            $j('ul', this).slideUp('fast');
        });


        //$j("#schInput").jry_home_ShowAutoComplete({ ajaxurl: '/Pages/Ajax/Performance/AutoComplete.aspx/GetPerformance', idkeyctrl: 'txtPerformanceId', minlength: 1 , splitchar: '$$' });

        $j("#schInput").unbind("keypress.schInput").bind("keypress.schInput", function (evt) { if (evt.which == 13) { jsf_mm_Search(); return false; } });
        $j("#TopSearchBtn").unbind("keypress.TopSearchBtn").bind("keypress.TopSearchBtn", function (evt) { if (evt.which == 13) { jsf_mm_Search(); return false; } });
        $j("#TopSearchBtn").bind('click', function () { jsf_mm_Search(); return false; });
    });


    function jsf_mm_Search() {

        if (document.getElementById('schInput').value.length == 0) {
            jsf_Alert("검색어를 입력하세요.");
            $j("#schInput").focus();
            return false;
        } else {
            //location.href = "/Pages/Search/Search.aspx?SearchText=" + escape($j("#schInput").val()) + "&amp;SearchType=" + $j("#searchType").val();
            location.href = "/Pages/Search/SearchList.aspx?SearchText=" + escape($j("#schInput").val()) + "&SearchType=" + escape($j("#searchType").val()) + "&Gcode=009_105";
        }
    }
</script>
