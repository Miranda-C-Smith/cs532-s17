<script type="text/javascript">
    //    $j("#LeftBanner").jry_home_FloatingCenter('Z0-013', 5, 5);
    var licount = $j('#angel_bn > li').length;
    //for (var i = 0; i < licount; i++) { $j('#angel_pos').append("<li><a class='dcursor'><span>" + i + "</span></a></li>"); }
    for (var i = 0; i < licount; i++) { $j('#angel_sort').append("<li><a class='dcursor'><span id ='angel_sort_span_" + i + "'><img src='http://tkfile.yes24.com/img/common/ic_num.gif' alt='banner" + i + "' /></span></a></li>"); }
    var angelRanNum = Math.floor(Math.random() * licount);

    $j('#angel_bn').ulslide({
        statusbar: false,
        width: 78,
        height: 160,
        mousewheel: false,
        current: angelRanNum,
        affect: 'fade', /* 슬라이드기능 fade도 있음*/
        axis: 'x', /* 움직이는 방향 */
        navigator: '#angel_sort a',
        autoslide: 3000, /*자동슬라이드 1000은 1초*/
        duration: 0, /* 가속도 입니다. */
        onAnimate: jsf_angel_bnClass
    });

    $j('#angel_sort > li').hover(
            function (n) {
                $j(this).children("a").trigger("click");
            }
        );

    function jsf_angel_bnClass(settings, thisEl) {
        var jvCurIdx = settings.current;
        var licount = $j('#angel_bn > li').length
        var jvimg_ov = "<img src='http://tkfile.yes24.com/img/common/ic_num_ov.gif' alt='banner" + jvCurIdx + "' />"

        for (var i = 0; i < licount; i++) {
            $j("#angel_sort_span_" + i).html("<img src='http://tkfile.yes24.com/img/common/ic_num.gif' alt='banner" + i + "' />");
        }

        $j("#angel_sort_span_" + jvCurIdx).html(jvimg_ov);
    }

</script>

