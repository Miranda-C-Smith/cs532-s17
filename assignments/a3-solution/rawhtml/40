<script type="text/javascript">
    function jsf_main_floating_right_EventBanner() {
        $j('#ev_bn > li > a > img').width(78).height(110);
        var licount = $j('#ev_bn > li').length;
        if (licount == 0) {
            $j(".qu_type01").eq(1).hide();
        }
        for (var i = 0; i < licount; i++) {
            $j('#ev_sort').append("<li><a class='dcursor' id ='event_sort_span_" + i + "'><img src='http://tkfile.yes24.com/img/common/ic_num.gif' alt='banner" + i + "' /></a></li>");
        }
        var angelRanNum = Math.floor(Math.random() * licount);

        $j('#ev_bn').ulslide({
            statusbar: false,
            width: 78,
            height: 112,
            mousewheel: false,
            current: angelRanNum,
            affect: 'fade', /* 슬라이드기능 fade도 있음*/
            axis: 'x', /* 움직이는 방향 */
            navigator: '#ev_sort a',
            autoslide: 3000, /*자동슬라이드 1000은 1초*/
            duration: 0, /* 가속도 입니다. */
            onAnimate: jsf_event_bnClass
        });

        $j('#ev_sort > li').hover(
            function (n) {
                $j(this).children("a").trigger("click");
            }
        );
    };

    function jsf_event_bnClass(settings, thisEl) {
        
        var jvCurIdx = settings.current;
        var licount = $j('#ev_bn > li').length
        var jvimg_ov = "<img src='http://tkfile.yes24.com/img/common/ic_num_ov.gif' alt='banner" + jvCurIdx + "' />"

        for (var i = 0; i < licount; i++) {
            $j("#event_sort_span_" + i).html("<img src='http://tkfile.yes24.com/img/common/ic_num.gif' alt='banner" + i + "' />");
        }

        $j("#event_sort_span_" + jvCurIdx).html(jvimg_ov);
    }

    $j(document).ready(function () {
        if ($j(".angel_bn").find("li").length == 0) {
            $j("#divLeftWingAngel").css("display", "none")
        }

        jsf_main_floating_right_EventBanner();

        $j('#floatingGoTop').click(function (e) { e.preventDefault(); jsf_com_GoToTop(); });

        $j('#RightBanner a.accessCheck').unbind('click.accessCheck').bind('click.accessCheck', function (e) {
            e.preventDefault();

            if (('False').toLowerCase() == 'false') {
                $j("#dialogAlert").jAlert({ "msg": "로그인을 해야 합니다.", "loc": jsf_base_GetYes24Login() });
            } else {
                $j(location).attr('href', $j(this).attr("href"));
            }

        });

        //           # 나의 관심 공연 서버코드 프로퍼티
        //        if ('GetMyRegionSetting' != '0') {
        //            $j('div.RightBanner a.loca').removeClass('off').removeClass('on').addClass('on');
        //        }

        if ('0' != '0') {
            $j('div.RightBanner a.loca').attr('href', '/Pages/MyPage/MyPage_CustomSetting.aspx?Gcode=009_124');
        }

        jsf_main_floating_right_TodayViewPerformance("");

        $j('div#RightBanner').removeClass('hidden');


        var joPanel1 = $j('#divMyInterestPerformance ul');
        var licount1 = joPanel1.children('li').length;

        if (licount1 == 0) {
            joPanel1.siblings('h3').css('height', '32px');
        }
        else {
            joPanel1.siblings('h3').css('height', '33px');
        }

    });


    function jsf_main_floating_right_TodayViewPerformance(removeId) {

        var joPanel = $j('#ulTodayViewList');

        $j.ajax({
            async: true,
            type: "POST",
            url: "/Pages/Controls/Main/Ajax/TodayViewPerformance.aspx",
            data: { removeId: removeId },
            dataType: "html",
            success: function (data, textStatus) {

                joPanel.html(data);

                var joLi = joPanel.find('li');
                var tmpContainer = $j('<div id="divtmpContainer"></div>');
                var itemCount = 2;

                for (i = 0; i < joLi.length / itemCount; i++) {
                    var tmpUlObjet = $j('<ul></ul>');
                    tmpUlObjet.append(joLi.slice(i * itemCount, (i * itemCount) + itemCount));
                    tmpContainer.append(tmpUlObjet);
                }

                tmpContainer.find('ul').wrap('<li />');
                joPanel.html(tmpContainer.html());

                var licount = joPanel.children('li').length;
                var joCounter = joPanel.parent('div').find('p.counter span');

                joCounter.eq(1).text(licount == 0 ? 1 : licount);
                if (licount == 0) {
                    joPanel.siblings('h3').css('height', '15px');
                    joPanel.siblings('p.counter').removeClass('displayN').addClass('displayN');
                    joPanel.css('margin-top', '0px');
                }
                else {
                    joPanel.siblings('h3').css('height', '16px');
                    joPanel.siblings('p.counter').removeClass('displayN');
                    joPanel.css('margin-top', '3px');
                }

                joPanel.ulslide({
                    statusbar: false,
                    width: 80,
                    height: 88 * (joLi.length > 2 ? 2 : joLi.length),
                    duration: 200, /* 가속도 입니다. */
                    effect: {
                        type: 'slide', // slide, fade or carousel (use showCount for carousel)
                        axis: 'x',     // x, y
                        distance: 0,   // Distance between frames
                        showCount: 2
                    },
                    nextButton: 'div.RightBanner div.qu_type01 p.counter a.next',
                    prevButton: 'div.RightBanner div.qu_type01 p.counter a.pre',
                    printCurrentTo: joCounter.eq(0)
                });

                // 삭제버튼 컨트롤
                joPanel.find('a[name="btnTodayViewClose"]').each(function () {
                    $j(this).unbind('click.btnTodayViewClose').bind('click.btnTodayViewClose', function (e) {
                        e.preventDefault();

                        jsf_main_floating_right_TodayViewPerformance($j(this).attr('token'));
                    });
                });
            },
            error: function (xhr, textStatus, errorThrown) {
                //jsf_axAlert("공지사항이 존재하지 않습니다.");
            },
            beforeSend: function (xhr, settings) {
                //jsf_com_AjaxLoader(jcAJAX_BEFORESEND, 0, 0, "", joPanel);
            },
            complete: function (xhr, textStatus) {
                //jsf_com_AjaxLoader(jcAJAX_COMPLETE, 0, 0, "", joPanel);
            }
        });
    }

</script>
            
        </div>
