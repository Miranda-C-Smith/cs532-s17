        function jsf_def_IFrameResizeHeight(jpiFrameDocHeight) {
            $j("div.cont_infor").find("iframe").css("height", jpiFrameDocHeight);
        }

        function jsf_def_IFramePopup(msg, param) {
            var loc = jsf_base_GetYes24Login() + param;
            $j('#dialogAlert').jAlert({
                "msg": msg,
                'loc': loc
            });
        }

        function jsf_def_IFrameSlimPopup(url) {
            $j('#dialogPopup').jSlimPopup({ 'modal': true, 'width': 650, 'height': 310, 'url': url, 'title': '' });
        }             

