    <script type="text/javascript">
    var g_luserid = false;
    var g_loginWindow = false;
    var g_cancelbp = false;
    var g_timer = false;
    var g_reloadable = true;
    
    $("img.item").tooltip({
                          placement: "bottom"
                          });
    
    function giftItem(tuid, itemid, cancelbp){
        var h = 'auto';
        if (cancelbp) g_cancelbp = true; else g_cancelbp = false;
        if (g_luserid){
            var dlgurl = "/gearajax.php?c=sendgift&tuser=" + tuid;
            if(itemid){
                dlgurl += "&itemid=" + itemid;
            }else{
                h = 540;
            }
            showLoadingDialog();
            $.ajax({
                   url: dlgurl,
                   dataType: "html",
                   type: "GET",
                   success: function(data){
                   $("#dlgdata").html(data);
                   showDialog(h);
                   },
                   error: function (XMLHttpRequest, textStatus, errorThrown) {
                   //alert(errorThrown);
                   
                   }
                   });
        } else {
            window.location.href = '/indexloginwindow.php';
        }
    }
    
    function showItemDialog(tuid, itemid, bp){
        var durl = "/gearajax.php?c=showitem&itemid=" + itemid;
        if (tuid){
            durl += "&u=" + tuid;
        }
        if (bp){
            durl += "&bp=" + bp;
        }
        showLoadingDialog();
        $.ajax({
               url: durl,
               dataType: "html",
               type: "GET",
               success: function(data){
               $("#dlgdata").html(data);
               showDialog();
               },
               error: function (XMLHttpRequest, textStatus, errorThrown) {
               //alert(errorThrown);
               
               }
               });
    }
    
    function selectItemFrame(){
        var durl = "/gearajax.php?c=selectitemframe";
        showLoadingDialog();
        $.ajax({
               url: durl,
               dataType: "html",
               type: "GET",
               success: function(data){
               $("#dlgdata").html(data);
               showDialog();
               },
               error: function (XMLHttpRequest, textStatus, errorThrown) {
               //alert(errorThrown);
               
               }
               });
    }
    
    function selectItemStamp(){
        var durl = "/gearajax.php?c=selectitemstamp";
        showLoadingDialog();
        $.ajax({
               url: durl,
               dataType: "html",
               type: "GET",
               success: function(data){
               $("#dlgdata").html(data);
               showDialog();
               },
               error: function (XMLHttpRequest, textStatus, errorThrown) {
               //alert(errorThrown);
               
               }
               });
    }
    
    function showUserGear(tuid){
        showLoadingDialog();
        $.ajax({
               url: "/gearajax.php?c=showgear&u=" + tuid,
               dataType: "html",
               type: "GET",
               success: function(data){
               $("#dlgdata").html(data);
               showDialog();
               },
               error: function (XMLHttpRequest, textStatus, errorThrown) {
               //alert(errorThrown);
               
               }
               });
    }
    
    function showLoadingDialog(){
        $("#dlgdata").html('<p><img src="/img/loading.gif"></p>');
        $("#windowcontainer").modal();
    }
    
    function showDialog(h,title){
        if (g_timer){
            clearTimeout(g_timer);
            g_timer = false;
        }
        if (!h){
            h = 'auto';
        }else{
            //$("#windowcontainer .modal-body").css("height", h + 260);
        }
        
        $("#windowcontainer").modal();
        

		if (title){
			$("#dlg_title").text(title);
		}else{
			$("#dlg_title").text("Item");
		}

        // Height
        var height = $(window).height() - 260;
        if (height < 300) height = 300;
        $("#windowcontainer .modal-body").css("height", "" + height + "px");
        $("#windowcontainer .modal-body").css("max-height", "" + height + "px");
        /*
         $("#windowcontainer").dialog({
         minHeight: 300,
         minWidth: 556,
         zIndex: 30000,
         height: h,
         modal: true,
         buttons:{
         "Close": function() { $(this).dialog("close"); }
         }
         });
         */
    }
    
    function closeDialog(){
        $("#windowcontainer").modal('hide');
        // $("#windowcontainer").dialog("close");
    }
    
    function updateItems(){
        if ($("#itemboxcontainer").length){
                            var durl = "/gearajax.php?c=showitems&u=yuipekosama_urt";
                if (movieid){
                    durl += "&p=" + movieid;
                }
				                $.ajax({
                       url: durl,
                       dataType: "html",
                       type: "GET",
                       success: function(data){
                       $("#itemboxcontainer").html(data);
                       },
                       error: function (XMLHttpRequest, textStatus, errorThrown) {
                       //alert(errorThrown);
                       }
                       });
                    }else if (g_reloadable){
            location.reload();
        }
    }
    
    function onCloseLoginItemDialog(result, userid){
        if (g_loginWindow){
            g_loginWindow.close();
            g_loginWindow = false;
        }
        if(result){
            location.reload();
        } else {
            alert("Unable to login Twitter.");
        }
    }
    </script>
    
    

