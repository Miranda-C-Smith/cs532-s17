	<script type="text/javascript">
    var reloadon = 0;
	function onBackerButtonLoginFinished(result, userid){
		if (result) back_user('yuipekosama_urt', 1);
	}
	function show_back_user(){
		$("#back_user").show();
		$("#backing_user").hide();
		$("#back_user").unbind("click");
		$("#back_user").click(function(){
			back_user('yuipekosama_urt', 1);
			return false;
		});
	}
	function show_backing_user(){
		$("#back_user").hide();
		$("#backing_user").show();
		$("#backing_user").unbind("click");
		$("#backing_user").click(function(){

			var message = 'Are you sure you want to quit being a fan for this user?' + '(yuipekosama_urt)';
			var r =  confirm(message);
			if (r){
				back_user('yuipekosama_urt', 0);
			}
			return false;
		});
	}
	function back_user(target, mode){
        if (reloadon) {
            $("#loadingimg" + mode).show();
        }
		$.ajax({
			url: "/userajax.php?",
			data: {
								"c": "backuser",
				"m": mode,
				"u": target
			},
			dataType: "json",
			type: "POST",
			success: function(data){
				if (data){
					if (mode){
						show_backing_user();
					}else{
						show_back_user();
					}
                    if (reloadon) {
					    setTimeout('location.href = "\/yuipekosama_urt\/movie\/345350467"', 1000);
                    }
				}
			}
		});
	}
	</script>
