			if (window.performance && typeof window.performance.mark === 'function') {
				window.performance.mark(markStr);
			}
		})("mark_fully_visible")</script><script type="text/javascript">(function () {
	// TODO Get the flyout triggers by ID and bind to them
	var triggers = document.querySelectorAll('a[data-toggle="flyout"]');
	var flyoutMask = document.getElementById('flyout-mask');

	function getFlyoutByName(flyoutName) {
		return document.getElementById(flyoutName + '-flyout');
	}

	function getOpenFlyout() {
		var list = document.getElementsByClassName('flyout flyout-open');

		if (list.length) {
			return list[0];
		}

		return null;
	}

	function hideFlyout(flyoutName) {
		var flyout = flyoutName;
		if (typeof flyoutName === 'string') {
			flyout = getFlyoutByName(flyoutName);
		}

		if (flyout && flyout.classList) {
			flyout.classList.remove('flyout-open');
			flyoutMask.classList.remove('flyout-open');
			document.body.classList.remove('scroll-lock');
		}
	}

	function toggleFlyout(flyoutName) {
		var flyout = getFlyoutByName(flyoutName);

		if (flyoutName === 'close') {
			hideFlyout(getOpenFlyout());
		} else if (flyout && flyout.classList) {
			flyout.classList.toggle('flyout-open');
			flyoutMask.classList.toggle('flyout-open');
			document.body.classList.toggle('scroll-lock');
		}
	}

	function navClicked(e) {
		e.stopPropagation();
		e.preventDefault();
		var target = e.currentTarget;
		var flyoutName = target.getAttribute('data-target');

		toggleFlyout(flyoutName);
	}

	function maskClicked(e) {
		e.stopPropagation();
		e.preventDefault();

		hideFlyout(getOpenFlyout());
	}

	function bindFlyouts() {
		for (var i = 0; i < triggers.length; i++) {
			triggers[i].addEventListener('click', navClicked);
		}

		flyoutMask.addEventListener('click', maskClicked);
	}

	function unbindFlyouts() {
		for (var i = 0; i < triggers.length; i++) {
			triggers[i].removeEventListener('click', navClicked);
		}
		flyoutMask.removeEventListener('click', maskClicked);
	}

	bindFlyouts();

	window.ggFlyouts = {
		toggleFlyout: toggleFlyout,
		unbindFlyouts: unbindFlyouts
	};
