    width: 75%; /* Could be more or less, depending on screen size */
}

/* The Close Button */
.close {
    color: #aaa;
	display:block;
    font-size: 28px;
    font-weight: bold;
}

.close:hover,
.close:focus {
    color: black;
    text-decoration: none;
    cursor: pointer;
}
