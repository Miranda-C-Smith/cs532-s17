        , bCommentOpen : false
        , bIsSmr : false
        , htComment2Option : {"domain":"http://static.cbox.naver.net","apiDomain":"http://apis.naver.com/commentBox/cbox","login":false,"ticket":"channel01","templateId":"default","objectId":"1428616","language":"ko","country":"KR","pageType":"default","pageSize":20,"indexSize":5,"replyPageSize":5,"cssId":"tvcast","cssUrl":"http://tv.naver.com/resources/release/css/comment/pc_20170209060108.css","id":"cbox_module","likeItId":"1428616","help":"down","groupId":"sstv"}
