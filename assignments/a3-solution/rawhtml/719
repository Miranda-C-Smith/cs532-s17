						<script type="text/javascript">
							/* DROPDOWN */
							$(document).ready(function(){
								NavDropDownSwitches = {
									"NavDroppedDown" : false,
									"MouseOverNavItem" : false,
									"MouseOverDropDown" : false,
									"CurrentSubNav" : 0
								};

								$(".navItem").hover(function(){
									NavDropDownSwitches["MouseOverNavItem"] = true;
									NavDropDownSwitches["CurrentSubNav"] = $(this).attr("navnum");
									$(".subnav").hide();
									$(".subnav"+NavDropDownSwitches["CurrentSubNav"]).fadeIn(300);
									if(!NavDropDownSwitches["NavDroppedDown"])
										NavDropDown();
								}, function(){
									NavDropDownSwitches["MouseOverNavItem"] = false;
								});

								$("#navDropdown").hover(function(){
									NavDropDownSwitches["MouseOverDropDown"] = true;
								}, function(){
									NavDropDownSwitches["MouseOverDropDown"] = false;
								});

								function NavDropDown()
								{
									NavDropDownSwitches["NavDroppedDown"] = true;
									$("#navDropdown").fadeIn();

									setInterval(function(){
										if(!NavDropDownSwitches["MouseOverNavItem"] && !NavDropDownSwitches["MouseOverDropDown"])
										{
											setTimeout(function(){
												if(!NavDropDownSwitches["MouseOverNavItem"] && !NavDropDownSwitches["MouseOverDropDown"])
												{
													NavDropDownSwitches["NavDroppedDown"] = false;
													$("#navDropdown").fadeOut();
													return;
												}
											}, 400);
										}
									}, 100);
								}
							});
						</script>
						
