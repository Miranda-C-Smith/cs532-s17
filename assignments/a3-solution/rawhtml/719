		<script type="text/javascript">
			/*
				FOOTER SIZING
			*/
			$(window).load(function(){
				FooterOrigionalHeight = $("#footer").height();
				$(window).resize(FooterSizeFix);
				FooterSizeFix();
			});

			function FooterSizeFix(){
				var newHeight = $(window).height() - $('#footer').offset().top;

				if(newHeight > FooterOrigionalHeight)
					$("#footer").height($(window).height() - $('#footer').offset().top);

				$("#footerBGHider").height($("#footer").height());
			}
		</script>

