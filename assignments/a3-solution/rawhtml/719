				$(".mobileExpandableNav").click(function(){
					if(!$(this).next().hasClass("expanded"))
					{
						$(this).next().slideDown(function(){
							$(this).addClass("expanded");
						});
					}
					else
					{
						$(this).next().slideUp(function(){
							$(this).removeClass("expanded");
						});
					}
				});
