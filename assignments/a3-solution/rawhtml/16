<script type='text/javascript'>
$(function(){
	
	
	$(".popbox_btn").click(function(){
            
		CommentDisp('/mycollection_add/1000105843/').done(function(result) {
			var text = result;
			if(text == "notlogin"){
				location.href="/login/";
			}else{
				if(text == "success"){
					$(".pop_space p").text("お気に入りに登録しました");
					$(".popbox_btn").hide();
					$(".favAdded").show();

					$(".pop_space").fadeIn();
					$("body").append('<p class="dummy_crose"></p>');
				}else if(text == "success2"){
					$(".popbox_btn").hide();
					$(".favAdded").show();

					$(".favAdded_space").fadeIn();
					$("body").append('<p class="dummy_crose"></p>');
				}else{
					$(".pop_space p").text(text);

					$(".pop_space").fadeIn();
					$("body").append('<p class="dummy_crose"></p>');
				}


/*
				$(".popbox_btn span").text("お気に入りに登録済み");
				$(".popbox_btn").addClass("favAdded");
				$(".popbox_btn span").addClass("favAdded_link");		
  				$('a[href="javascript:void(0);"]').each(function(){
  				$(this).replaceWith($(this).html());
  				});
*/	

		
		
				$( ".dummy_crose , #spaceClose" ).unbind().click( function(){
					$(".pop_space").fadeOut( function(){
						$(".dummy_crose").remove();
					});
				});
                        }
 		});
	});

	$(".favAdded").click(function(){
		CommentDisp('/mycollection_add/1000105843/').done(function(result) {
			var text = result;
			if(text == "success" || text == "success2"){
				$(".favAdded_space").fadeIn();

				$("body").append('<p class="dummy_crose"></p>');
				$( ".dummy_crose , #spaceClose" ).unbind().click( function(){
					$(".favAdded_space").fadeOut( function(){
						$(".dummy_crose").remove();

					});
				});
			}else{
				$(".pop_space p").text(text);
				$(".pop_space").fadeIn();
				$("body").append('<p class="dummy_crose"></p>');

				$( ".dummy_crose , #spaceClose" ).unbind().click( function(){
					$(".pop_space").fadeOut( function(){
						$(".dummy_crose").remove();
					});
				});
			}
		});
	});
});

function favorite_error1(){

	$(".pop_space p").text("お気に入り商品の登録エラーが発生しました。");

	$(".pop_space").fadeIn();
	$("body").append('<p class="dummy_crose"></p>');
		
		
	$( ".dummy_crose , #spaceClose" ).unbind().click( function(){
		$(".pop_space").fadeOut( function(){
			$(".dummy_crose").remove();
		});
	});
	var element = document.getElementById("box");
	var rect = element.getBoundingClientRect();
	var positionX = rect.left + window.pageXOffset ;
	var positionY = rect.top + window.pageYOffset ;

	// 要素の位置にスクロールさせる
	window.scrollTo( positionX, positionY ) ;
}
function favorite_error2(){
	$(".pop_space p").text("お気に入り商品の登録数が上限に達しています");

	$(".pop_space").fadeIn();
	$("body").append('<p class="dummy_crose"></p>');
		
		
	$( ".dummy_crose , #spaceClose" ).unbind().click( function(){
		$(".pop_space").fadeOut( function(){
			$(".dummy_crose").remove();
		});
	});
	var element = document.getElementById("box");
	var rect = element.getBoundingClientRect();
	var positionX = rect.left + window.pageXOffset ;
	var positionY = rect.top + window.pageYOffset ;
	// 要素の位置にスクロールさせる
	window.scrollTo( positionX, positionY ) ;
}
function favorite_success1(){
	$(".popbox_btn").hide();
	$(".favAdded").show();

	$(".pop_space").fadeIn();
	$("body").append('<p class="dummy_crose"></p>');
		
	$( ".dummy_crose , #spaceClose" ).unbind().click( function(){
		$(".pop_space").fadeOut( function(){
			$(".dummy_crose").remove();
		});
	});
	var element = document.getElementById("box");
	var rect = element.getBoundingClientRect();
	var positionX = rect.left + window.pageXOffset ;
	var positionY = rect.top + window.pageYOffset ;

	// 要素の位置にスクロールさせる
	window.scrollTo( positionX, positionY ) ;
}
function favorite_success2(){
	$(".popbox_btn").hide();
	$(".favAdded").show();

	$(".favAdded_space").fadeIn();
	$("body").append('<p class="dummy_crose"></p>');

	$( ".dummy_crose , #spaceClose" ).unbind().click( function(){
		$(".favAdded_space").fadeOut( function(){
			$(".dummy_crose").remove();

		});
	});
	var element = document.getElementById("box");
	var rect = element.getBoundingClientRect();
	var positionX = rect.left + window.pageXOffset ;
	var positionY = rect.top + window.pageYOffset ;

	// 要素の位置にスクロールさせる
	window.scrollTo( positionX, positionY ) ;
}

