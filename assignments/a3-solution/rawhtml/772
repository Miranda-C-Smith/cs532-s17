<script src='//www.google.com/recaptcha/api.js?hl=en&render=explicit'></script>
<script type="text/javascript">

            var ticketForm = $('#selecttickets');
            var captchaFormId = 'captchaForm';
            var overlayContentClass = 'section.g-overlay-content';
            var captchaForm = $('#' + captchaFormId);
            var ticketSelectionsValid = false;
            var captchaCompleted = false;
            var captchaLoaded = false;
            
            ticketForm.submit(function (e) {

                // Prevent the submit/default validation behaviour.
                e.stopImmediatePropagation();

                if (ticketSelectionsValid && captchaCompleted) {
                    return true;
                }

                // Run main ticket selection validation (in /Skins/Shared/Scripts/Base/base.js)
                ticketSelectionsValid = validateTicketSelectionForm();

                if (ticketSelectionsValid) {
                    showGoogleCaptcha();
                }

                return false;
            });

            var showGoogleCaptcha = function () {

                var overlayClick = $('#overlayClick');

                overlayClick.attr('data-handler', 'contextOverlay');
                overlayClick.attr('data-target', 'captchaFormHolder');
                overlayClick.attr('data-callback', 'onModalDisplay');
                overlayClick.attr("data-overlay-type", "static");
                overlayClick.attr("data-overlay-style", "alternate");

                // Trigger a click on the 'fake' button to trigger the overlay.
                overlayClick.trigger('click');
            };


            var onModalDisplay = function () {

                if (!captchaLoaded) {
                    grecaptcha.render(captchaFormId, {
                        'sitekey': '6Ld22RIUAAAAAKuewq4IIbJIVByRYCLrRlpdEZ8F',
                        'callback': verifyCallback
                    });

                    captchaLoaded = true;
                }

                // Re-size the overlay.
			    $(overlayContentClass).css('width', 'auto');
			}

            var verifyCallback = function (response) {

                // Set timeout to allow captcha animation to finish.
                setTimeout(function () {

                    // Make sure the captcha popup goes away in case there is a validation error with the page data.
                    if (!!window.closeOverlay) {
                        window.closeOverlay();
                    }

                    $('<input type="hidden">').attr({
                        name: 'g-recaptcha-response',
                        value: response
                    }).appendTo(ticketForm);

                    captchaCompleted = true;

                    ticketForm.submit();

                }, 650);
            };
		</script>
<script type="text/javascript">
        var restPricesFrom = 0;
        var pricesFromTemplate = "Prices from <i>&#163;<priceFrom></i>";
        var canSellHotels = true;
        var anyRestaurants = false;
		var gotPrice = false;
        $(document).ready(function() {
			
			$('#hotel-prices .js-accordion-trigger').on('click',function(){

				if (anyRestaurants && !canSellHotels) {
					$("#prices-from-text").html(pricesFromTemplate.replace(/<priceFrom>/ig, restPricesFrom));
					$("#prices-from-text").show();
				}
				else if (canSellHotels && !anyRestaurants && !gotPrice) {
					$.ajax({
					    dataType: "json",
					    cache: true,
					    url: "/hotel/pricesfrom?itemCode=1065401",
					    error: function() {
					        $("#prices-from-text").hide();
					    },
					    success: function(data) {
					        gotPrice = true;
					        if (data != null) {

					            var lowestPrice = new Number();

					            if (data.pricesFrom.toString().indexOf('.') != -1)
					                lowestPrice = new Number(data.pricesFrom).toFixed(2);
					            else
					                lowestPrice = new Number(data.pricesFrom);

					            $("#prices-from-text").html(pricesFromTemplate.replace(/<priceFrom>/ig, lowestPrice)); 
					            $("#prices-from-text").fadeIn().fadeOut().fadeIn().fadeOut().fadeIn();
					        }
					    },
					    timeout: 15000
					});
				}


			});
        });
