function repostBox(e)
{var boxId=$(e).attr('data-boxid');var boxCoverImage=$(e).attr('data-boxcoverimage');$('#rePostBox_boxCoverImage').attr('src',boxCoverImage);getBoxShortUrl(boxId);}
function repostBox_save()
{var post=$('#rePostBox_textArea').val();var mediaURL=$('#rePostBox_boxCoverImage').attr('src');var postSensitiveMedia=$('#streamPostSensitiveMedia').is(':checked');$.ajax({type:"POST",url:"saveStreamPost.php",data:{post:post,mediaURL:mediaURL,postSensitiveMedia:postSensitiveMedia},success:function(data){swal("Posted","It will appear in the Stream page","success");}})}
function getBoxShortUrl(boxId)
{$('#rePostBoxModal_ProgressBar').css('display','block');var call=$.ajax({type:"POST",url:"getShortUrl.php",data:{boxId:boxId},success:function(data){$('#rePostBox_textArea').val(data);$('#rePostBoxModal_ProgressBar').css('display','none');}});}</script>

