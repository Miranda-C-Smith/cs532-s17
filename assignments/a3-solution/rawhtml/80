function impA_rotate(list) {
	var i = list.length;
	while (--i) {
		var j = Math.floor(Math.random() * (i + 1));
		if (i == j) continue;
		var k = list[i]; list[i] = list[j]; list[j] = k;
	}
	for ( i = 0; i <= list.length; i++){ if ( list[i] == "" ) list.splice(i,1);}
	return list;
}
