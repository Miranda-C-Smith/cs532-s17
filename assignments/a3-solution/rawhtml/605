<script type="text/javascript">
	var link = document.querySelector('.categories-down');

	if (window.addEventListener) {
		window.addEventListener('load', loadCategoryImagesOnDemand, false);
		link.addEventListener('mouseenter', loadCategoryImagesOnDemand, false);
	} else if (window.attachEvent)  {
		window.attachEvent('onload', loadCategoryImagesOnDemand);
		link.attachEvent('onmouseenter', loadCategoryImagesOnDemand);
	}

	function loadCategoryImagesOnDemand() {
		var images = document.querySelectorAll('.categories-image img'),
			source;

		for (i = 0; i < images.length; ++i) {
			source = images[i].getAttribute('data-src');
			if(source) {
				images[i].setAttribute("src", source);
			}
		}

		if (window.addEventListener) {
			window.removeEventListener('load', loadCategoryImagesOnDemand, false);
			link.removeEventListener('mouseenter', loadCategoryImagesOnDemand, false);
		} else if (window.attachEvent)  {
			window.detachEvent('onload', loadCategoryImagesOnDemand);
			link.detachEvent('onmouseenter', loadCategoryImagesOnDemand);
		}
	}
</script>
