	function changeLogoForHoliday() {
		var urlhash = location.hash;
		var previewHash = null;
		if (urlhash && (urlhash.indexOf('#previewLogoId=') >= 0)) {
			previewHash = urlhash.replace(/^#previewLogoId=/, '');
		}
		var logo = document.querySelector(".logo");
		var currentDate         = moment().format("YYYY-MM-DD H:mm:ss");
		var isHomepage          = '';
		var isBrazilLogo        = '';
		var isUsLogo            = '1';
		var geoLocation         = 'US';
		var domainName 			= 'US';

		var values = {
			startDate  : currentDate,
			isHomepage   : isHomepage,
			isBrazilLogo : isBrazilLogo,
			isUsLogo     : isUsLogo,
			geoLocation  : geoLocation,
			domainName 	 : domainName
		};

		var selected = -1;

		if (previewHash && previewHash.length) {
            var previewLogo = 'url(//thumbs-cdn.redtube.com/media/logos/'+previewHash+'.png) no-repeat 0 0';
			logo.style.background = previewLogo;
			return;
		}

		if (!holidays) {
			return;
		}
		
		var i;
		var found = false;
		var found2 = false;


		// If target site defined and geoCountryCode is equal with geoLocation 		
		for (i=0; i<holidays.length; i++) {
			if (found) {break;}
			var hol = holidays[i];
			var currentDate = values['startDate'];
			var startDate = hol.startDate;
			var endDate = hol.endDate;
			if (hol.platform == "pc") {
				if ((hol.targetSite != "") && (hol.targetSite.length != 2)) {
					if (hol.targetSite == values['domainName']) {
						if (hol.geoCountryCode != "") {
							for (var k=0; k < hol.geoCountryCode.length; k++) {
								if (hol.geoCountryCode[k] == values['geoLocation']) {
									if ((currentDate >= startDate) && (currentDate <= endDate)) {
										found = true;
										break;
									}
								}
							}			
						}
					}
				}				
			}
		}

		// If target site is defined and geoCountryCode is not defined
		if (!found) {		
			for (i=0; i<holidays.length; i++) {
				if (found) {break;}
				var hol = holidays[i];
				var currentDate = values['startDate'];
				var startDate = hol.startDate;
				var endDate = hol.endDate;
				if (hol.platform == "pc") {
					if ((hol.targetSite != "") && (hol.targetSite.length != 2)) {
