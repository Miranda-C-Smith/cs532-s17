<script type="text/javascript">
    //--------------- SR: Click Events related functionality ------------------------------------------

    $(document).ready(function () {
        $("input[name='signup-remember']").prop( "checked", true );
        $("input[name='remember']").prop( "checked", true );
        $(".open-login-dialog").fancybox({
            fitToView: true,
            autoSize: true,
            height: 'auto',
            maxWidth: 280,
            scrolling: 'auto',
            hideOnOverlayClick : false
        });
        $('.signup-with-email').on('click', function(){
            $('.signup-fields').removeClass('hide');
            $('#signup-btn').removeClass('hide');
            $('.email-btn').addClass('hide');
            $('#email-exist-alert').addClass('hide');
            $('#email-exist-social-alert').addClass('hide');
            $('#forgot-email-success-alert').addClass('hide');
            $('#forgot-email-failure-alert').addClass('hide');
            $('#forgot-email-social-alert').addClass('hide');
            $.fancybox.update();
        });
        $('.show-signup').on('click', function(){
            $("#login-signup-dialog").removeClass('hide');
            $('.has-error').text('');
            $('.email-btn').removeClass('hide');
            $('#signup-btn').addClass('hide');
            $('.signup-form').removeClass('hide');
            $('.signup-fields').addClass('hide');
            $('#cpass-field').addClass('hide-this');
            $('.signin-form').addClass('hide');
            $('.previous-logins-form').addClass('hide');
            $('.forgot-form').addClass('hide');
            $('#email-exist-alert').addClass('hide');
            $('#email-exist-social-alert').addClass('hide');
            $('#forgot-email-success-alert').addClass('hide');
            $('#forgot-email-failure-alert').addClass('hide');
            $('#forgot-email-social-alert').addClass('hide');
            $.fancybox.update();
        });
        $('.show-signin').on('click', function(){
            if($('.previous-logins-form').children().hasClass('local-storage-thumbnail')){
                $('.previous-logins-form').removeClass('hide');
                $('.signin-form').addClass('hide');
            }
            else{
                $('.previous-logins-form').addClass('hide');
                $('.signin-form').removeClass('hide');
            }
            $("#login-signup-dialog").removeClass('hide');
            $('.signup-form').addClass('hide');
            $('.forgot-form').addClass('hide');
            $('#email-exist-alert').addClass('hide');
            $('#email-exist-social-alert').addClass('hide');
            $('#forgot-email-success-alert').addClass('hide');
            $('#forgot-email-failure-alert').addClass('hide');
            $('#forgot-email-social-alert').addClass('hide');
            $.fancybox.update();
        });
        $('.show-forgot').on('click', function(){
            $('.previous-logins-form').addClass('hide');
            $('.signup-form').addClass('hide');
            $('.signin-form').addClass('hide');
            $('.forgot-form').removeClass('hide');
            $('#email-exist-alert').addClass('hide');
            $('#email-exist-social-alert').addClass('hide');
            $('#forgot-email-success-alert').addClass('hide');
            $('#forgot-email-failure-alert').addClass('hide');
            $('#forgot-email-social-alert').addClass('hide');
            $.fancybox.update();
        });
        $('.login-as-another-user').on('click', function(){
            $('.previous-logins-form').addClass('hide');
            $('.signin-form').removeClass('hide');
            $("#login-signup-dialog").removeClass('hide');
            $('.signup-form').addClass('hide');
            $('.forgot-form').addClass('hide');
            $('#email-exist-alert').addClass('hide');
            $('#email-exist-social-alert').addClass('hide');
            $('#forgot-email-success-alert').addClass('hide');
            $('#forgot-email-failure-alert').addClass('hide');
            $('#forgot-email-social-alert').addClass('hide');
            $.fancybox.update();
        });
    });
    //--------------- END Click Events related functionality ------------------------------------------

    //--------------- SR: local storage related functionality ------------------------------------------
        if(window.localStorage && window.localStorage.getItem('user_data')){
        var getLocalData = JSON.parse(window.localStorage.getItem('user_data'));
        for(var i=0; i < getLocalData.length; i++){
            var html = "<a class='local-storage-thumbnail' user-id='"+ getLocalData[i].user_id +"'>" +
                "<span class='fa fa-close'></span>" +
                "<img src='"+ getLocalData[i].picture +"'>" +
                "<span class='label-username'>"+ getLocalData[i].username +"</span>" +
                "<span class='label-email'>"+ getLocalData[i].email +"</span>" +
                "</a>";
            $(".login-as-another-user").before(html);
        }
    }
        if(window.localStorage){
        $(document).on('click', '.local-storage-thumbnail .fa-close', function(){
            var user_id = $(this).parent().attr('user-id');
            var localData = JSON.parse(window.localStorage.getItem('user_data'));
            for(var i=0; i < localData.length; i++){
                if(localData[i].user_id == user_id)
                    localData.splice(i);
            }
            window.localStorage.setItem('user_data', JSON.stringify(localData));

            if($(this).parent().parent().hasClass('previous-logins-form-view')){
                $('.local-storage-thumbnail[user-id="'+user_id+'"]').remove();
                if(!$('.previous-logins-form-view').children().hasClass('local-storage-thumbnail'))
                    $(".login-as-another-user-form").trigger('click');
            }
            else{
                $('.local-storage-thumbnail[user-id="'+user_id+'"]').remove();
                if(!$('.previous-logins-form').children().hasClass('local-storage-thumbnail')){
                    $(".login-as-another-user-form").trigger('click');
                    $(".login-as-another-user").trigger('click');
                }
            }
            $.fancybox.update();
            return false;
        });
        $(document).on('click', '.local-storage-thumbnail', function(){
            var user_id = $(this).attr('user-id');
            var localData = JSON.parse(window.localStorage.getItem('user_data'));
            var is_remember = false;
            for(var i=0; i < localData.length; i++){
                if(localData[i].user_id == user_id){
                    if(localData[i].channel == "Email"){
                        if(localData[i].email == $.cookie('email') && localData[i].pwd){
                            e_auth(localData[i].email, localData[i].pwd)
                            is_remember = true;
                            break;
                        }
                    }
                    else if(localData[i].channel == "Facebook"){
                        is_remember = true;
                        window.location.href = $('#login-btn-facebook').attr('href');
                    }
                    else if(localData[i].channel == "Google"){
                        is_remember = true;
                        window.location.href = $('#login-btn-google').attr('href');
                    }
                }
            }
            if(!is_remember){
                if($(this).parent().hasClass('previous-logins-form-view')){
                    $("input[name='signin-email-form']").val($(this).children('.label-email').text());
                    $(".login-as-another-user-form").trigger('click');
                }
                else{
                    $("input[name='signin-email']").val($(this).children('.label-email').text());
                    $(".login-as-another-user").trigger('click');
                }
            }
        });
    }
    //--------------- END local storage related functionality ------------------------------------------

    //--------------- SR: forgot password and password strength related functionality ------------------

    $("input[name='signin-email']").on('input', function (e) {
        $("input[name='forgot-email']").val($("input[name='signin-email']").val());
    });
    $("input[name='signup-email']").on('input', function (e) {
        $("input[name='forgot-email']").val($("input[name='signup-email']").val());
        $("input[name='signin-email']").val($("input[name='signup-email']").val());
    });

    $("#password").on('input', function (e) {
        $('#result').html(checkStrength($("#password").val()));
    });

    function checkStrength(password) {
        //initial strength
        var strength = 0
        //if the password length is less than 6, return message.
        if (password.length < 6 && password.length > 0) {
            $('#result').removeClass();
            $('#result').addClass('short');
            $('#cpass-field').removeClass('hide-this');
            $.fancybox.update();
            return 'Your password is too short.';
        }

        if(password.length == 0){
            $('#result').removeClass();
            $('#cpass-field').addClass('hide-this');
            $.fancybox.update();
        }
        else{
            $('#result').removeClass();
            $('#cpass-field').removeClass('hide-this');
            $.fancybox.update();
        }


        //length is ok, lets continue.

        //if length is 8 characters or more, increase strength value
        if (password.length > 5) strength += 1

        //if password contains both lower and uppercase characters, increase strength value
        if (password.match(/([a-z].*[A-Z])|([A-Z].*[a-z])/))  strength += 1

        //if it has numbers and characters, increase strength value
        if (password.match(/([a-zA-Z])/) && password.match(/([0-9])/))  strength += 1

        //if it has one special character, increase strength value
        if (password.match(/([!,%,&,@,#,$,^,*,?,_,~])/))  strength += 1

        //if it has two special characters, increase strength value
        if (password.match(/(.*[!,%,&,@,#,$,^,*,?,_,~].*[!,%,&,@,#,$,^,*,?,_,~])/)) strength += 1

        //now we have calculated strength value, we can return messages

        //if value is less than 2
        if (strength == 0) {
            $('#result').removeClass()
            return ''
        }
        else if (strength == 1) {
            $('#result').removeClass()
            $('#result').addClass('weak')
            return 'Your password is weak.'
        }
        else if (strength == 2) {
            $('#result').removeClass()
            $('#result').addClass('good')
            return 'Your password is good.'
        }
        else {
            $('#result').removeClass()
            $('#result').addClass('strong')
            return 'Your password is strong.'
        }
    }

    function validateEmail(email) {
        var re = /^(([^<>()[\]\\.,;:\s@\"]+(\.[^<>()[\]\\.,;:\s@\"]+)*)|(\".+\"))@((\[[0-9]{1,3}\.[0-9]{1,3}\.[0-9]{1,3}\.[0-9]{1,3}\])|(([a-zA-Z\-0-9]+\.)+[a-zA-Z]{2,}))$/;
        return re.test(email);
    }
    //--------------- END forgot password and password strength related functionality ------------------

    //--------------- SR: Signin / Signup / Forgot password related functionality ----------------------

    var is_verified = true;

    function sign_in() {
        var signin_email = $("input[name='signin-email']").val();
        var signin_password = $("input[name='signin-password']").val();
        is_verified = true;
        if(signin_email.length == 0 || !validateEmail(signin_email)){
            $('#signin-email').text('Your valid email is required.');
            is_verified = false;
        }
        else
            $('#signin-email').text('');
        if(signin_password.length == 0){
            $('#signin-password').text('Password is required.');
            is_verified = false;
        }
        else
            $('#signin-password').text('');
        $.fancybox.update();
        if(is_verified) {
            if($("input[name='remember']").prop('checked')){
                $.cookie('email', signin_email, { expires: 3650, path: '/' });
            }
            e_auth(signin_email, md5(signin_password));
        }
    }

    function e_auth(email, password){
        $.ajax({
            type: "POST",
            dataType: 'json',
            url: 'https://swaggable.com/auth/e_auth',
            data: {email: email, pwd: password},
            success: function (obj) {
                if(obj.status == "success")
                    window.location = obj.data;
                else
                    $.pnotify(obj.message);
            }
        });
    }

    function sign_up(){
        var signup_email = $("input[name='signup-email']").val();
        var signup_password = $("input[name='signup-password']").val();
        var confirm_password = $("input[name='signup-cpassword']").val();
        is_verified = true;
        if(signup_email.length == 0 || !validateEmail(signup_email)){
            $('#signup-email').text('Your valid email is required.');
            is_verified = false;
        }
        else
            $('#signup-email').text('');
        if(signup_password.length < 6){
            $('#signup-password').text('Password is required with minimum length of 6.');
            is_verified = false;
        }
        else
            $('#signup-password').text('');
        if(confirm_password != signup_password){
            $('#signup-cpassword').text('Make sure both fields match');
            is_verified = false;
        }
        else
            $('#signup-cpassword').text('');
        $.fancybox.update();
        if(is_verified) {
            if($("input[name='signup-remember']").prop('checked')){
                $.cookie('email', signup_email, { expires: 3650, path: '/' });
            }
            $.ajax({
                type: "POST",
                dataType: 'json',
                url: 'https://swaggable.com/auth/e_signup',
                data: { email: signup_email, pwd: md5(signup_password)},
                success: function (obj) {
                    if(obj.status == "success")
                        window.location = obj.data;
                    else if(obj.message == "email-exists") {
                        $('#email-exist-alert').removeClass('hide');
                        $('#email-exist-social-alert').addClass('hide');
                        $.fancybox.update();
                    }else if(obj.message == "email-exists-against-social-channel") {
                        $('#email-exist-social-alert').removeClass('hide');
                        $('#email-exist-alert').addClass('hide');
                        $.fancybox.update();
                    }
                    else
                        $.pnotify(obj.message);
                }
            });
        }
    }

    function forgot_password(){
        var signin_email = $("input[name='forgot-email']").val();
        is_verified = true;
        if(signin_email.length == 0 || !validateEmail(signin_email)){
            $('#forgot-email').text('Your valid email is required.');
            is_verified = false;
        }
        else
            $('#forgot-email').text('');
        $.fancybox.update();
        if(is_verified) {
            $.ajax({
                type: "POST",
                dataType: 'json',
                url: 'https://swaggable.com/auth/e_forgot',
                data: {email: signin_email},
                success: function (obj) {
                    if(obj.status == "success"){
                        $('#forgot-email-success-alert').removeClass('hide');
                        $('#forgot-email-failure-alert').addClass('hide');
                        $('#forgot-email-social-alert').addClass('hide');
                        $.fancybox.update();
                    } else if(obj.message == "email-not-found"){
                        $('#forgot-email-success-alert').addClass('hide');
                        $('#forgot-email-failure-alert').removeClass('hide');
                        $('#forgot-email-social-alert').addClass('hide');
                        $.fancybox.update();
                    }
                    else if(obj.message == "email-exists-against-social-channel"){
                        $('#forgot-email-success-alert').addClass('hide');
                        $('#forgot-email-failure-alert').addClass('hide');
                        $('#forgot-email-social-alert').removeClass('hide');
                        $.fancybox.update();
                    }
                    else{
                        $('#forgot-email-success-alert').addClass('hide');
                        $('#forgot-email-failure-alert').addClass('hide');
                        $('#forgot-email-social-alert').addClass('hide');
                        $.fancybox.update();
                        $.pnotify(obj.message);
                    }
                }
            });
        }
    }
    //--------------- END Signin / Signup / Forgot password related functionality ----------------------
</script>
<script type="text/javascript">

            // Hide Footer on on scroll down
        var didScroll;
        var lastScrollTop = 0;
        var delta = 5;
        var navbarHeight = $('.join-banner').outerHeight();

        $(window).scroll(function(event){
            didScroll = true;
        });

        setInterval(function() {
            if (didScroll) {
                hasScrolled();
                didScroll = false;
            }
        }, 250);

        function hasScrolled() {
            var st = $(this).scrollTop();

            // Make sure they scroll more than delta
            if(Math.abs(lastScrollTop - st) <= delta)
                return;

            // If they scrolled down and are past the navbar, add class .nav-up.
            // This is necessary so you never see what is "behind" the navbar.
            if (st > lastScrollTop && st > navbarHeight){
                // Scroll Down
                $('.join-banner').removeClass('nav-down-footer').addClass('nav-up-footer');
            } else {
                // Scroll Up
                if(st + $(window).height() < $(document).height()) {
                    $('.join-banner').removeClass('nav-up-footer').addClass('nav-down-footer');
                }
            }

            lastScrollTop = st;
        }
    
    $(document).on('click', '.cart-checkout', function(){
        $.fancybox.close();
    });
    $(document).on('click','.suggested-product-close',function(){
        $(this).parent().remove();
        var remaining_thumbnails = [];
        $.each($('.sidebar-thumbnail'), function( index, value ) {
            remaining_thumbnails.push($(this).attr('product-id'));
        });
        var product_id = $(this).parent().attr('product-id');
        var slug = window.location.href.substring(window.location.href.lastIndexOf('/') + 1);
        $.ajax({
            type: "POST",
            dataType: 'json',
            url: 'https://swaggable.com/set_data/mark_suggested_product',
            data: {
                product_id: product_id,
                slug: slug,
                remaining_thumbnails : remaining_thumbnails
            },
            success: function (resp) {
                if (resp.status == "success") {
                    $("#sidebar-suggested-products").append(resp.data.html);
                } else {
                    if(!$("#sidebar-suggested-products").children().hasClass('thumbnail')){
                        $('.sidebar-suggested-products-div').addClass('hide-me');
                    }
                    if(!$("#sidebar-suggested-products").children().hasClass('thumbnail') && (!$(".sidebar-questions-div").children().is("#sidebar-questions") || $(".sidebar-questions-div").hasClass("hide-me"))){
                        $(".visible-desktop").addClass("hide-me");
                        $(".container").css("margin-right", "auto");
                        $(".container").css("width", "auto");
                    }
                }
            }
        });
    });
    $(document).on('click','.popup-close-btn',function(){
        $(this).parent().hide("fade", 1000);
    });
    $(document).ready(function () {
        $('.search-btn').on('click', function (){
            $(this).addClass('hidden');
            $('.mobile-logo').addClass('hidden');
            $('#mobile-search-wrapper').removeClass('hidden');
            $('.site-search-mobile').focus();
        });
        $(".open-cart-popup").on('click', function(){
            $(".open-cart-popup").fancybox({
                fitToView: true,
                width: '450',
                height: 'auto',
                autoSize: false,
                closeClick: false,
                closeEffect: 'none',
                scrolling: 'no'
            });
        });
        $("#amazon-cart-div").slimscroll({
            height: '300px',
            railVisible: true
        });

        $("#coming-from").val('consumer');
        // Marking the visible set of notifications as read when the header button is clicked
        $("#clear-notifications").on('click', function (e) {
            $('#loading-icon').show();
            var notifications = [];
            $(".read-notification").each(function (index) {
                notifications.push($(this).val());
            });
            $.ajax({
                type: "POST",
                dataType: 'json',
                url: 'https://swaggable.com/set_data/mark_as_read',
                data: {notification_id: JSON.stringify(notifications)},
                success: function (obj) {
                    if (obj != 0) {
                        $("#notifications-count").text(obj);
                        $('#loading-icon').hide();
                        location.reload();
                    } else {
                        $("#notifications-count").hide();
                        $('#loading-icon').hide();
                    }
                }
            });

        });

        $("#clear-coupon").on('click', function (e) {
            $('#loading-icon').show();
            var notifications = [];
            $(".read-coupon").each(function (index) {
                notifications.push($(this).val());
            });
            $.ajax({
                type: "POST",
                dataType: 'json',
                url: 'https://swaggable.com/set_data/mark_as_read_coupon',
                data: {coupon_codes: JSON.stringify(notifications)},
                success: function (obj) {
                    if (obj != 0) {
                        $("#coupon-notifications-count").text(obj);
                        $('#loading-icon').hide();
                        location.reload();
                    } else {
                        $("#coupons").hide();
                        $('#loading-icon').hide();
                    }
                }
            });
        });

        $('.close-help-popup').on('click', function(){
            $('#help-popup').addClass('hide');
        });

        $(".help-sticky").on('click', function(){
            $('#help-popup').removeClass('hide');
            if($("#is-page-has-tour").hasClass("has-tour")){
                $('.tour-container').removeClass('hide');
            }
            else{
                $('.tour-container').addClass('hide');
            }
        });

        $('#start-tour').on('click', function(){
            if($("#is-page-has-tour").hasClass("has-tour")){
                $('#help-popup').addClass('hide');
                tour.restart();
                tour.start();
            }
            else{
            }
        });
    });

    function logout_now(){
        $.ajax({
            type: "POST",
            dataType: 'json',
            url: 'https://swaggable.com/main/clear_session',
            data: {},
            success: function (obj) {
                if(obj.status == "success"){
                    window.location = obj.data;
                } else {
                    $.pnotify(obj.message);
                }
            }
        });
    }

    function hide_coupon(coupon_code) {
        var notifications = [];
        notifications.push(coupon_code);
        $.ajax({
            type:"POST",
            url:'https://swaggable.com/index.php/set_data/mark_as_read_coupon',
            data:{coupon_codes: JSON.stringify(notifications)},
            success:function (html) {
                $(".coupon-div-"+coupon_code).hide();
                $("#coupon-notifications-count").text(html);
                if(html == 0){
                    window.location.reload();
                }
            }
        });
    }

    function saveAnswerChoice(question_id, answer, type) {
        if($('#question-section').children().hasClass('questions-tip')){
            $('.questions-tip').addClass('hide');
        }
        $('#saving-icon').removeClass('hide');

        if(!answer || answer.length < 1){
            $.pnotify("Please select an option and press submit.");
            $('#saving-icon').addClass('hide');
            return false;
        }
        $.ajax({
            type:"POST",
            dataType:'json',
            url:'https://swaggable.com/want/save_answer_choice/',
            data:{ question_id:question_id, answer:answer, type: type},
            success:function (html) {
                if (html.status == "failed") {
                    $.pnotify(html.message);
                    $('#saving-icon').addClass('hide');
                }
                else {
                    if(type != 'question') {
                        if(html.data.question){
                            //hide sidebar if no questions else load new
                            $("#sidebar-questions").html(html.data.view);
                            $('#saving-icon').addClass('hide');
                            return 1;
                        } else {
                            $.pnotify(html.message);
                            $(".sidebar-questions-div").addClass("hide-me");
                            $('#saving-icon').addClass('hide');
                            if(!$("#sidebar-suggested-products").children().hasClass('thumbnail') && (!$(".sidebar-questions-div").children().is("#sidebar-questions") || $(".sidebar-questions-div").hasClass("hide-me"))){
                                $(".visible-desktop").addClass("hide-me");
                                $(".container").css("margin-right", "auto");
                                $('.main-header-inner').attr('style', 'margin-right: auto !important; max-width: 1020px;    left: 0; margin: auto; right: 0; ');
                                $(".container").css("width", "auto");
                                $(".container").css("width", "auto");
                                
                            }
                            return 1;
                        }
                    }
                    if(html.data.question){
                        $("#question_div").html(html.data.view);
                        $("#progress_num").html(html.data.question.percentage_done+'%');
                        $("#progress_bar").css("width", html.data.question.percentage_done+'%');
                        $('#saving-icon').addClass('hide');
                    } else {
                        $("#question-div").hide();
                        $("#progress_num").html('100%');
                        $("#progress_bar").css("width", '100%');
                        $.pnotify(html.message);
                        $('#saving-icon').addClass('hide');
                        $("#question_div").html('<h3 class="heading" style="font-size: 16px; margin: 0; text-align: center;">No more Questions</h3>');
                        $("#profile-status-message").addClass('hide');
                        if ($('div#question-section-popup').length){
                            $.fancybox.close()
                        }
                    }
                }
            }
        });
    }

    function skip(question_id, type) {
        if($('#question-section').children().hasClass('questions-tip')){
            $('.questions-tip').addClass('hide');
        }
        $('#saving-icon').removeClass('hide');

        $.ajax({
            type:"POST",
            dataType:'json',
            url:'https://swaggable.com/want/skip_answer/',
            data:{question_id:question_id, type:type},
            success:function (html) {
                if (html.status == "failed") {
                    $.pnotify(html.message);
                    $('#saving-icon').addClass('hide');
                }
                else {
                    if(type != 'question') {
                        if(html.data.question){
                            //hide sidebar if no questions else load new
                            $("#sidebar-questions").html(html.data.view);
                            $('#saving-icon').addClass('hide');
                            return 1;
                        } else {
                            $.pnotify(html.message);
                            $(".sidebar-questions-div").addClass("hide-me");
                            $('#saving-icon').addClass('hide');
                            if(!$("#sidebar-suggested-products").children().hasClass('thumbnail') && (!$(".sidebar-questions-div").children().is("#sidebar-questions") || $(".sidebar-questions-div").hasClass("hide-me"))){
                                $(".visible-desktop").addClass("hide-me");
                                $(".container").css("margin-right", "auto");
                                $(".container").css("width", "auto");
                                $('.main-header-inner').attr('style', 'margin-right: auto !important; max-width: 1020px;    left: 0; margin: auto; right: 0; ');

                            }
                            return 1;
                        }
                    }
                    if(html.data.question){
                        $("#question_div").html(html.data.view);
                        $("#progress_num").html(html.data.question.percentage_done+'%');
                        $("#progress_bar").css("width", html.data.question.percentage_done+'%');
                        $('#saving-icon').addClass('hide');
                    } else {
                        $("#question-div").hide();
                        $("#progress_num").html('100%');
                        $("#progress_bar").css("width", '100%');
                        $.pnotify(html.message);
                        $('#saving-icon').addClass('hide');
                        $("#question_div").html('<h3 class="heading" style="font-size: 16px; margin: 0; text-align: center;">No more Questions</h3>');
                        $("#profile-status-message").addClass('hide');
                        if ($('div#question-section-popup').length){
                            $.fancybox.close()
                        }
                    }
                }
            }
        });
    }

    function is_valid_email(email){
        var re = /^(([^<>()[\]\\.,;:\s@\"]+(\.[^<>()[\]\\.,;:\s@\"]+)*)|(\".+\"))@((\[[0-9]{1,3}\.[0-9]{1,3}\.[0-9]{1,3}\.[0-9]{1,3}\])|(([a-zA-Z\-0-9]+\.)+[a-zA-Z]{2,}))$/;
        return re.test(email);
    }

    function saveAnswerMultiple(question_id, answers, type) {
        if($('#question-section').children().hasClass('questions-tip')){
            $('.questions-tip').addClass('hide');
        }
        $('#saving-icon').removeClass('hide');

        if(!answers || answers.length < 1){
            $.pnotify("Please select an option and press submit.");
            $('#saving-icon').addClass('hide');
            return false;
        }

        $.ajax({
            type:"POST",
            dataType:'json',
            url:'https://swaggable.com/want/save_answer_multiple/',
            data:{ question_id:question_id, answer:answers, type:type },
            success:function (html) {
                if (html.status == "failed") {
                    $.pnotify(html.message);
                    $('#saving-icon').addClass('hide');
                }
                else {
                    if(type != 'question') {
                        if(html.data.question){
                            //hide sidebar if no questions else load new
                            $("#sidebar-questions").html(html.data.view);
                            $('#saving-icon').addClass('hide');
                            return 1;
                        } else {
                            $.pnotify(html.message);
                            $(".sidebar-questions-div").addClass("hide-me");
                            $('#saving-icon').addClass('hide');
                            if(!$("#sidebar-suggested-products").children().hasClass('thumbnail') && (!$(".sidebar-questions-div").children().is("#sidebar-questions") || $(".sidebar-questions-div").hasClass("hide-me"))){
                                $(".visible-desktop").addClass("hide-me");
                                $(".container").css("margin-right", "auto");
                                $(".container").css("width", "auto");
                            }
                            return 1;
                        }
                    }
                    if(html.data.question){
                        $("#question_div").html(html.data.view);
                        $("#progress_num").html(html.data.question.percentage_done+'%');
                        $("#progress_bar").css("width", html.data.question.percentage_done+'%');
                        $('#saving-icon').addClass('hide');
                    } else {
                        $("#question-div").hide();
                        $("#progress_num").html('100%');
                        $("#progress_bar").css("width", '100%');
                        $.pnotify(html.message);
                        $('#saving-icon').addClass('hide');
                        $("#question_div").html('<h3 class="heading" style="font-size: 16px; margin: 0; text-align: center;">No more Questions</h3>');
                        $("#profile-status-message").addClass('hide');
                        if ($('div#question-section-popup').length){
                            $.fancybox.close()
                        }
                    }
                }
            }
        });
    }

    //  ----------------------  SR: update cart  ---------------------------
    function update_amazon_cart(user_cart){
        if(user_cart != null){
            $('.wallet-purchase-url').removeClass('disabled-link');
            $('.wallet-purchase-url').addClass('btn btn-cart');
            $('.wallet-purchase-url').html('<span><i class="fa fa-amazon" style="font-size: 13px; position: inherit;"></i> </span> Go to Store');
            $('.wallet-purchase-url').attr({
                href: user_cart.purchase_url,
                target: '_blank'
            });
            $('.open-cart-popup').removeClass('disabled-link');
            $('.open-cart-popup').text(user_cart.cart_items.length + " Item(s)");
            var html = "";
            for(var i=0; i<user_cart.cart_items.length; i++){
                html += '<div class="row-fluid cart-item"><div class="cart-img pull-left">'+
                    '<img src="'+ user_cart.cart_items[i].image +'" /></div>'+
                    '<p>'+ user_cart.cart_items[i].quantity +' <span>x</span> '+ user_cart.cart_items[i].title +'</p></div>';
            }
            $('#amazon-cart-div').html(html);
            $('.cart-checkout').attr({
                href: user_cart.purchase_url,
                target: '_blank'
            });
            $('.btn-cart').attr({
                href: user_cart.purchase_url,
                target: '_blank'
            });
        }
        else{
            $('.wallet-purchase-url').addClass('disabled-link');
            $('.wallet-purchase-url').removeAttr('href');
            $('.open-cart-popup').addClass('disabled-link');
            $('.open-cart-popup').text("0 Item(s)");
            $('#amazon-cart-div').html("");
            $('.cart-checkout').removeAttr('href');
        }
    }

</script>
