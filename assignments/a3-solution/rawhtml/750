    function when_external_loaded(callback) {
        if (typeof timeago === 'undefined' || typeof $.fn.raty === 'undefined') {
            setTimeout(function () {
                when_external_loaded(callback);
            }, 100); // wait 100 ms
        } else {
            callback();
        }
    }

