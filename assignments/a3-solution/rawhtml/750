<script type="text/javascript">

    $(document).ready(function () {
                $('#product-review-popularity').addClass('active');
            });

    var $container = $('#reviews');
    $container.infinitescroll({
            navSelector: '#page-nav',    // selector for the paged navigation
            nextSelector: '#page-nav a',  // selector for the NEXT link (to page 2)
            itemSelector: '.reviews',     // selector for all items you'll retrieve
            debug: false,
            loading: {
                finishedMsg: 'No more reviews to load.',
                msgText: 'Loading more reviews...',
                img: 'https://i.imgur.com/6RMhx.gif'
            }
        },
        function (newElements) {
            // hide new items while they are loading
            var $newElems = $(newElements).css({ opacity: 0 });
            $container.append(newElements);
            $(newElements).css({ opacity: 1 });
            // ensure that images load before adding to masonry layout
            jQuery("abbr.timeago").timeago();
            $('.star').each(function () {
                var current_rating = $(this).attr("value");
                $(this).raty({ readOnly: true, score: current_rating });
            });
            $('.my-tooltip').tooltipster({
                interactive: true,
                animation: 'grow'
            });

            $('.votes a').unbind('click');
            $('.votes a').each(function () {
                if ($.trim($(this).html()) != '0')
                {
                    $(this).fancybox();
                }
            });
            $('.votes a').on('click',function() {
                $('#voters-names').empty();
                get_review_id(this);
            });
        }
    );
</script><style>

