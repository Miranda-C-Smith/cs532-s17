<script type="text/javascript">
    var visited_login_snippet = false;
    var res_test;
    var clicked = 0;
    var fb_loaded = false;
    var alt_fb_method = false;
    setTimeout(is_fb_alt, 5000);
    window.fbAsyncInit = function () {
        FB.init({ appId:  223829680964591, channelUrl: 'https://swaggable.com//channel.php', status: true, cookie: true, xfbml: true, oauth: true});

                /* All the events registered */
        FB.Event.subscribe('auth.login', function (response) {
            if(!visited_login_snippet){
                //loginClicked();
            }
        });
        FB.Event.subscribe('auth.logout', function (response) {
            return false;
        });
                fb_loaded = true;
        alt_fb_method = false;
        if(clicked) {
            loginClicked();
        }
    };

    // Load the SDK Asynchronously
    (function (d) {
        var js, id = 'facebook-jssdk', ref = d.getElementsByTagName('script')[0];
        if (d.getElementById(id)) {
            return;
        }
        js = d.createElement('script');
        js.id = id;
        js.async = true;
        js.src = "//connect.facebook.net/en_US/all.js";
        ref.parentNode.insertBefore(js, ref);
    }(document));

        var get_permissions = false;
    var scope = 'email,user_work_history,user_education_history,friends_work_history,friends_education_history,friends_location,user_location,user_hometown,user_birthday,friends_birthday,user_likes,offline_access';
        function isEmpty(map) {
        for(var key in map) {
            if (map.hasOwnProperty(key)) {
                return false;
            }
        }
        return true;
    }

    function is_fb_alt() {
        if(fb_loaded) {
            alt_fb_method = false;
            return false;
        } else {
            alt_fb_method = true;
            return true;
        }
    }

    function fb_login_snippet( called_from, arg1 ) {
        if (typeof(called_from) === 'undefined') called_from = '';
        if (typeof(arg1) === 'undefined') arg1 = '';
        visited_login_snippet = true;
        if(navigator.userAgent.match('CriOS') || alt_fb_method) {
            var source_page = window.btoa(window.location.toString()); //base64 encoded.
            source_page = source_page.replace("#", "");
            source_page = source_page.split("=").join("");
            source_page = source_page.split("/").join("_");
            source_page = source_page.split("+").join("-");
            var redirect_url_for_dialog = 'https://swaggable.com/main/exchange_code/'+source_page;
            window.location.replace('https://www.facebook.com/dialog/oauth?client_id=223829680964591&redirect_uri=' + redirect_url_for_dialog, '', null);
        }
        else {
            FB.login(function (response) {
                if (isEmpty(response) || isEmpty(response.authResponse)) {

                } else {
                    var locat = curr_window_locat();
                    //console.log("locat:   " + locat);

                    //------------- SR: code for get controller name from source page link and redirection accordingly ---------------//

                                        //   console.log("main");
                    link = 'https://swaggable.com/main/update_auth';
                                        //anchor parts are removed by browser before sending a request to server, they are added as a parameter to the get request
                    if (window.location.hash) {
                        link = link + "?anchorpart=" + window.location.hash.substring(1) + "&auth_token=" + response.authResponse.accessToken + "&source_page=" + locat;
                    }
                    else {

                        link = link + "?auth_token=" + response.authResponse.accessToken + "&source_page=" + locat;
                    }

                    if (get_permissions == false) {
                        setTimeout(window.location.replace(link), 50);
                    }
                    else {
                        link = 'https://swaggable.com/main/update_auth_after_publish_actions';
                        link += '?auth_token=' + response.authResponse.accessToken;

                        call_upadte_auth(called_from, arg1, link);

                    }
                }
            }, {scope: scope});
        }
    }

    function curr_window_locat() {
        var locat = window.location;
        locat = locat.toString();
        if (locat.substr(-1, 1) == "/") {
            locat = locat.slice(0, -1);
        }
        locat = locat.replace("&", "%26");
        locat = locat.replace("?", "%3F");
        locat = locat.replace("#", "%23");
        return locat;
    }

    function test_permission() {
        FB.getLoginStatus(function(response) {
            if (response.status === 'connected') {
                // the user is logged in and has authenticated your
                // app, and response.authResponse supplies
                // the user's ID, a valid access token, a signed
                // request, and the time the access token
                // and signed request each expire
                var uid = response.authResponse.userID;
                var accessToken = response.authResponse.accessToken;
            } else if (response.status === 'not_authorized') {
                // the user is logged in to Facebook,
                // but has not authenticated your app
            } else {
                // the user isn't logged in to Facebook.
            }
        });
    }


    function loginClicked( called_from, arg1 ) {
        clicked++;
        if(clicked > 3){
            alt_fb_method = true;
        }
        if(fb_loaded || alt_fb_method) {
            if (typeof(called_from) === 'undefined') called_from = '';
            if (typeof(arg1) === 'undefined') arg1 = '';
            fb_login_snippet( called_from, arg1 );
        }
    }

    function call_upadte_auth(called_from,arg1, link)
    {
        $.ajax({
            type: 'GET',
            dataType: 'HTML',
            url: link,
            success: function ( msg) {
                window[called_from]( arg1 );

            }
        });
    }

</script>

