    function share_this_callback(event, service, product_id, type) {
        $.ajax({
            type: "POST",
            url: 'https://swaggable.com/account/user_shared_on_service',
            data: {service: service, product_id: product_id, type: type},
            success: function (html) {
                if (html == "1") {
//                    $.pnotify("Shared on " + service);
                }
            }
        });
    }

    function getURLParameter(url, name) {
        return (RegExp(name + '=' + '(.+?)(&|$)').exec(url)||[,null])[1];
    }
    function urldecode(url) {
        return decodeURIComponent(url.replace(/\+/g, ' '));
    }
    var attributes = [];
    var selectedAttributes = [];
    $(document).ready(function(){
        $(".addthis_btn").click(function(){
            var id = $(this).parent().attr('id');
            var url = decodeURI($(this).attr('href'));
            var addthis_url = urldecode(getURLParameter(url, "url"));
            var type = getURLParameter(addthis_url, "st_type");
            var source = getURLParameter(addthis_url, "st_source");
            share_this_callback(null, source, id, type);
            window.open($(this).attr('href'),"Sharing Window " + id,"menubar=1,resizable=1,width=600,height=650").moveTo((screen.width/2) - 300,(screen.height/2) - 325);
            return false;
        });

        $(".product_attributes").select2({
            placeholder: 'Type an attribute name',
            allowClear: true,
            tags: true
        });

        $(".product_attributes").select2('data', selectedAttributes);
        $(".product_attributes").on("change", function(){
            var attribues_array = $(this).select2('val');
            var product_id = $(this).parent().attr('product-id');
            var existing_attribues_array = [];
            var new_attribues_array = [];
            for(var i=0; i<attribues_array.length; i++){
                var flag = true;
                for(var j=0; j<attributes.length; j++){
                    if(attribues_array[i] == attributes[j].id){
                        flag = false;
                        existing_attribues_array.push(attribues_array[i]);
                    }
                }
                if(flag)
                    new_attribues_array.push(attribues_array[i]);
            }
            addProductAttributes(existing_attribues_array, new_attribues_array, product_id);
        });
    });

    function addProductAttributes(existing_attribues_array, new_attribues_array, product_id){
        $.ajax({
            type: "POST",
            dataType: 'json',
            url: 'https://swaggable.com/set_data/add_user_product_attributes',
            data: {
                existing_attributes: existing_attribues_array,
                new_attributes: new_attribues_array,
                productId: product_id
            },
            success: function (data) {
            }
        });
    }

    function sendData(user_id,share,product_id){
        dataLayer.push({
           event : 'track',
           user_id  : user_id,
           share_channel : share,
           product_id : product_id
        });
    }

    function want(product_id, el) {
        $.ajax({
            type: "POST",
            dataType: 'json',
            url: 'https://swaggable.com/set_data/want_product',
            data: {product_id: product_id },
            success: function (html) {
                if (html.status == 'failed') {
                    $.pnotify(html.message);
                }
                else if (html.status == 'success') {
                    if (html.data.want_state == "added") {
//                        $.pnotify(html.message);
                        $('.want-button-' + product_id).html("<i class='fa fa-check' style='position: initial; font-size: 14px;'></i> Wanted");
                        $('.want-count-' + product_id).html(html.data.want_count);
                        if((' ' + el.className + ' ').indexOf('suggested-product-want') > -1){
                            $('.suggested-product-want-sharing-popup-'+product_id).show("fade", 1000);
                        }
                        else{
                            $('.want-sharing-popup-'+product_id).show("fade", 1000);
                        }
                        $('#hidden-want-'+ product_id).trigger('click');
                        attributes = [];
                        for(var i=0; i<html.data.product_attributes.length; i++){
                            attributes.push({id: html.data.product_attributes[i].attribute_id, text: html.data.product_attributes[i].name})
                        }
                        $('.product_attributes-' + product_id).select2({
                            placeholder: 'Type an attribute name',
                            tags: attributes
                        });
                        if(html.data.user_attributes.length > 0){
                            var user_attributes = [];
                            for(var j=0; j<html.data.user_attributes.length; j++){
                                user_attributes.push({id: html.data.user_attributes[j].attribute_id, text: html.data.user_attributes[j].name})
                            }
                            $('.product_attributes-' + product_id).select2('data', user_attributes);
                        }
                        if(html.data.is_online_coupon_invite && html.data.is_invite_created){
                            var event = 'want_coupon_invite';
                            coupon_invite_notification(event, product_id, html);
                        }
                        else if(html.data.is_invite_created && !html.data.is_online_coupon_invite){
                            var event = 'want_invite';
                            sample_invite_notification(event, product_id, html);
                        }
                    }
                    else if (html.data.want_state == "removed") {
                        $('.want-button-' + product_id).html('Want it!');
                        $('.want-count-' + product_id).html(html.data.want_count);
                        if((' ' + el.className + ' ').indexOf('suggested-product-want') > -1){
                            $('.suggested-product-want-sharing-popup-'+product_id).hide("fade", 1000);
                        }
                        else{
                            $('.want-sharing-popup-'+product_id).hide("fade", 1000);
                        }
                        //var onclick_val = 'want(\'' + user_id + '\',true); return false;';
                        //$('#want-button').attr('onclick', onclick_val);
                        $.pnotify(html.message);
                    }
                }
            }
        });
    } //else $.pnotify("If you want to try this product you need to be logged in. Please <a href='#' onclick='loginClicked();return false;'>click here</a> to login.");
    window.onerror = function (errorMsg, url, lineNumber)
    {
        return errorMsg.indexOf('stLight') > 0;
    }

    function coupon_invite_notification(event, product_id, html) {
        dataLayer.push({
            'event' : event,
            'user_id' :  '',
            'product_id' : product_id
        });
        $("#coupon-notifications-count").removeClass('badge-green');
        var count = 0;
        if($("#coupon-notifications-count").text() != "")
            count = parseInt($("#coupon-notifications-count").text());
        else
            count = 0;
        $("#coupon-notifications-count").text(count + 1);

        var count = 0;
        $("#coupons #coupon-notifications-count").removeClass('badge-green');
        if($("#coupons #coupon-notifications-count").text() != "")
            count = parseInt($("#coupons #coupon-notifications-count").text());
        else
            count = 0;
        $("#coupons #coupon-notifications-count").text(count + 1);
        $.toast({
            heading: 'Congratulations!',
            text: 'You have just been invited to try ' + html.message.replace(' added to your Wishlist.',''),
            showHideTransition: 'slide',
            icon: 'info',
            position: 'bottom-left',
            allowToastClose: true,
            hideAfter: false,
            bgColor: '#836fa9',
            textColor: '#ffffff'
        });
    }

    function sample_invite_notification(event, product_id, html) {
        dataLayer.push({
            'event' : event,
            'user_id' :  '',
            'product_id' : product_id
        });
        $("#notifications #offers-dropdown").prepend(html.data.sample_notification);
        $("#offers-dropdown .top-divider").after(html.data.sample_notification);
        var count = 0;
        if($(".samples-count").text() == "")
            count = 0;
        else
            count = parseInt($(".samples-count").text());

        $(".samples-count").text(parseInt(count) + parseInt(1));
        $(".mobile-samples-count").text(parseInt(count) + parseInt(1));
        jQuery("abbr.timeago").timeago();
        $.toast({
            heading: 'Congratulations!',
            text: 'You have just been invited to try ' + ((event.indexOf("review") !== -1) ? html.data.sample_data.product_name : html.message.replace(' added to your Wishlist.','')),
            showHideTransition: 'slide',
            icon: 'info',
            position: 'bottom-left',
            allowToastClose: true,
            hideAfter: false,
            bgColor: '#836fa9',
            textColor: '#ffffff'
        });
    }

    // to make the product boxes same height
    $(window).load(function() {
        var maxHeight=0;
        $('.eheight').each(function(){
            if($(this).height()>maxHeight) {
                maxHeight=$(this).height();
            }
        });

        $('.eheight').height(maxHeight);

    });

    (function () {
        'use strict';
        var queryString = {};

        queryString.parse = function (str) {
            if (typeof str !== 'string') {
                return {};
            }

            str = str.trim().replace(/^\?/, '');

            if (!str) {
                return {};
            }

            return str.trim().split('&').reduce(function (ret, param) {
                var parts = param.replace(/\+/g, ' ').split('=');
                var key = parts[0];
                var val = parts[1];

                key = decodeURIComponent(key);
                // missing `=` should be `null`:
                // http://w3.org/TR/2012/WD-url-20120524/#collect-url-parameters
                val = val === undefined ? null : decodeURIComponent(val);

                if (!ret.hasOwnProperty(key)) {
                    ret[key] = val;
                } else if (Array.isArray(ret[key])) {
                    ret[key].push(val);
                } else {
                    ret[key] = [ret[key], val];
                }

                return ret;
            }, {});
        };

        queryString.stringify = function (obj) {
            return obj ? Object.keys(obj).map(function (key) {
                var val = obj[key];

                if (Array.isArray(val)) {
                    return val.map(function (val2) {
                        return encodeURIComponent(key) + '=' + encodeURIComponent(val2);
                    }).join('&');
                }

                return encodeURIComponent(key) + '=' + encodeURIComponent(val);
            }).join('&') : '';
        };

        queryString.push = function (key, new_value) {
            var params = queryString.parse(location.search);
            params[key] = new_value;
            var new_params_string = queryString.stringify(params)
            history.pushState({}, "", window.location.pathname + '?' + new_params_string);
        }

        if (typeof module !== 'undefined' && module.exports) {
            module.exports = queryString;
        } else {
            window.queryString = queryString;
        }
    })();
    $(window).ready(function() {
//    SR: category sidebar menu
        $("#category-sidebar-menu").mmenu({
            "slidingSubmenus": true,
            "extensions": [
                "pagedim-black",
                "effect-slide-menu",
                "theme-white"
            ],
            navbar: {
                title: 'FILTERS'
            },
            navbars: [
                {
