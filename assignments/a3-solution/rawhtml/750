<script type="text/javascript">

function get_review_id( selector )
{
    var temp = $(selector).attr('review');
    voters_names(temp);
}

function voters_names(review_id)
{
    $.ajax({
        type: "POST",
        dataType: 'html',
        url: 'https://swaggable.com/interactive_calls/get_voters_names',
        data: {review_id: review_id},
        success: function (html) {
            var h1 = '<h3 class="heading-large">People Who Voted This Up </h3>';
            var div = '<div id="slim-scroll-vote-up-' + review_id + '">';
            div += h1;
            div += html;
            div += '</div>';
            $('#voters-names').append(div);
            $('#slim-scroll-vote-up-'+review_id).slimscroll({
                height: '370px',
                railVisible: true
            });
            $('.my-tooltip').tooltipster({
                interactive: true,
                animation: 'grow'
            });
        }
    });
}

var vote_check = false;
function vote(type, object_id, vote, user_id) {

    if (vote_check == false) {

        vote_check = true;
        $.ajax({
            type: "POST",
            dataType: 'json',
            url: 'https://swaggable.com/index.php/set_data/vote/',
            data: {object_id: object_id, type: type, vote: vote, user_id: user_id   },
            success: function (html) {
                if (html.status == "success") {
                    $("body #review-" + object_id + " .votes").html(html.data.vote_count);
                    $("body #activity-review-" + object_id + " .votes").html(html.data.vote_count);
                    if (vote == "up") {
                        $("i#vote-up-" + object_id).removeClass('fa fa-angle-up');
                        $("i#vote-up-" + object_id).addClass('fa fa-chevron-circle-up');

                        $("i#vote-down-" + object_id).removeClass('fa fa-angle-down');
                        $("i#vote-down-" + object_id).removeClass('fa fa-chevron-circle-down');
                        $("i#vote-down-" + object_id).addClass('fa fa-angle-down');
                    }
                    else if (vote == "down") {
                        $("i#vote-down-" + object_id).removeClass('fa fa-angle-down');
                        $("i#vote-down-" + object_id).addClass('fa fa-chevron-circle-down');

                        $("i#vote-up-" + object_id).removeClass('fa fa-angle-up');
                        $("i#vote-up-" + object_id).removeClass('fa fa-chevron-circle-up');
                        $("i#vote-up-" + object_id).addClass('fa fa-angle-up');
                    }
                    $.pnotify(html.message);
                }
                else if (html.status == "failed") {
                    $.pnotify(html.message);
                }
            }
        });
        vote_check = false;
    }
}

function activity_comment_box_appear(review_id) {
    $("body #activity-review-" + review_id + " .comment-box").show();
}


function comment_box_appear(review_id) {
    $("body #review-" + review_id + " .comment-box").show();

}

function post_comments(review_id, activity_feed, reviews_on_profile) {
    if (typeof(activity_feed) === 'undefined') activity_feed = 0;
    if (typeof(reviews_on_profile) === 'undefined') reviews_on_profile = 0;
    var comment;
    if (activity_feed == 1) {
        comment = $('body #activity-review-' + review_id + ' .comment-box #text-area').val();
    }
    else {
        comment = $('body #review-' + review_id + ' .comment-box #text-area').val();
    }
    if (!comment == "") {
        $.ajax({
            type: "POST",
            dataType: 'json',
            url: 'https://swaggable.com/set_data/post_comment/',
            data: {review_id: review_id, comment: comment, activity_type: "review_post"},
            success: function (html) {
                if (html.status == "failed") {
                    if (html.message == "duplicate") {
                        $.pnotify("Duplicate comments are not allowed. Kindly check it.");
                    }
                    else {
                        $.pnotify("You need to signup to post comments. <a href='https://www.swaggable.com'>Signup</a>");
                    }
                }
                else if (html.status == "success") {
                    $.pnotify("Your comment has been posted.");
                    var check;
                    if (activity_feed == 0) {
                        check = $('#review-' + review_id + ' #comments #comments_show').attr('value');
                    }
                    else {
                        check = $('#activity-review-' + review_id + ' #comments #comments_show').attr('value');
                    }
                    if (check == "comments_showing") {
                        var divs = '<div class="media">';
                        divs = divs + '<a class="pull-left" href="#">';
                        divs = divs + '<img src="'+html.data.commenter_data[html.data.commenter_id].picture;
                        divs = divs + '?width=64&height=64" style="width: 64px; height: 64px;" /></a>';
                        divs = divs + '<div class="media-body">';
                        divs = divs + '<h4 class="media-heading">';
                        divs = divs + '<a href="#">';
                        divs = divs + html.data.commenter_name;
                        divs = divs + '</a>';
                        divs = divs + '</h4>';
                        divs = divs + comment;
                        divs = divs + '<div class="block"><small>';
                        divs = divs + '<abbr class="timeago" title="';
                        divs = divs + html.data.time + 'Z';
                        divs = divs + '"></abbr>';
                        divs = divs + '</small></div>';
                        divs = divs + '</div>';
                        divs = divs + '</div>';
                        if (activity_feed == 0) {
                            $('#review-' + review_id + ' #comments').append(divs);
                            $('#review-' + review_id + ' .media-meta #total-comments').html(html.data.comment_count + " Comments");
                            $('body #review-' + review_id + ' .comment-box #text-area').val('');
                        }
                        else {
                            $('#activity-review-' + review_id + ' #comments').append(divs);
                            $('#activity-review-' + review_id + ' .media-meta #total-comments').html(html.data.comment_count + " Comments");
                            $('body #activity-review-' + review_id + ' .comment-box #text-area').val('');
                        }
                        $("abbr.timeago").timeago();
                    }
                    else {
                        if (activity_feed == 0) {
                            $('#review-' + review_id + ' .media-meta #total-comments').html(html.data.comment_count + " Comments");
                            if (reviews_on_profile == 1) {
                                get_comments(review_id, 0, 2);
                            }
                            else {
                                get_comments(review_id, 0, 1);
                            }
                            $('body #review-' + review_id + ' .comment-box #text-area').val('');
                        }
                        else {
                            $('#activity-review-' + review_id + ' .media-meta #total-comments').html(html.data.comment_count + " Comments");
                            get_comments(review_id, 1, 1);
                            $('body #activity-review-' + review_id + ' .comment-box #text-area').val('');
                        }
                    }
                }
            }
        });
    }
    else {
        $.pnotify("Kindly write the comment first.");
    }
}

function get_comments(review_id, activity_feed, scroll_down) {
    if (typeof(activity_feed) === 'undefined') activity_feed = 0;
    if (typeof(scroll_down) === 'undefined') scroll_down = 0;
    var check;
    var flag = '';
    if (activity_feed == 0) {
        check = $('#review-' + review_id + ' #comments #comments_show').attr('value');
        flag = 'review';
    }
    else {
        check = $('#activity-review-' + review_id + ' #comments #comments_show').attr('value');
        flag = 'activity-review';
    }
    if (check == "comments_showing" && flag == 'review') {
        if($('#review-' + review_id).find('#comments')){
            if($('#review-' + review_id).find('#comments').hasClass('review-hide')){
                $('#review-' + review_id).find('#comments').removeClass('review-hide');
            }
            else{
                $('#review-' + review_id).find('#comments').addClass('review-hide');
            }
            return;
        }
    }
    if (check == "comments_showing" && flag == 'activity-review') {
        if($('#activity-review-' + review_id).find('#comments')){
            if($('#activity-review-' + review_id).find('#comments').hasClass('review-hide')){
                $('#activity-review-' + review_id).find('#comments').removeClass('review-hide');
            }
            else{
                $('#activity-review-' + review_id).find('#comments').addClass('review-hide');
            }
            return;
        }
    }
    $.ajax({
        type: "POST",
        dataType: 'json',
        url: 'https://swaggable.com/interactive_calls/get_comments/',
        data: {review_id: review_id },
        success: function (html) {
            if (html.status == "success") {
                if(html.data){
                    $.each(html.data, function (i, item) {
                        if (item.commenter_name == null) {
                            item.commenter_name = "Facebook User is";
                        }
                        var user_id = item.commenter;
                        var user_name = item.user_data[user_id].name;
                        var user_city = item.user_data[user_id].address.city;
                        var user_state = item.user_data[user_id].address.state;
                        var user_reviews = item.user_data[user_id].user_reviews;
                        var user_votes = item.user_data[user_id].user_votes;
                        var user_link = item.user_data[user_id].link;
                        var comment_id = item.comment_id;

                        var title = 'title="';
                        title += '<b>' + user_name + '</b>';
                        title += '<br /><div style=\'margin-top: 3px; margin-bottom: 10px; font-size: 11px\'>';
                        if (typeof(user_city) === 'undefined') {
                        }
                        else {
                            if (user_city != "" && user_city != "0") {
                                title += user_city + ', ' + user_state;
                                title += '<br />';
                            }
                        }
                        if (user_reviews != 0) {
                            title += user_reviews + ' reviews';
                            title += '<br />';
                        }
                        if (user_votes != 0) {
                            title += +user_votes + ' votes';
                        }
                        title += '</div>';
                        title += '<div class=\'pagination-centered\'><a style=\' margin-top: -5px;\' class=\'btn btn-small btn-success\' href=\'' + user_link + '\'>Visit Profile</a></div>';
                        title += '"';
                        var divs = '<div class="media" id="comment-' + comment_id + '">';
                        divs = divs + '<a class="pull-left" href="' + user_link + '">';
                        divs = divs + '<img class="my-tooltip" ' + title + ' src="'+item.user_data[item.commenter].picture;
                        divs = divs + '?width=64&height=64" style="width: 64px; height: 64px;" /></a>';
                        divs = divs + '<div class="media-body">';
                        divs = divs + '<h4 class="media-heading">';
                        divs = divs + '<a href="' + user_link + '">';
                        divs = divs + item.commenter_name;
                        divs = divs + '</a>';
                        divs = divs + '</h4>';
                        divs = divs + item.comment;
                        divs = divs + '<div class="block"><small>';
                        divs = divs + '<abbr class="timeago" title="';
                        divs = divs + item.time + 'Z';
                        divs = divs + '"></abbr>';
                        divs = divs + '</small></div>';
                        divs = divs + '</div>';
                        divs = divs + '</div>';
                        if (activity_feed == 0) {
                            $('#review-' + review_id + ' #comments').append(divs);
                        }
                        else {
                            $('#activity-review-' + review_id + ' #comments').append(divs);
                        }
                    });
                    var divs = '<input type="hidden" id="comments_show" value="comments_showing" />';
                    if (activity_feed == 0) {
                        $('#review-' + review_id + ' #comments').append(divs);
                        if (scroll_down == 1) {
                            var comment_box_height = $('#review-' + review_id + ' .media-body .comment-box').height();
                            var review_body_height = $('#review-' + review_id + ' .media-body .review-body').height();
                            var document_scroll = $(document).scrollTop();
                            var scroll = document_scroll + $('#review-' + review_id).get(0).scrollHeight - comment_box_height - review_body_height - 10;
                            $('html, body').animate({
                                scrollTop: scroll
                            }, 2000);
                        }
                        else if (scroll_down == 2) {
                            var comment_box_height = $('#review-' + review_id + ' .media-body .comment-box').height();
                            var review_body_height = $('#review-' + review_id + ' .media-body .review-body').height();
                            var document_scroll = $('#reviews').scrollTop();
                            var scroll = document_scroll + $('#review-' + review_id).get(0).scrollHeight - comment_box_height - review_body_height - 10;
                            $('#reviews').animate({
                                scrollTop: scroll
                            }, 2000);
                        }
                    }
                    else {
                        $('#activity-review-' + review_id + ' #comments').append(divs);
                        if (scroll_down == 1) {
                            var comment_box_height = $('#activity-review-' + review_id + ' .media-body .comment-box').height();
                            var review_body_height = $('#activity-review-' + review_id + ' .media-body .review-body').height();
                            var document_scroll = $('#activity-feed').scrollTop();
                            var scroll = document_scroll + $('#activity-review-' + review_id).get(0).scrollHeight - comment_box_height - review_body_height - 10;
                            $('#activity-feed').animate({
                                scrollTop: scroll
                            }, 2000);
                        }
                    }
                    $("abbr.timeago").timeago();
                    $('.my-tooltip').tooltipster({
                        interactive: true,
                        animation: 'grow'
                    });
                }

            }
        }
    });
}


    $(document).ready(function() {
        $('.votes a').each(function () {
            if ($.trim($(this).html()) != '0')
            {
                $(this).fancybox();
            }
        });
        $('.votes a').on('click',function(){
            $('#voters-names').empty();
            get_review_id(this);
        });
        $('.star').each(function () {
            var current_rating = $(this).attr("value");
            $(this).raty({ readOnly: true, score: current_rating });
        });
    });
</script><div id="reviews">
    <div class="span9 voters-names" id="voters-names" style="width: 520px; min-height: 400px; overflow-y:hidden;
    overflow-x:hidden; display: none;"></div>
    <style>
    .star-review img{
        height: 24px;
    }
</style>

