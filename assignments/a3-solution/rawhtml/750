<script type="text/javascript">
    var clipboard_sharing_url = new Clipboard(".copy-sharing-url");
    clipboard_sharing_url.on('success', function(e) {
        $(".copy-sharing-url").text("Copied");
        $.pnotify('Sharing URL copied to clipboard.');
        e.clearSelection();
    });

    clipboard_sharing_url.on('error', function(e) {
        $.pnotify('Unable to copy.');
    });

    $("#send-email").on('click', function (){
        var is_verified = true;
        var emails = $("#email-to").tagsinput('items');
        if(emails.length == 0){
            $.pnotify('Please enter an email address.');
            is_verified = false;
            return false;
        }
        else{
            for(var i=0; i<emails.length; i++){
                if(!is_valid_email(emails[i])){
                    $.pnotify('Only valid email addresses are allowed.');
                    is_verified = false;
                    return false;
                }
            }
        }
        var user_id = "";

        if(is_verified){
            $.ajax({
                type:'POST',
                dataType: "json",
                url: 'https://swaggable.com/set_data/invite_friend_by_email',
                data:{email_to:emails,
                    user_id:user_id
                },
                success: function(data){
                    if(data.status == "success"){
                        $.pnotify(data.message);
                        $.fancybox.close();
                    }
                    else{
                        $.pnotify("Email sending failed. Please try again later.");
                        $.fancybox.close();
                    }
                }
            });
        }
    });

    function close_fancybox(){
        $.fancybox.close();
    }
    $(document).ready(function(){
        $(".open-invite-friends-dialog").on('click', function(){
            $('.share-url').removeClass('hide');
            $('#fb-share-button').removeClass('hide');
            $('#DIV_7').removeClass('hide');

            $('.email-form').addClass('hide');
            $('.email-friend-btn').removeClass('hide');
            $(".open-invite-friends-dialog").fancybox({
                maxWidth: 280,
                fitToView: true,
                width: 'auto',
                minHeight: 300,
                autoSize: true,
                closeClick: false,
                closeEffect: 'none',
                scrolling: 'no'
            });
        });
        $('.email-friend-btn').on('click', function(){
            $('.email-form').removeClass('hide');
            $('.email-friend-btn').addClass('hide');

            $('#fb-share-button').addClass('hide');
            $('#DIV_7').addClass('hide');
            $('.share-url').addClass('hide');

            $.fancybox.update();
        });
    });

