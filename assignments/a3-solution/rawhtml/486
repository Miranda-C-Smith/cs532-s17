    });

    var cpHeight = $('#thecomment').height();
    $('#thecomment').attrchange({
        callback: function (e) {
            var ccHeight = $(this).height();
            if (cpHeight !== ccHeight) {
                $(document.body).trigger("sticky_kit:recalc");
                console.log('change attr for comment');
                cpHeight = ccHeight;
            }
        }
    }).resizable();


</script>
