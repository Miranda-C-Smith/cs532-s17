(function(){
function cx_click_test(test_id){
	cX.setEventAttributes({origin:'itm-website',persistedQueryId:'7afa26d5cf882cbdf1133ef0a74bc60ab5581d17'});
	cX.sendEvent('click',{'itm_test_id':test_id});
	console.log(test_id);
};
var facebook_event_callback = function(action,url,html_element) {
	if (html_element.parentNode.id == 'thirdPartyWidget') {
		var test_id = 'rm44215_FB_main' + action;
		cx_click_test(test_id);
	} else if (html_element.parentNode.className == 'sharebox_btn_fb') {
		var test_id = 'rm44215_FB_main' + action;
		cx_click_test(test_id);
	} else if (html_element.parentNode.className.match(/colBoxFollowBtnFacebook/)) {
		var test_id = 'rm44215_FB_sub' + action;
		cx_click_test(test_id);
	}
};
window.fbAsyncInit = function(){
	FB.Event.subscribe('edge.create', facebook_event_callback.bind(null,'_like'));
	FB.Event.subscribe('edge.remove', facebook_event_callback.bind(null,'_remove'));
};
})();
</script>
