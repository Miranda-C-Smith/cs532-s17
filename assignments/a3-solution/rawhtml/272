<script>
(function(){
	var d = document,
			endlink,         /* 関連記事枠 */
			endlink_ul,      /* 関連記事＆関連リンク */
			endlink_art,     /* 関連記事ul */
			endlink_artlist, /* 関連記事li */
			copyright,       /* コピーライト */
			div = d.createElement('div'),
			atag = d.createElement('a'),
			msg_pre, /* preview でのメッセージ用 */
			refURL = getUrl().path_cutpageing_cutparam.replace('.html','_00.html');
	atag.href = refURL;
	atag.innerHTML = 'この記事の関連情報';
	div.appendChild(atag);
	div.id = 'cmsReferenceBtn';
	if (d.getElementById('endlinkConnection')) {
		/* 関連記事枠がある場合 */
		endlink = d.getElementById('endlinkConnection');
		endlink_ul = endlink.getElementsByTagName('ul');
		for (var i=0; i<endlink_ul.length; i++) {
			if (endlink_ul[i].id == 'art') {
				endlink_art = endlink_ul[i];
				endlink_artlist = endlink_art.getElementsByTagName('li');
			}
		}
		/* 関連記事6件目以降を非表示 */
		if(endlink_artlist && endlink_artlist.length > 5) {
			// preview では隠さず、非表示になる旨を表示
			if(getUrl().domain.match(/preview/)) {
				msg_pre = d.createElement('div');
				msg_pre.innerHTML = '※↓6件目以降は公開時に非表示になります↓※';
				msg_pre.style.margin = '10px 0px';
				msg_pre.style.fontSize = '16px';
				msg_pre.style.color = '#f75555';
				endlink_art.insertBefore(msg_pre,endlink_artlist[5]);
			} else {
				for (var j=5; j<endlink_artlist.length; j++) {
					endlink_artlist[j].style.display = 'none';
				}
			}
		}
		/* 関連記事がある場合はその下に、無ければ関連記事枠の上に表示 */
		if (endlink_art !== undefined) {
			div.style.margin = '-30px 0 0';
			endlink_art.parentNode.insertBefore(div,endlink_art.nextSibling);
		} else {
			endlink.insertBefore(div,endlink.firstChild);
		}
	} else if (d.getElementById('end') || d.getElementsByClassName('ctrl').length == 0) {
		/* 最終ページもしくは単体ページで、関連記事枠が無い場合 */
		copyright = d.getElementById('cmsCopyright');
		copyright.parentNode.insertBefore(div,copyright);
	}
})();
</script>

<script src="/js/lib/cx_ab.js"></script>
<script>
(function(){
	if(getUrl().domain.match(/preview/)) return;
	var d = document;
	var group_a = cx_ab_flag('itm\_(7)'); // オリジナル
	var group_b = cx_ab_flag('itm\_(8)'); // popIn レコメンド
	var group_c = cx_ab_flag('itm\_(9)'); // logly コンテキストマッチ
	if( (group_a || group_b || group_c) && (d.getElementById('art')) ){
		var ticket = 'rm44819',
				target, // テスト対象となる div
				target_ready, // テスト対象の読込完了確認
				target_atags, // テスト対象の a タグの配列
				endlink_art = d.getElementById('art'),  // 元の関連記事枠
				endlink = endlink_art.parentNode, // 記事下関連記事枠全体(endlinkConnection)
				endlink_click, // 関連記事 click 時の処理
				refBtn = d.getElementById('cmsReferenceBtn');  // リファレンス誘導ボタン
		if(group_a == true){
			target = endlink_art;
		}else if(group_b == true){
			target = d.getElementById('_popIn_recommend');
			// popIn レコメンドを移動
			refBtn.parentNode.insertBefore(target,refBtn);
			refBtn.style.margin = '20px 0 0';
			// 元の関連記事は非表示に
			endlink.removeChild(endlink.getElementsByTagName('h4')[0]);
			endlink.removeChild(endlink_art);
		}else if(group_c == true){
			target = d.createElement('div');
			target.id = 'logly_wrap';
			// logly レコメンドパーツを配置
			var logly = d.createElement('div');
			logly.id = 'logly-lift-3307805';
			target.appendChild(logly);
			refBtn.parentNode.insertBefore(target,refBtn);
			refBtn.style.margin = '20px 0 0';
			// js の読み込み
			var _lgy_lw = document.createElement("script");
			_lgy_lw.type = "text/javascript";
			_lgy_lw.charset = "UTF-8";
			_lgy_lw.async = true;
			_lgy_lw.src= (("https:" == document.location.protocol) ? "https://" : "http://")+"l.logly.co.jp/lift_widget.js?adspot_id=3307805";
			var _lgy_lw_0 = document.getElementsByTagName("script")[0];
			_lgy_lw_0.parentNode.insertBefore(_lgy_lw, _lgy_lw_0);
			// 元の関連記事は非表示に
			endlink.removeChild(endlink_art);
		}
		var set_viewthru = function() {
			if(group_b == true){
				// 「ソーシャルで話題」を本来の位置に戻す
				var cmsBodyInner = d.getElementById('cmsBody').getElementsByTagName('div')[0];
				cmsBodyInner.appendChild(d.getElementById('rm42241'));
				cmsBodyInner.appendChild(d.getElementById('popIn_sns_wrapper'));
			}
			target_ready = setInterval(function(){
				cx_viewthru(target,ticket + '_rel','top',100);
				target_atags = target.getElementsByTagName('a');
				for(var i = 0; i < target_atags.length; i++){
					endlink_click = function(){
						cx_count(ticket + '_rel_link','click');
					};
					appendEventListener(target_atags[i],'click',endlink_click,false);
				}
				clearInterval(target_ready);
			},500);
		};
		appendEventListener(window,'load',set_viewthru,false);
	}
})();
</script>



