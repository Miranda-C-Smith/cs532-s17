<script type="text/javascript">
$(document).ready(function(){
	$("#links a.colorbox").colorbox();
	$("#newsBox article.news a").colorbox({
		inline:true,
		maxWidth:"80%",
		maxHeight:"80%"
	});
	$("#links a.contact").colorbox({
		iframe:true,
		width:"60%",
		height:"80%"
	});

	var imgs = ["centerimg1.png", "centerimg2.png", "centerimg3.png"];
	var rand = Math.floor( Math.random()*imgs.length);
	$('#animation').attr("src", "./images/"+imgs[rand]);
	$('#animation').each(function(){
		$(this).addClass("fadeInDown");
	});

	var i = 0, newsMax = 4;
	for (; i<$("#newsBox article.news").length && i<newsMax; i++) {
		$("#newsBox article.news").eq(i).show();
	}
	$("#newsBox div.newsmore a").click(function(){
		$("#newsBox div.newsmore").fadeOut(200);
		for (; i<$("#newsBox article.news").length; i++) {
			$("#newsBox article.news").eq(i).show(200);
		}
	});
});
</script>
