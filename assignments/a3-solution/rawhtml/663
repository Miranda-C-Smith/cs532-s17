        data.DATETIME_PRICE  = data.DC;
        data.DATETIME_BID  = data.DB;
        data.DATETIME_ASK  = data.DA;

                if (Array.isArray(ZBL_notation) && ZBL_notation[data.ID_NOTATION] != "undefined")
        {
         if (ZBL_notation[data.ID_NOTATION]=='fv')
          ZBL_priceFV(data);
         else if (ZBL_notation[data.ID_NOTATION]=='fv2')
          ZBL_priceFV2(data);
         else if (ZBL_notation[data.ID_NOTATION]=='multi' || ZBL_notation[data.ID_NOTATION]=='multi2')
          ZBL_Mutli(data); 
        }
       }
       else if (type=="NEWS")
       {
        ZBL_update(data.myElement,data.sMessage);
       }
       else if (type=="TOP_FLOP"&&false)
       {
