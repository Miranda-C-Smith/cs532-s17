  function share_mail()
  {       
      if(!$('#sendbymail_share').is(':visible'))
      {
          $('html,body').animate({scrollTop: $("#ancremail").offset().top}, 'slow'      );
         $("#sendbymail_share").show(); 
      }
      else
      {
         $("#sendbymail_share").hide(); 
      }
      
  }
