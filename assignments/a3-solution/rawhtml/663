         }
         sToDiv += '<tr>';
         sToDiv += '<td class="large20 center"'+sAdd+'>'+sImg+'</td>';
         sToDiv += '<td class="newsColCT nobr"'+sAdd+'><a href="/zbat.php?codezb='+data.data[key].I+'">'+data.data[key].N.toUpperCase().substring(0,27)+(data.data[key].N.length>27?'&hellip;':'')+' '+key+'</a></td><td>';
         sToDiv += '<td class="large60 pright5 right"'+sAdd+'>'+data.data[key].C+'</td>';
         if (data.data[key].Q == 'RLT')
          sToDiv += '<td class="large8"><img width="6" height="6" title="Cours en temps réel." alt="Cours en temps réel." src="/images/ico_t.gif"></td>';
