<script type="text/javascript">
//after a function to hide and another one to show, a simple toggle
function itemToggle(obj) {
var el = document.getElementById(obj);
if (!el ){
return false;
}
el.style.display = (el.style.display != 'none' ? 'none' : '' );
// Track clicks on togglable items with event tracking (only when opened)
if(el.style.display === ''){
booking.google.trackEvent(booking.google.hotelPageTracker, "FAQ clicked", obj);
}
}
</script>
