<div id="HLCXComparisonJumplinkContent" class="a-section a-spacing-small a-spacing-top-small aok-hidden">
    <span class="a-declarative" data-action="hlcx-send-reftag" data-hlcx-send-reftag="{&quot;refTag&quot;:&quot;psdc_2474054011_c_B00ILLV46Y&quot;,&quot;allowLinkDefault&quot;:&quot;true&quot;}">
        <a id="HLCXComparisonJumplinkLink" class="a-link-normal" href="#HLCXComparisonWidget_feature_div">
            <span>Compare with similar items</span>
        </a>
    </span>
</div>



    <script type="text/javascript">
        
        P.when("A").execute(function(A) {
            
            var comparisonWidgetNotFirstLoad = (A.state("comparisonWidget-Not-First-Load") && A.state("comparisonWidget-Not-First-Load").comparisonWidgetFirstLoad) || false;
            if(comparisonWidgetNotFirstLoad) {
                return;
            }
             A.state.replace("comparisonWidget-Not-First-Load", {comparisonWidgetFirstLoad:true});

            
            A.declarative("hlcx-send-reftag", "click", function(event) {
                var reftag = event.data.refTag;
                A.$.get("/gp/product/ajax-handlers/reftag.html/ref=" + reftag);
            });

            
            P.when("atf","ready").execute(function() {
                function updateJumplink() {
                    var jumplinkDestinationATF = A.$("#HLCXComparisonJumplink_feature_div");
                    var comparisonTableDiv = A.$("#HLCXComparisonWidget_feature_div");
                    var jumplinkContentBTF = comparisonTableDiv.find("#HLCXComparisonJumplinkContent");
                    var isTableVisible = comparisonTableDiv.has("#HLCXComparisonTable").length;

                    jumplinkDestinationATF.empty();

                    if(isTableVisible) {
                        jumplinkContentBTF.clone().removeClass("aok-hidden").appendTo(jumplinkDestinationATF);
                    }
                }

                
                updateJumplink();

                
                A.on("a:pageUpdate", function() {
                    updateJumplink();
                });
            });
        });


        
        P.when('A', 'jQuery', 'ready').execute(function(A, $, ready) {
            $('#HLCXComparisonWidget_feature_div>table>tbody>tr').not(
                    ':first').not(':last').hover(function() {
                 $(this).addClass('comparison_tablerow_highlight');
            }, function() {
                 $(this).removeClass('comparison_tablerow_highlight');
            });
        });
    </script>


    <style type="text/css">
        .comparison_tablerow_highlight {
            border-top: 1px solid #999999;
            border-bottom: 1px solid #999999;
        }
        .comparison_image_title_cell{
            border: 1px dotted #ccc;
            padding-top: 20px;
            padding-left: 10px;
            width:20%;
        }

        .comparison_baseitem_column {
            background-color: #ffffdd;
            border: 1px dotted #ccc;
            padding-left: 10px;
        }

        .comparison_attribute_name_column {
            background-color: #f3f3f3;
            border: 1px dotted #ccc;
            padding-left: 10px;
        }

        .comparison_sim_items_column {
            border: 1px dotted #ccc;
            padding-left: 10px;
        }

        .comparison_table {
            table-layout: fixed;
            width:auto;
        }
        .comparison_sim_asin{
            visibility: hidden;
        }

        a:hover span.comparison_zeitgeist_badge_store_name {
            color: #E47911 !important;
            text-decoration: underline;
        }
        .comparison_zeitgeist:hover {
            text-decoration: none;
        }
        .comparison_zeitgeist_badge {
            margin-right: 4px;
            background-color: #E47911 !important;
            padding-top: 2px;
            padding-bottom: 2px;
        }
        .comparison_zeitgeist_badge:before {
            border-bottom-color: #E47911 !important;
        }
        .comparison_zeitgeist_badge:after {
            border-top-color: #E47911 !important;
        }
        .comparison_table_first_col{
            width:20% !important;
        }
    </style>


            
        
    


	                
                
            
        
        
        
        
        
        
    
    
    

    















    </div>
