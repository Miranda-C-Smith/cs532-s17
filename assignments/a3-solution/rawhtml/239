        P.when('jQuery').execute(function($) {
            if ($("#giveaway").is(":hidden") &&
                    (typeof giveawayGiftingShareVar !== 'undefined' && giveawayGiftingShareVar)) {
                $("#giveaway").removeClass("a-hidden");
            }
        });
    </script>


	                
                
            
        
        
        
        
        
        
    
    
    

    















    </div>
</div>
