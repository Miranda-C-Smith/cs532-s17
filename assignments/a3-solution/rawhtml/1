							//728x90 for desktop/tablet, resize to 320x50 for mobile
																					var mapping728 = googletag.sizeMapping().addSize([0, 0], [320, 50]).addSize([700, 0], [728,90]).build();
							googletag.defineSlot('/43459271/loc-desktop/kbtx/web/content/aggiesports', [728,90],'gdm-ad-728x90-A').setTargeting("wnsz",["01"]).defineSizeMapping(mapping728).addService(googletag.pubads());
							
