							if(event.slot.getTargeting("wnsz") == "101") {
								if(!event.isEmpty) {
									if(event.size[1] == 90 || event.size[1] == 96) {
										console.log('Creative with target: ' + event.slot.getTargeting("wnsz") + ' is rendered to slot of size: ' + event.size[0] + 'x' + event.size[1]);
										jQuery("#gdm-footer-container-ad").css("height","90px");
										jQuery("#footerAdMargin").css("height","90px");
									} else if(event.size[1] == 50) {
										jQuery("#gdm-footer-container-ad").css("height","50px");
										jQuery("#footerAdMargin").css("height","50px");
									}
								}
							} else if(event.slot.getTargeting("wnsz") == "07") {
								if(!event.isEmpty) {
									if(event.size[1] == 600) {
										//console.log('Creative with target: ' + event.slot.getTargeting("wnsz") + ' is rendered to slot of size: ' + event.size[0] + 'x' + event.size[1]);
										$('#gdm-ad-300x250-A').attr('id','gdm-ad-300x600-A');
									}
								}
							} else if(event.slot.getTargeting("wnsz") == "09") {
								var hasPencil = false;
								if(!event.isEmpty) {
									if(event.size[1] == 30) {
										hasPencil = true;
									}
								}
								if(!hasPencil) {
									$('#gdm-ad-pencil').css("display","none");
									$('#gdm-ad-pencil').css("height","0px");
								}
							}
						});
						googletag.pubads().collapseEmptyDivs();
						googletag.pubads().enableAsyncRendering();
